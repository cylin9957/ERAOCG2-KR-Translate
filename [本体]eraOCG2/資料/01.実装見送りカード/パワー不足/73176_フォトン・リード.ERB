@CARDCAN_73176(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 条件達成,5

CALL CARD_NEGATE(決闘者,種類,ゾーン,場所,73176)
SIF RESULT == 1
	RETURN 0
IF 種類 == 魔罠起動 || 種類 == 手札起動 || 種類 == 墓地起動 || 種類 == フリー誘発
	IF ゾーン == "手札"
		SIF ターンプレイヤー != 決闘者
			RETURN 0
	ELSEIF ゾーン == "魔罠"
		SIF GETVAR(@"%決闘者%_魔_表:場所") == 3
			RETURN 0
	ELSEIF ゾーン == "墓地"
		SIF 発動済_1(決闘者,73176)
			RETURN 0
		SIF GETVAR(@"%決闘者%_除外NG置換") >= 1
			RETURN 0
		SIF メインフェイズ != 1
			RETURN 0
		SIF ターンプレイヤー != 決闘者
			RETURN 0
	ENDIF
	CALL CARD_SS_CHECK(決闘者,0,"手札")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	効果範囲手札 = 1
	効果条件属性 = 光属性
	CALLFORM 効果条件レベル_ADV(4, "以下")
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_73176(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード


IF 種類 == 魔罠起動 || 種類 == 手札起動 || 種類 == 墓地起動 || 種類 == フリー誘発
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,73176,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(決闘者,ゾーン,場所)
		CALL FULLRESET
		効果範囲手札 = 1
		効果条件属性 = 光属性
		CALLFORM 効果条件レベル_ADV(4, "以下")
		CALL CARD_SP_SUMMON_ADV(決闘者,ゾーン,場所)
	ENDIF
ENDIF