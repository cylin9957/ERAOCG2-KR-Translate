@CARDNAME_26604,参照先
;ココで指定カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = RR－アベンジ・ヴァルチャー
	CASE "略称"
		RESULTS = アベンジ・Ｖ
	CASE "カテゴリ"
		RESULTS:0 = レイドラプターズ
ENDSELECT


@CARD_26604(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "種類"
	
		RETURN 効果モン
	CASE "属性"
		RETURN 闇属性
	CASE "種族"
		RETURN 鳥獣族
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1700
	CASE "守備力"
		RETURN 100
ENDSELECT

RETURN 0

@CARD_EXPLANATION_26604(種類)
#DIM DYNAMIC 種類

CALLFORM CARD_ABLE(0,効果後誘発)
PRINTL ①：自分が戦闘・効果でダメージを受けた場合に発動できる。
PRINTL このカードを手札から特殊召喚する。
PRINTL

@CARDCAN_26604(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件達成
CALL CARD_NEGATE(決闘者,種類,ゾーン,場所,26604)
SIF RESULT == 1
	RETURN 0

IF 種類 == 効果後誘発
	SIF ゾーン != "手札"
		RETURN 0
	CALL CARD_SS_CHECK(決闘者,26604,"手札")
	SIF RESULT == 1
		RETURN 0
	CALL CARD_CHECK_FIN_BIT(決闘者,"被弾者")
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_26604(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 攻撃力修正

IF 種類 == 効果後誘発
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,26604)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(決闘者,ゾーン,場所)
	ENDIF
ENDIF
