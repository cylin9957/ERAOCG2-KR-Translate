@CARDNAME_92665,参照先
;ココで指定カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = レイド・ラプターズ・レプリカ
	CASE "略称"
		RESULTS = Ｒ・Ｒ・Ｒ
	CASE "カテゴリ"
		RESULTS:0 = レイドラプターズ
ENDSELECT


@CARD_92665(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "種類"

		RETURN 通常罠
ENDSELECT

RETURN 0

@CARD_EXPLANATION_92665(種類)
#DIM DYNAMIC 種類

CALLFORM CARD_ABLE(0,魔罠起動 ,フリー誘発)
PRINTL ①：自分フィールドの「RR」モンスター１体を対象として発動できる。
PRINTL その同名モンスター１体をデッキから特殊召喚する。
PRINTL この効果で特殊召喚したモンスターは攻撃誘導を得る。

@CARDCAN_92665(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者

CALL 対面者判定(決闘者)
対面者 = %RESULTS%

CALL CARD_NEGATE(決闘者,種類,ゾーン,場所,92665)
SIF RESULT == 1
	RETURN 0

IF 種類 == 魔罠起動 || 種類 == フリー誘発
	SIF ゾーン != "魔罠"
		RETURN 0
	IF ゾーン == "魔罠"
	SIF GETVAR(@"%決闘者%_魔_表:場所") == 3
		RETURN 0
	ENDIF
	SIF GETVAR(@"%決闘者%_魔_表:場所") == 1
		RETURN 0
	CALL CARD_SS_CHECK(決闘者,0,"デッキ")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	効果範囲戦場 = 1
	効果条件カテゴリ = レイドラプターズ
	効果条件種類 = レベル持ち
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_92665(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カードID
#DIMS DYNAMIC 選択カード名

IF 種類 == 魔罠起動 || 種類 == フリー誘発
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,92665)
	IF RESULT == 0
		CALL CARD_DISPOSE(決闘者,ゾーン,場所)
		CALL FULLRESET
		効果条件カテゴリ = レイドラプターズ
		効果条件種類 = レベル持ち
		CALL CARD_CHOOSE_BASIC(決闘者,"戦場",ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
			選択カードID = RESULT:1
			TRYCALLFORM CARDNAME_{選択カードID}, "名前"
			選択カード名 = %RESULTS%
			CALL FULLRESET
			効果範囲デッキ = 1
			効果条件名前 = %選択カード名%
			CALL CARD_SP_SUMMON_ADV(決闘者,ゾーン,場所)
			CALL STATCHANGE_EFFECT(決闘者,"戦場",RESULT,"攻誘")
		ENDIF
	ENDIF
ENDIF