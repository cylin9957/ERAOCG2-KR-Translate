@CARDNAME_26609,参照先
;ココで指定カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = RR－ナパーム・ドラゴニアス
	CASE "略称"
		RESULTS = ナパーム・Ｄ
	CASE "カテゴリ"
		RESULTS:0 = レイドラプターズ
ENDSELECT


@CARD_26609(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "種類"
	
		RETURN 効果モン
	CASE "属性"
		RETURN 闇属性
	CASE "種族"
		RETURN 鳥獣族
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1000
	CASE "守備力"
		RETURN 1000
ENDSELECT

RETURN 0

@CARD_EXPLANATION_26609(種類)
#DIM DYNAMIC 種類

CALLFORM CARD_ABLE(10,戦場起動)
PRINTL ①：１ターンに１度、発動できる。相手に６００ダメージを与える。
CALLFORM CARD_ABLE(0,破壊→墓地 ,戦闘→墓地)
PRINTL ②：このカードが戦闘・効果で破壊され墓地へ送られた場合に発動できる。
PRINTL デッキから「RR」モンスター１体を効果を無効にして特殊召喚する。

@CARDCAN_26609(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件達成
CALL CARD_NEGATE(決闘者,種類,ゾーン,場所,26609)
SIF RESULT == 1
	RETURN 0

IF 種類 == 戦場起動
	SIF 効果発動済(決闘者,場所,ゾーン)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 決闘者
		RETURN 0
	RETURN 1
ELSEIF 種類 == 破壊→墓地 || 種類 == 戦闘→墓地
	CALL CARD_SS_CHECK(決闘者,0,"デッキ")
	SIF RESULT == 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_26609(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

IF 種類 == 戦場起動
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,26609,10)
	IF RESULT == 0
		CALL LIFE_CHANGE(決闘者,"減少","効果",600,1)
	ENDIF
ELSEIF 種類 == 破壊→墓地 || 種類 == 戦闘→墓地
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,26609)
	IF RESULT == 0
		CALL FULLRESET
		効果範囲デッキ = 1
		効果条件カテゴリ = レイドラプターズ
		効果追加種類 = 効追無効
		CALL CARD_SP_SUMMON_ADV(決闘者,ゾーン,場所)
	ENDIF
ENDIF