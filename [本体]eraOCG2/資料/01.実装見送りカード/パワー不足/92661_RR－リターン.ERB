@CARDNAME_92661,参照先
;ココで指定カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = RR－リターン
	CASE "略称"
		RESULTS = RR－リターン
	CASE "カテゴリ"
		RESULTS:0 = レイドラプターズ
ENDSELECT


@CARD_92661(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "種類"

		RETURN 通常罠
ENDSELECT

RETURN 0

@CARD_EXPLANATION_92661(種類)
#DIM DYNAMIC 種類
PRINTL このカード名の②の効果は１ターンに１度しか使用できない。
CALLFORM CARD_ABLE(0,効果後誘発)
PRINTL ①：自分フィールドの「RR」モンスターが戦闘で破壊された場合、
PRINTL 自分の墓地の「RR」モンスター１体を対象として発動できる。
PRINTL そのモンスターを手札に加える。
CALLFORM CARD_ABLE(2,効果後誘発)
PRINTL ②：自分フィールドの「RR」モンスターが効果で破壊された場合、
PRINTL 墓地のこのカードを除外して発動できる。
PRINTL デッキから「RR」カード１枚を手札に加える。

@CARDCAN_92661(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 効果先
#DIMS DYNAMIC 対面者

CALL 対面者判定(決闘者)
対面者 = %RESULTS%

CALL CARD_NEGATE(決闘者,種類,ゾーン,場所,92661)
SIF RESULT == 1
	RETURN 0
IF 種類 == 効果後誘発
	IF ゾーン == "魔罠"
		SIF GETVAR(@"%決闘者%_魔_表:場所") == 3
			RETURN 0
		SIF GETVAR(@"%決闘者%_魔_表:場所") == 1
			RETURN 0
		CALL FULLRESET
		効果範囲戦場 = 1
		効果条件カテゴリ = レイドラプターズ
		効果条件種類 = モン全般
		CALL CARD_CHECK_FIN_DATA(決闘者,"戦闘破壊")
		SIF RESULT != 1
			RETURN 0
		RETURN 1
	ELSEIF ゾーン == "墓地"
		SIF 発動済_2(決闘者,92661)
			RETURN 0
		SIF GETVAR(@"%決闘者%_除外NG置換") >= 1
			RETURN 0
		CALL FULLRESET
		効果範囲戦場 = 1
		効果条件カテゴリ = レイドラプターズ
		効果条件種類 = モン全般
		CALL CARD_CHECK_FIN_DATA(決闘者,"効果破壊")
		SIF RESULT != 1
			RETURN 0
	ELSE
		RETURN 0
	ENDIF
	RETURN 1
ENDIF

@CARDEFFECT_92661(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード

IF 種類 == 効果後誘発
	IF ゾーン == "魔罠"
		CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,92661)
		IF RESULT == 0
			CALL CARD_DISPOSE(決闘者,ゾーン,場所)
			CALL FULLRESET
			効果範囲墓地 = 1
			効果条件カテゴリ = レイドラプターズ
			効果条件種類 = モン全般
			CALL CARD_ADD_BASIC(決闘者,"墓地",ゾーン,場所)
		ENDIF
	ELSEIF ゾーン == "墓地"
		CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,92661,2)
		IF RESULT == 0
			CALL CARD_DISPOSE(決闘者,ゾーン,場所)
			CALL FULLRESET
			効果条件カテゴリ = レイドラプターズ
			CALL CARD_ADD_BASIC(決闘者,"デッキ",ゾーン,場所)
		ENDIF
	ENDIF
ENDIF
