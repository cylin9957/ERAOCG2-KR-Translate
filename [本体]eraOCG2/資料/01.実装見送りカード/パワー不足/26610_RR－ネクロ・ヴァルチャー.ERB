@CARDNAME_26610,参照先
;ココで指定カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = RR－ネクロ・ヴァルチャー
	CASE "略称"
		RESULTS = ネクロ・Ｖ
	CASE "カテゴリ"
		RESULTS:0 = レイドラプターズ
ENDSELECT


@CARD_26610(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "種類"
	
		RETURN 効果モン
	CASE "属性"
		RETURN 闇属性
	CASE "種族"
		RETURN 鳥獣族
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1000
	CASE "守備力"
		RETURN 1600
ENDSELECT

RETURN 0

@CARD_EXPLANATION_26610(種類)
#DIM DYNAMIC 種類

CALLFORM CARD_ABLE(10,戦場起動)
PRINTL ①：１ターンに１度、自分フィールドの「RR」モンスター１体をリリースし、
PRINTL 自分の墓地の「RUM」魔法カード１枚を対象として発動できる。
PRINTL そのカードを手札に加える。

@CARDCAN_26610(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件達成
CALL CARD_NEGATE(決闘者,種類,ゾーン,場所,26610)
SIF RESULT == 1
	RETURN 0

IF 種類 == 戦場起動
	SIF 効果発動済(決闘者,場所,ゾーン)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 決闘者
		RETURN 0
	CALL FULLRESET
	効果範囲墓地 = 1
	効果条件カテゴリ = RUM
	効果条件種類 = 魔法全般
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_26610(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

IF 種類 == 戦場起動
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,26610,10)
	IF RESULT == 0
		CALL FULLRESET
		効果条件カテゴリ = レイドラプターズ
		CALL CARD_ADD_GY_BASIC(決闘者,"戦場",ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			効果条件カテゴリ = RUM
			効果条件種類 = 魔法全般
			CALL CARD_ADD_BASIC(決闘者,"墓地",ゾーン,場所)
		ENDIF
	ENDIF
ENDIF