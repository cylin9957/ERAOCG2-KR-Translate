@CARDNAME_xxxx,参照先
;ココで指定カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS
;TFのウリアちゃんは別カードにする
SELECTCASE 参照先
	CASE "名前"
		RESULTS = 聖斬のコード アシッド
	CASE "略称"
		RESULTS = アシッド
	CASE "カテゴリ"
		RESULTS:0 = アンノウン
		RESULTS:1 = セレス
ENDSELECT


@CARD_xxxx(参照先)
;名前専用
#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "種類"
		RETURN 効果モン
	CASE "属性"
		RETURN 光属性
	CASE "種族"
		RETURN 天使族
	CASE "レベル"
		RETURN 7
	CASE "攻撃力"
		RETURN 2300
	CASE "守備力"
		RETURN 2300
ENDSELECT

RETURN 0

