@CARDNAME_59885,参照先
;ココで지정卡片の名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 경세 도레미아이돌 쿨리아
	CASE "略称"
		RESULTS = ♫ 쿨리아
	CASE "カテゴリ"
		RESULTS:0 = 도레미코드
		RESULTS:1 = 도레미아이돌
		RESULTS:2 = E.R.A
ENDSELECT


@CARD_59885(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS
 
SELECTCASE 参照先
	CASE "종류"
		RETURN 싱크로
	CASE "속성"
		RETURN 바람속성
	CASE "종족"
		RETURN 천사족
	CASE "レベル"
		RETURN 8
	CASE "攻撃力"
		RETURN 2700
	CASE "守備力"
		RETURN 2500
	CASE "성별"
		RETURN 牝性
	CASE "連動ID"
		RETURN 2085
ENDSELECT
RETURN 0

@CARD_EXPLANATION_59885(종류)
#DIM DYNAMIC 종류

PRINTL 천사족튜너＋튜너이외의 モンスター1장 이상
PRINTL 이 카드명은 룰상 も「도레미코드」カード로서 취급한다。
CALLFORM CARD_ABLE(1,필드起動)
PRINTL ①：EX덱의 앞면 표시의 レベルが奇数と偶数の「도레미코드」Pモンスター各1장을 덱に戻して발동할 수 있다。
PRINTL    상대 필드의 「도레미아이돌오시」モンスター1体の隣接モンスター全てを対応する「도레미아이돌오시」몬스터와 する。
PRINTL    이 효과는 상대ターンでも발동할 수 있다。
CALLFORM CARD_ABLE(2,지속효과)
PRINTL ②：이 카드가 몬스터 존에 존재하는 한、
PRINTL    自身の필드의 「도레미코드」カードは상대의 효과의 대상이 되지 않는다。
CALLFORM CARD_ABLE(2,지속효과)
PRINTL ③：自身の필드에 「도레미코드」Pモンスター존재하는 경우、상대몬스터는 이 카드를 공격 대상으로 선택できない。

@CARDSUMMON_59885(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要レベル数 = 8
IF EX参照 == 0 || EX参照 == 1
	素材条件튜너 = 1
	素材条件종족 = 천사족
ELSEIF EX参照 >= 10
	素材条件튜너 = -1
ENDIF

IF EX参照 == 1
	CALL SYNCAN(듀얼리스트,소환カード,59885) 
	RETURN RESULT
ENDIF


@CARDCAN_59885(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,59885)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動
	SIF ゾーン != "필드"
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	CALL FULLRESET
	효과範囲EX덱 = 1
	효과条件カテゴリ = 도레미코드
	효과条件종류 = EX以外全般
	효과条件レベル:0 = 2
	효과条件レベル:1 = 4
	효과条件レベル:2 = 6
	효과条件レベル:3 = 8
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲EX덱 = 1
	효과条件カテゴリ = 도레미코드
	효과条件종류 = EX以外全般
	효과条件レベル:0 = 1
	효과条件レベル:1 = 3
	효과条件レベル:2 = 5
	효과条件レベル:3 = 7
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件종류 = モン全般
	효과条件カテゴリ:0 = 큐ティア오시
	효과条件カテゴリ:1 = 드리미아오시
	효과条件カテゴリ:2 = 엘리티아오시
	효과条件カテゴリ:3 = 팬시아오시
	효과条件カテゴリ:4 = 그레이시아오시
	효과条件カテゴリ:5 = 엔제리아오시
	효과条件カテゴリ:6 = 뷰티아오시
	효과条件표시 = 表側표시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_59885(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 布教者
#DIM DYNAMIC 布教者ID
#DIMS DYNAMIC 布教内容

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59885,1)
	IF RESULT == 0
		カウンタ = 0
		CALL FULLRESET
		효과範囲EX덱 = 1
		효과条件カテゴリ = 도레미코드
		효과条件종류 = EX以外全般
		효과条件レベル:0 = 1
		효과条件レベル:1 = 3
		효과条件レベル:2 = 5
		효과条件レベル:3 = 7
		효과条件行先플래그 = 덱行き
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"EX덱",ゾーン,場所)
		SIF RESULT != 9999
			カウンタ++
		CALL FULLRESET
		효과範囲EX덱 = 1
		효과条件カテゴリ = 도레미코드
		효과条件종류 = EX以外全般
		효과条件レベル:0 = 2
		효과条件レベル:1 = 4
		효과条件レベル:2 = 6
		효과条件レベル:3 = 8
		효과条件行先플래그 = 덱行き
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"EX덱",ゾーン,場所)
		SIF RESULT != 9999
			カウンタ++	
		IF カウンタ == 2
			CALL FULLRESET
			효과範囲상대필드 = 1
			효과条件종류 = モン全般
			효과条件カテゴリ:0 = 큐ティア오시
			효과条件カテゴリ:1 = 드리미아오시
			효과条件カテゴリ:2 = 엘리티아오시
			효과条件カテゴリ:3 = 팬시아오시
			효과条件カテゴリ:4 = 그레이시아오시
			효과条件カテゴリ:5 = 엔제리아오시
			효과条件カテゴリ:6 = 뷰티아오시
			효과条件표시 = 表側표시
			CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
			IF RESULT != 9999
				布教者 = RESULT - 2000
				布教者ID = RESULT:99
				布教内容 = %GETVARS(@"%対面者%_MZ_追カテ:布教者")%
		
				PRINTFORML 
				SELECTCASE 布教内容
					CASE "큐ティア오시"
							SETCOLOR C_H_PINK
						TRYCALLFORM CARDNAME_{布教者ID},"名前"
						PRINTFORML 「%RESULTS%」が隣接するモンスターたちに「큐ティア」の매력を宣伝します！
					CASE "드리미아오시"
							SETCOLOR C_G_YELLOW
						TRYCALLFORM CARDNAME_{布教者ID},"名前"
						PRINTFORML 「%RESULTS%」が隣接するモンスターたちに「드리미아」の매력を宣伝します！
					CASE "엘리티아오시"
							SETCOLOR C_L_BLUE
						TRYCALLFORM CARDNAME_{布教者ID},"名前"
						PRINTFORML 「%RESULTS%」が隣接するモンスターたちに「엘리티아」の매력を宣伝します！
					CASE "팬시아오시"
							SETCOLOR C_W_RED
						TRYCALLFORM CARDNAME_{布教者ID},"名前"
						PRINTFORML 「%RESULTS%」が隣接するモンスターたちに「팬시아」の매력を宣伝します！
					CASE "그레이시아오시"
							SETCOLOR C_P_BLUE
						TRYCALLFORM CARDNAME_{布教者ID},"名前"
						PRINTFORML 「%RESULTS%」が隣接するモンスターたちに「그레이시아」の매력を宣伝します！
					CASE "엔제리아오시"
							SETCOLOR C_ORANGE
						TRYCALLFORM CARDNAME_{布教者ID},"名前"
						PRINTFORML 「%RESULTS%」が隣接するモンスターたちに「엔제리아」の매력を宣伝します！
					CASE "뷰티아오시"
							SETCOLOR C_W_BLUE
						TRYCALLFORM CARDNAME_{布教者ID},"名前"
						PRINTFORML 「%RESULTS%」が隣接するモンスターたちに「뷰티아」の매력を宣伝します！
				ENDSELECT
				PRINTFORMW
					RESETCOLOR
				CALL CARD_SET_CATEGORY(対面者,"필드",MAX(布教者-1,0),布教内容)
				CALL CARD_SET_CATEGORY(対面者,"필드",MIN(布教者+1,4),布教内容)
			ENDIF
		ENDIF
	ENDIF
ELSEIF 종류 == 지속효과
	CALL FULLRESET
	효과条件カテゴリ = 도레미코드
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"対耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"마함",場所,"対耐")
	
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종류 = モン全般
	효과条件カテゴリ = 도레미코드
	효과条件分割 = 펜듈럼
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"攻避")
ENDIF
