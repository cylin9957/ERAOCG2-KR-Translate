;==================================================
;朝起きたとき（おはよう口上）
;==================================================
@M_KOJO_EVENT_K1335_2(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;空行
	PRINTFORML
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;非同棲かつ相手が先に起きてる
	;eraOCG2_V0.40_BT_240522時点では流れる状況を作れない
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;TARGET私室にMASTERが入っている（恋慕時添い寝中就寝時のみ？
		;eraOCG2_V0.40_BT_240522時点では、TARGETの私室には入れない
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTER私室にTARGETが入ってきてる(恋慕無しでも入ってくる可能性有り
		;eraOCG2_V0.40_BT_240522時点では、MASTERの睡眠＝1日の終了のため、実質流れない
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ相手が起きている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「起きちゃった？%CALLNAME:MASTER%もセキュリティが甘いね♡」
			CASE 1
				PRINTFORMW 「おはよう！　%CALLNAME:MASTER%！　今日もハッキング日和だね」
			CASE 2
				PRINTFORML 「ふわぁ……おはよ…、%CALLNAME:MASTER%……」
				PRINTFORMW %CALLNAME:TARGET%も起きたばかりなのか、眠たげに挨拶を交わした
		ENDSELECT
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ相手が寝ている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		SELECTCASE RAND:2
			CASE 1
				PRINTFORML 「んぅ……」
				PRINTFORMW %CALLNAME:TARGET%は猫のように丸まりながら、だらしない寝顔を晒している
			CASE 2
				PRINTFORML 「むにゃあ……%CALLNAME:MASTER%……」
				PRINTFORMW %CALLNAME:TARGET%は夢の中で%CALLNAME:MASTER%の寝言を呟いているようだ
		ENDSELECT
		RETURN 1
	ENDIF
	[SKIPSTART]
	;-------------------------------------------------
	;MASTERが起きていて、相手が起きてきた
	;添い寝時TARGET起床はこちら（TARGET衰弱or添い寝中TARGET睡魔負け後そのままMASTERが起き続けた上で相手が起床した場合限定
	;eraOCG2_V0.40_BT_240522時点では流れることを確認できなかったため注意
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;-------------------------------------------------
		;非同棲
		;eraOCG2_V0.40_BT_240522時点では、非同棲時に寝るとTARGETの自室に帰ってしまう。注意
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 「ふわぁ……おはよー、%CALLNAME:MASTER%……」
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;同棲中
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 「ふわぁ……おはよー、%CALLNAME:MASTER%……」
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	[SKIPEND]
	;その他
	PRINTFORMW   
ENDIF
RETURN 1

