;==================================================
;朝起きたとき（おはよう구상）
;==================================================
@M_KOJO_EVENT_K1335_2(ARG,ARG:1)
;구상主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------
IF LOCAL
	;空行
	PRINTFORML
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;非同棲かつ상대가 先に起きてる
	;eraOCG2_V0.40_BT_240522時点では流れる상황を作れない
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;TARGET私室にMASTERが入っている（연모時添い寝中就寝時のみ？
		;eraOCG2_V0.40_BT_240522時点では、TARGETの私室には入れない
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTER私室にTARGETが入ってきてる(연모無しでも入ってくる可能性有り
		;eraOCG2_V0.40_BT_240522時点では、MASTERの수면＝1日の終了のため、実質流れない
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ상대가 起きている
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「起きちゃった？%CALLNAME:MASTER%もセキュリティが甘いね♡」
			CASE 1
				PRINTFORMW 「おはよう！%CALLNAME:MASTER%！今日もハッキング日和だね」
			CASE 2
				PRINTFORML 「ふわぁ……おはよ…、%CALLNAME:MASTER%……」
				PRINTFORMW %CALLNAME:TARGET%も起きたばかりなのか、眠たげに挨拶を交わした
		ENDSELECT
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ상대가 寝ている
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		SELECTCASE RAND:2
			CASE 1
				PRINTFORML 「すぅ……」
				PRINTFORMW %CALLNAME:TARGET%は素直な寝息をたてながら、だらしない寝顔を晒している
			CASE 2
				PRINTFORML 「ぴぃ……%CALLNAME:MASTER%……」
				PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%の寝言を呟いているようだ
		ENDSELECT
		RETURN 1
	ENDIF
	[SKIPSTART]
	;-------------------------------------------------
	;MASTERが起きていて、상대가 起きてきた
	;添い寝時TARGET起床はこちら（TARGET쇠약or添い寝中TARGET睡魔負け後そのままMASTERが起き続けた上で상대가 起床した場合限定
	;eraOCG2_V0.40_BT_240522時点では流れることを確認できなかったため주의
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;-------------------------------------------------
		;非同棲
		;eraOCG2_V0.40_BT_240522時点では、非同棲時に寝るとTARGETの自室に帰ってしまう。주의
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 「ふわぁ……おはよー、%CALLNAME:MASTER%……」
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;同棲中
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 「ふわぁ……おはよー、%CALLNAME:MASTER%……」
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	[SKIPEND]
	;その他
	PRINTFORMW   
ENDIF
RETURN 1

;==================================================
;夜寝るとき（おやすみ구상）
;==================================================
@M_KOJO_EVENT_K1335_3(ARG,ARG:1)
;구상主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;自室に帰って寝る（～は【現在地】から【캐릭터の自室】に戻りました）
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:TARGET:연모
			PRINTFORMW 「ふわぁ……ワタシ、先に帰ってるね……」
		ELSE
			PRINTFORMW 「そろそろ帰るよ。また今度ね！」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;眠そうにしている（～は眠そうにしている）
	;eraOCG2_V0.40_BT_240522時点では流れることを確認できなかったため주의
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 「んん……ハッ！ね、眠ってなんか！」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;수면中（～は眠っている）
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML 「すぅ……」
		PRINTFORMW %CALLNAME:TARGET%はすやすやと眠っている
		RETURN 1
	ENDIF

	;-------------------------------------------------
	;쇠약して寝る（체력500以下,우후후時は우후후強制終了）
	;（캐릭터の自室以外：～を【現在地】から【캐릭터の自室】に運びました）
	;（캐릭터の自室　　：～は疲れたと言って、横になりました）
	;この後ARG == 1 が流れる
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		IF CFLAG:우후후
			;우후후中にダウン
			PRINTFORML 「ぅ゛……❤あぁ゛……❤」
			PRINTFORMW %CALLNAME:TARGET%はビクビクと身体を震わせながら、쾌락の中で意識を手放してしまったようだ
		ELSE
			PRINTFORMW 「うう……もうダメ、休ませてぇ～……」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERがダウン（연모のみ,そのまま一日が終了）
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		IF CFLAG:우후후
			;우후후中にダウン
			PRINTFORMW 「ん……❤もう疲れちゃった？それじゃあ、続きのハッキングはまた今度ね❤」
		ELSE
			PRINTFORMW 「わっ！どうしたの！？%CALLNAME:MASTER%！無理はしちゃダメ！」
		ENDIF
		RETURN 1
	ENDIF
	[SKIPSTART]
	;-------------------------------------------------
	;TARGETが寝るために自室まで戻ってきた
	;a.m.0:00頃に突然、수면中のTARGETが寝室に出現する
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		;-------------------------------------------------
		;そのまま着替えて寝た（【TARGET】が部屋に戻ってきた→寝間着へと着替え、寝床に潜った）
		;未確認、eraOCG2_V0.40_BT_240522時点ではこの상황にならない？
		;記入チェック（=0, 非표시、1, 표시）
