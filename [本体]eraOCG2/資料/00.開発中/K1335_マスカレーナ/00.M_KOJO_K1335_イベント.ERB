	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		PRINTFORMW  
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MAIN_MAP上で一日最初の挨拶
	;未確認だが、おそらくeraOCG2_V0.40_BT_240522時点では流れないため주의
