;==================================================
@M_KOJO_EVENT_K1335_1(ARG,ARG:1)
;구상主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------
IF LOCAL
	;空行
	PRINTFORML
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF TALENT:TARGET:연모
