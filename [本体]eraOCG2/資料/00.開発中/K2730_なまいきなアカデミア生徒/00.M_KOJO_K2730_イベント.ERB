	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		IF CFLAG:うふふ
			;うふふ中にダウン
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERがダウン（恋慕のみ,そのまま一日が終了）
	;記入チェック（=0, 非表示、1, 表示）
