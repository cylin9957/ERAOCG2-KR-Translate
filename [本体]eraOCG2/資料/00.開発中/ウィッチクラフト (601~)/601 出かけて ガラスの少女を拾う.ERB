@IRAI_EVENT_601(参照先)
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 対象キャラ

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE  "名前"
		RESULTS = (CMV-1)出かけて ガラスの少女を拾う  
	CASE "イベント種"
		RETURN イベント_通常
	CASE "依頼先"
		RETURN 場所_公園
	CASE "详细"
		PRINTFORML 一日の疲れを感じた%CALLNAME:MASTER%は夕食後、近くの公園へ散歩に行くことにした  
		PRINTL どんな不思議な出会いが待っているでしょうか
		PRINTL
	CASE "条件"
		RETURN 1
ENDSELECT

RETURN 0

