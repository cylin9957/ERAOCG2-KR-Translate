	RETURN 1
ELSEIF 種類 == 戦場起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 決闘者
		RETURN 0
	SIF ゾーン != "戦場"
		RETURN 0
	CALL FULLRESET
	効果範囲EXデッキ = 1
	効果範囲手札 = 1
	効果範囲デッキ = 1
	効果条件カテゴリ = 召喚獣
	効果条件種類 = 融合
	CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 種類 == 発動誘発
	SIF 効果発動済(決闘者,場所,ゾーン)
		RETURN 0
	SIF ゾーン != "戦場"
		RETURN 0
	SIF 手札数(決闘者) == 0
		RETURN 0
	CALL FULLRESET
	効果条件種類 = 魔罠全般
	CALL CARD_CHECK_START_DATA(決闘者,1)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 種類 == 永続効果
	SIF ゾーン != "戦場"
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_202050(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIM DYNAMIC 戦闘カード
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 調教対象
#DIM DYNAMIC 光不可
#DIM DYNAMIC 闇不可
#DIM DYNAMIC 炎不可
#DIM DYNAMIC 風不可
#DIM DYNAMIC 地不可
#DIM DYNAMIC 水不可



IF 種類 == 攻撃時誘発
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,202050)
	IF RESULT == 0
		戦闘カード = 戦闘場所(決闘者)
		CALL FULLRESET
		;CALL SET_FIELD_INFO(決闘者,"戦場",戦闘カード,"一時",1000)
		CALL SET_FIELD_INFO(決闘者,"戦場",戦闘カード,"一攻",1000)
		CALL SET_FIELD_INFO(決闘者,"戦場",戦闘カード,"一守",1000)
	ENDIF

ELSEIF 種類 == 戦場起動
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,202050)
	IF RESULT == 0
		CALL FULLRESET
		効果範囲戦場 = 1
		効果条件属性 = 光属性
		CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
		SIF RESULT == 1
			光不可 = 1
		CALL FULLRESET
		効果範囲戦場 = 1
		効果条件属性 = 闇属性
		CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
		SIF RESULT == 1
			闇不可 = 1
		CALL FULLRESET
		効果範囲戦場 = 1
		効果条件属性 = 炎属性
		CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
		SIF RESULT == 1
			炎不可 = 1
		CALL FULLRESET
		効果範囲戦場 = 1
		効果条件属性 = 水属性
		CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
		SIF RESULT == 1
			水不可 = 1
		CALL FULLRESET
		効果範囲戦場 = 1
		効果条件属性 = 地属性
		CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
		SIF RESULT == 1
			地不可 = 1
		CALL FULLRESET
		効果範囲戦場 = 1
		効果条件属性 = 風属性
		CALL CARD_CHECK_ADV(決闘者,種類,ゾーン,場所)
		SIF RESULT == 1
			風不可 = 1


		CALL FULLRESET
		効果範囲手札 = 1
		効果範囲デッキ = 1
		効果範囲EXデッキ = 1
		効果条件特不無視 = 1
		効果条件カテゴリ = 召喚獣
		効果条件種類 = 融合
		効果条件属性:0 = 7
		SIF 光不可 != 1
			効果条件属性:0 = 光属性
		SIF 闇不可 != 1
			効果条件属性:1 = 闇属性
		SIF 炎不可 != 1
			効果条件属性:2 = 炎属性
		SIF 風不可 != 1
			効果条件属性:3 = 風属性
		SIF 地不可 != 1
			効果条件属性:4 = 地属性
		SIF 水不可 != 1
			効果条件属性:5 = 水属性
		CALL CARD_SP_SUMMON_ADV(決闘者,ゾーン,場所)
	ENDIF
ELSEIF 種類 == 発動誘発
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,202050,10)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_ADD_GY_BASIC(決闘者,"手札",ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(決闘者,"発動無効",場所,通常罠)
			RETURN 1
		ENDIF
	ENDIF
ELSEIF 種類 == 永続効果
	CALL STATCHANGE_EFFECT(決闘者,"自身",場所,"対耐")
	CALL STATCHANGE_EFFECT(決闘者,"自身",場所,"破耐")
	CALL STATCHANGE_EFFECT(決闘者,"自身",場所,"戦耐")
	CALL STATCHANGE_EFFECT(決闘者,"自身",場所,"不材")
ENDIF
