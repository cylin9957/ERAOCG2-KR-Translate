;==================================================
;怒り口上
;==================================================
@M_KOJO_EVENT_K2140_29(ARG,ARG:1)
;口上主はTARGET
;ARGはTARGETの度胸で分岐
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;怒らせてしまった
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORML 「哼！」
		PRINTFORML  %CALLNAME:TARGET%还在气头上吧、没给你好脸色
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;泣かせてしまった　[臆病]持ち
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORML 「%CALLNAME:MASTER%、为什么要这样对待我」
		PRINTFORML  %CALLNAME:TARGET%不住地哭泣着
		RETURN 1
	ENDIF
ENDIF
RETURN 1
