@M_KOJO_MESSAGE_COM_KX_80
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_KX_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_KX_80_1
#DIM DYNAMIC 호감도修正
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	호감도修正 = 3
	SIF TALENT:TARGET:애처
		호감도修正 += 0
	SIF TALENT:TARGET:연모 == 2
		호감도修正 += 1
	SIF TALENT:TARGET:연모 >= 1
		호감도修正 += 1
	SELECTCASE RAND:호감도修正 + 1
		CASE 1
			PRINTFORMW 
		CASE 2
			PRINTFORMW 
		CASE 3
			PRINTFORMW 
		CASE 4
			PRINTFORMW 
		CASE 5
			PRINTFORMW 
	ENDSELECT
ENDIF
RETURN 1

;==================================================
;81,펠라치오
;==================================================
@M_KOJO_MESSAGE_COM_KX_81
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_KX_81_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_KX_81_1
#DIM DYNAMIC 호감도修正
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;ファースト키스
		IF EXP:TARGET:Ｍ경험 == 0 && EXP:TARGET:펠라경험 == 1 && CALLNAME:(CFLAG:TARGET:키스상실상대) == CALLNAME:MASTER
			PRINTFORMW 
		;それ以外
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	호감도修正 = 3
	SIF TALENT:TARGET:애처
		호감도修正 += 0
	SIF TALENT:TARGET:연모 == 2
		호감도修正 += 1
	SIF TALENT:TARGET:연모 >= 1
		호감도修正 += 1
		
	;お掃除펠라
	IF STAIN:MASTER:2 & 4 && ABL:TARGET:기교 >= 3
		PRINTFORMW 

	;通常
	ELSE
		SELECTCASE RAND:호감도修正 + 1
			CASE 1
				PRINTFORMW 
			CASE 2
				PRINTFORMW 
			CASE 3
				PRINTFORMW 
			CASE 4
				PRINTFORMW 
			CASE 5
				PRINTFORMW 
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

