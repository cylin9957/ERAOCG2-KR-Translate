IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;ファーストキス
		IF EXP:TARGET:Ｍ経験 == 0 && EXP:TARGET:口淫経験 == 1 && CALLNAME:(CFLAG:TARGET:キス喪失相手) == CALLNAME:MASTER
			PRINTFORMW 
		;それ以外
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	好感度修正 = 3
	SIF TALENT:TARGET:愛妻
		好感度修正 += 0
	SIF TALENT:TARGET:恋慕 == 2
		好感度修正 += 1
	SIF TALENT:TARGET:恋慕 >= 1
		好感度修正 += 1
		
	;お掃除フェラ
	IF STAIN:MASTER:2 & 4 && ABL:TARGET:技巧 >= 3
		PRINTFORMW 

	;通常
	ELSE
		SELECTCASE RAND:好感度修正 + 1
			CASE 1
				PRINTFORMW 
			CASE 2
				PRINTFORMW 
			CASE 3
				PRINTFORMW 
			CASE 4
				PRINTFORMW 
			CASE 5
				PRINTFORMW 
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

