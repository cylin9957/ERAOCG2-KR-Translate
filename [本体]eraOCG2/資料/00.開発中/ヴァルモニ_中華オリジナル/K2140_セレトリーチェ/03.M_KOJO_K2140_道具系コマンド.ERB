;40,ローター
;クリトリスに当てる
;==================================================
@M_KOJO_MESSAGE_COM_KX_40
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_KX_40_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_KX_40_1
#DIM DYNAMIC 好感度修正
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	好感度修正 = 3
	SIF TALENT:TARGET:愛妻
		好感度修正 += 0
	SIF TALENT:TARGET:恋慕 == 2
		好感度修正 += 0
	SIF TALENT:TARGET:恋慕 >= 1
		好感度修正 += 0
	SELECTCASE RAND:好感度修正 + 1
		CASE 1
			PRINTFORMW 
		CASE 2
			PRINTFORMW 
		CASE 3
			PRINTFORMW 
	ENDSELECT
ENDIF
RETURN 1

