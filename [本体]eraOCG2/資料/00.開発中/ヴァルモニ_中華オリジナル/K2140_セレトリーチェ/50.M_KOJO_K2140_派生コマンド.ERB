#DIM DYNAMIC 好感度修正
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	好感度修正 = 3
	SIF TALENT:TARGET:愛妻
		好感度修正 += 0
	SIF TALENT:TARGET:恋慕 == 2
		好感度修正 += 1
	SIF TALENT:TARGET:恋慕 >= 1
		好感度修正 += 1
	SELECTCASE RAND:好感度修正 + 1
		CASE 1
			PRINTFORMW 
		CASE 2
			PRINTFORMW 
		CASE 3
			PRINTFORMW 
		CASE 4
			PRINTFORMW 
		CASE 5
			PRINTFORMW 
	ENDSELECT
ENDIF
