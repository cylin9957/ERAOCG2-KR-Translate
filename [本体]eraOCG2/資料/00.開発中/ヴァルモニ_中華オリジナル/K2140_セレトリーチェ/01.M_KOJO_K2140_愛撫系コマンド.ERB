			PRINTFORMW 
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF

	;-------------------------------------------------
	;ふたなり
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		PRINTFORMW 
		RETURN 1
	ENDIF

	;基本セット
	好感度修正 = 3
	SIF TALENT:TARGET:愛妻
		好感度修正 += 1
	SIF TALENT:TARGET:恋慕 == 2
		好感度修正 += 2
	SIF TALENT:TARGET:恋慕 >= 1
		好感度修正 += 2
	SELECTCASE RAND:好感度修正 + 1
		CASE 1
			PRINTFORMW 
		CASE 2
			PRINTFORMW 
		CASE 3
			PRINTFORMW 
		;恋慕
		CASE 4
			PRINTFORMW 
		CASE 5
			PRINTFORMW 
		;親愛
		CASE 6
			PRINTFORMW 
		CASE 7
			PRINTFORMW 
		;愛妻
		CASE 8
			PRINTFORMW 
	ENDSELECT
ENDIF
RETURN 1
