@CARDNAME_40079,参照先
;ココで指定卡片の名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 惑乱魔阿洛美勒丝
	CASE "略称"
		RESULTS = 阿洛美勒丝(惑)
	CASE "カテゴリ"
		RESULTS:0 = 惑乱魔
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_40079(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "種類"
		RETURN エクシーズ
	CASE "属性"
		RETURN 地属性
	CASE "種族"
		RETURN 昆虫族
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1100
	CASE "守備力"
		RETURN 1700
	CASE "性別"
		RETURN 牝性
ENDSELECT

RETURN 0

@CARDSUMMON_40079(決闘者,召喚カード,EX参照)
#DIMS DYNAMIC 決闘者
#DIM DYNAMIC 召喚カード
#DIM DYNAMIC EX参照
#DIM DYNAMIC カウンタ

CALL RESETCONDITION_MAT
必要素材数 = 2
素材条件レベル = 4
素材条件カテゴリ:0 = 蟲惑魔
素材条件カテゴリ:1 = 惑乱魔

IF EX参照 == 1
	CALL XYZCAN(決闘者,召喚カード,40079) 
	RETURN RESULT
ENDIF



@CARD_EXPLANATION_40079(種類)
#DIM DYNAMIC 種類

CALL TEXT_DECORATION("ROGUE")
PRINTL レベル4の「蟲惑魔」モンスターまたは「惑乱魔」モンスタx2
PRINTL このカード名の③④⑤の効果はそれぞれ１ターンに１度しか使用できない。
CALLFORM CARD_ABLE(0,永続効果)
PRINTL ①：Ｘ素材を持っているこのカードは罠耐性を持つ。
PRINTL 装備カードが装備されているこのカードは攻撃宣言ができません。
CALLFORM CARD_ABLE(0,攻守変動)
PRINTL ②：このカードの攻撃力•守備力上升用这张卡效果装备怪兽的攻撃力•守備力数值。
CALLFORM CARD_ABLE(3,ターン終了時)
PRINTL ③：ターン終了時,このカードにモンスターが装備されている場合に発動できる。
PRINTL 这张卡的攻撃力•守備力上升1000点,获得1个X素材。
CALLFORM CARD_ABLE(1,戦場起動)
PRINTL ④：このカードにはが装備されのいる自分のメインフェイズがある,このカードのＸ素材を１つ取り除いて発動できる。
PRINTL 自己魔罠区1只牝性「蜜罐」特殊召唤,この效果特殊召唤的卡视为「惑乱魔」怪兽。
CALLFORM CARD_ABLE(2,発動誘発)
PRINTL ⑤：相手フィールド有性別のモンスタ把効果発動的场合,このカードのＸ素材を１つ取り除いて発動できる。
PRINTL その効果を無効にする,そのモンスターは「蜜罐」としてこのカードに装備されている
CALL TEXT_DECORATION("ROGUE")



@CARDCAN_40079(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 必要的卡

CALL 対面者判定(決闘者)
対面者 = %RESULTS%

CALL CARD_NEGATE(決闘者,種類,ゾーン,場所,40079)
SIF RESULT == 1
	RETURN 0

IF 種類 == 戦場起動
	SIF 発動済_1(決闘者,40079)
		RETURN 0
    SIF ターンプレイヤー != 決闘者
    	RETURN 0
    SIF メインフェイズ != 1
    	RETURN 0
	SIF ゾーン != "戦場"
		RETURN 0
	SIF GETVAR(@"%決闘者%_MZ_Ｘ:場所") < 0
		RETURN 0

    CALL FULLRESET
	効果条件種類 = モン全般
	効果条件性別 = 牝性
	効果条件カテゴリ = 蜜罐
	CALL CARD_EQUIP_CHECK(決闘者,場所,ゾーン)
    SIF RESULT != 1
    	RETURN 0	
    RETURN 1

ELSEIF 種類 == 発動誘発
	SIF ゾーン != "戦場"
		RETURN 0
	SIF 発動済_2(決闘者,40079)
		RETURN 0
	SIF GETVAR(@"%決闘者%_MZ_Ｘ:場所") < 0
		RETURN 0

    CALL FULLRESET
    効果範囲戦場 = 1
    効果条件種類 = モン全般
    効果条件性別 = 性別在
    CALL CARD_CHECK_START_DATA(決闘者,1)
    SIF RESULT != 1
		RETURN 0
		
    RETURN 1

ELSEIF 種類 == ターン終了時
	SIF ゾーン != "戦場"
		RETURN 0
	SIF 発動済_3(決闘者,40079)
		RETURN 0

    CALL FULLRESET
	効果条件種類 = モン全般
    効果条件性別 = 性別在
	CALL CARD_EQUIP_CHECK(決闘者,場所,ゾーン)
    SIF RESULT != 1
    	RETURN 0
    RETURN 1

ELSEIF 種類 == 攻守変動 || 種類 == リアクション効果
	SIF ゾーン != "戦場"
		RETURN 0

    CALL FULLRESET
	CALL CARD_EQUIP_CHECK(決闘者,場所,ゾーン)
    SIF RESULT != 1
    	RETURN 0
    RETURN 1


ELSEIF 種類 == 永続効果
	SIF ゾーン != "戦場"
		RETURN 0
	RETURN 1 
ENDIF


@CARDEFFECT_40079(決闘者,種類,ゾーン,場所)
#DIMS DYNAMIC 決闘者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 種類
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 守備力修正
#DIM DYNAMIC X消費
#DIM DYNAMIC X素材数
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 調教対象
#DIM DYNAMIC 戦場单体
#DIM DYNAMIC カード_ID

CALL 対面者判定(決闘者)
対面者 = %RESULTS%

IF 種類 == 戦場起動
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,40079,1,X消費1)
	IF RESULT == 0
		効果範囲魔罠 = 1
		効果条件種類 = モン全般
		効果条件性別 = 牝性
		効果条件カテゴリ = 蜜罐
		効果追加種類 = 140 + 4
		効果追加カテ = 蜜罐
		効果追加カテ = 惑乱魔
        CALL CARD_SP_SUMMON_ADV(決闘者,ゾーン,場所)

		CALL CARD_LOADUP

		調教対象 = GETVAR(@"%決闘者%_戦場:カード召喚先")
        CALL CARD_EROTICS(調教対象)
		IF RESULT != -1
			調教対象 = RESULT
			SETCOLOR C_PINK
			PRINTFORML
			PRINTFORMW %CALLNAME:調教対象%在阿洛美勒丝的调教中屈服了,成为了阿洛美勒丝的宠物……！
			PRINTFORML
			RESETCOLOR
			CALL EROTIC_MOVE("全身レズ愛撫",調教対象, -1)
		ENDIF
    ENDIF

ELSEIF 種類 == 発動誘発
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,40079,2,X消費1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(決闘者,"発動無効",場所,"選択")
		IF RESULT == 1
			選択カード = RESULT:1
			IF 選択カード != 9999
				TRYCALLFORM CARDNAME_{GETVAR(@"%対面者%_戦場:選択カード")}("名前")
				調教対象 = GETVAR(@"%対面者%_戦場:選択カード")

				CALL CARD_EROTICS(調教対象)
				IF RESULT != -1
					調教対象 = RESULT
					SETCOLOR C_PINK
					PRINTFORML
					PRINTFORMW %CALLNAME:調教対象%被阿洛美勒丝偷袭,就那样被捕获改造成了蜜罐……！
					PRINTFORML
					RESETCOLOR
					CALL EROTIC_MOVE("苗床捕縛",調教対象, -1)
				ENDIF
				効果追加カテ = 蜜罐
				CALL CARD_EQUIP_MONSTER(決闘者,場所,"相手戦場",選択カード,(1 + 2))
			ENDIF
		ENDIF
	ENDIF

ELSEIF 種類 == ターン終了時
	CALL CARD_CHOICE(決闘者,種類,ゾーン,場所,40079,3)
	IF RESULT == 0
		FOR カウンタ,0,5
			SIF GETVAR(@"%決闘者%_魔罠:カウンタ") == -1
				CONTINUE
			SIF GETVAR(@"%決闘者%_魔_装:カウンタ") != 場所
				CONTINUE
			SIF GETVAR(@"%決闘者%_魔_リ数:カウンタ") >= 10
				CONTINUE
		
			CALL CARD_EROTICS(GETVAR(@"%決闘者%_魔罠:カウンタ"))
			IF RESULT != -1
				調教対象 = RESULT
				TRYCALLFORM CARD_{GETVAR(@"%決闘者%_魔罠:カウンタ")}, "性別"
				IF RESULT == 牝性
					SETCOLOR C_PINK
					PRINTFORML
					PRINTFORML 阿洛美勒丝用舌头在%CALLNAME:調教対象%腔内贪婪的汲取着养分。
					PRINTFORMW 阿洛美勒丝的攻守与エクシーズ素材増加了！
					PRINTFORML
					RESETCOLOR
					CALL EROTIC_MOVE("クリトリス凌辱",調教対象,-1)
					CALL SET_FIELD_INFO(決闘者,"魔罠",カウンタ,"リ数",1,0,1)
				ELSEIF RESULT == 雄性 || RESULT == ふたなり
					SETCOLOR C_PINK
					PRINTFORML
					PRINTFORML 阿洛美勒丝用嘴榨取着%CALLNAME:調教対象%的精液。
					PRINTFORMW 阿洛美勒丝的攻守与エクシーズ素材増加了！
					PRINTFORML
					RESETCOLOR
					CALL EROTIC_MOVE("全身愛撫",調教対象,-1)
					CALL SET_FIELD_INFO(決闘者,"魔罠",カウンタ,"リ数",1,0,1)
				ENDIF
			ENDIF
		NEXT

		CALL SET_FIELD_INFO(決闘者,"戦場",場所,"永攻",1000)
		CALL SET_FIELD_INFO(決闘者,"戦場",場所,"永守",1000)
		CALL SET_FIELD_INFO(決闘者,"戦場",場所,"Ｘ",1)
	ENDIF

ELSEIF 種類 == 永続効果
    CALL FULLRESET
	CALL CARD_EQUIP_CHECK(決闘者,場所,ゾーン)
	SIF RESULT == 1
	CALL STATCHANGE_EFFECT(決闘者,"自身",場所,"戦無")

	IF GETVAR(@"%決闘者%_MZ_Ｘ:場所") >= 0
		CALL FULLRESET
		CALL STATCHANGE_EFFECT(決闘者,"自身",場所,"罠耐")
	ENDIF

ELSEIF 種類 == リアクション効果
	FOR カウンタ,0,5
		SIF GETVAR(@"%決闘者%_魔罠:カウンタ") == -1
			CONTINUE
		SIF GETVAR(@"%決闘者%_魔_装:カウンタ") != 場所
			CONTINUE
		SIF GETVAR(@"%決闘者%_魔_リ数:カウンタ") >= 10
			CONTINUE
			
		CALL CARD_EROTICS(GETVAR(@"%決闘者%_魔罠:カウンタ"))
		IF RESULT != -1
			調教対象 = RESULT
			TRYCALLFORM CARD_{GETVAR(@"%決闘者%_魔罠:カウンタ")}, "性別"
			IF RESULT == 牝性
				SETCOLOR C_PINK
				PRINTFORML
				PRINTFORML 阿洛美勒丝的手指刺激着%CALLNAME:調教対象%的腔道。
				PRINTFORML
				RESETCOLOR
				CALL EROTIC_MOVE("クリトリス凌辱",調教対象,-1)
				CALL SET_FIELD_INFO(決闘者,"魔罠",カウンタ,"リ数",1,0,1)
			ELSEIF RESULT == 雄性 || RESULT == ふたなり
				SETCOLOR C_PINK
				PRINTFORML
				PRINTFORML 阿洛美勒丝撸动着%CALLNAME:調教対象%的肉棒。
				PRINTFORML
				RESETCOLOR
				CALL EROTIC_MOVE("全身愛撫",調教対象,-1)
				CALL SET_FIELD_INFO(決闘者,"魔罠",カウンタ,"リ数",1,0,1)
			ENDIF
		ENDIF
	NEXT
ENDIF