;上枠部分
;########
FONTSTYLE 1
SETCOLOR 色_枠
PRINTFORML %UNICODE(0x2582)*82%
RESETCOLOR
FONTREGULAR

;############
;スロット上段
;############
FOR LOCAL, 1, 10
	SETCOLOR 色_枠
	PRINTFORM %UNICODE(0x2589)%
	RESETCOLOR
	CALL SLOT_DISPLAY(絵柄_左上,LOCAL,暗転_左上)
	SETCOLOR 色_枠
	PRINTFORM |%UNICODE(0x2589)%|
	RESETCOLOR
	CALL SLOT_DISPLAY(絵柄_中上,LOCAL,暗転_中上)
	SETCOLOR 色_枠
	PRINTFORM |%UNICODE(0x2589)%|
	RESETCOLOR
