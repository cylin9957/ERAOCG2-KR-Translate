;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K2035_310
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K2035_310_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K2035_310_1
#DIM DYNAMIC 호감도修正
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;친밀。ここを記入した場合、これより下は流れない
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1
		;６以上
		IF ABL:TARGET:친밀 >= 6
			PRINTFORMW 
		;３以上
		ELSEIF ABL:TARGET:친밀 >= 3
			PRINTFORMW 
		;２以下
		ELSEIF ABL:TARGET:친밀 <= 2
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF

	;基本セット
	호감도修正 = 3
	SIF TALENT:TARGET:애처
		호감도修正 += 0
	SIF TALENT:TARGET:연모 == 2
		호감도修正 += 1
	SIF TALENT:TARGET:연모 >= 1
		호감도修正 += 1
	SELECTCASE RAND:호감도修正 + 1
		CASE 1
			PRINTFORMW 「히양！？　부、부끄러워요」
		CASE 2
			PRINTFORMW 「텔레비에서 봤어요……。이거、성희롱이라고 하는 거죠……？」
		CASE 3
			PRINTFORMW 「……%CALLNAME:MASTER% 씨、엣치。호색가」
		;연모
		CASE 4
			PRINTFORML 「……%CALLNAME:MASTER% 씨는 멋진 분이시니까、여성에게 이런 짓 하면 안 되잖아요？」
			PRINTFORMW 「저니까 봐주는 거예요？　알겠어요？」
		;親愛
		CASE 5
			PRINTFORMW 「제 엉덩이、그렇게 좋아하세요？　……후훗♪」
	ENDSELECT
ENDIF
RETURN 1
