;-------------------------------------------------
IF LOCAL
	IF TFLAG:SELECTCOM분기 == 1 || TFLAG:SELECTCOM분기 == 0
		PRINTFORMW 
	ELSE
		PRINTFORMW 
	ENDIF
ENDIF
RETURN 1

;==================================================
;617,甘味処に誘う
;TFLAG:SELECTCOM분기(1=大成功 0=成功 -1=失敗)(地の文분기は大成功のみ추가
;==================================================
@M_KOJO_SUCCESS_COM_K2035_617
;成否판정
;TFLAG:192(-2=コマンド終了、-1＝強制失敗、0=COM依存、1=強制成功or大成功
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K2035_617
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K2035_617_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K2035_617_1
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	SELECTCASE TFLAG:SELECTCOM분기
		CASE 1,0
			PRINTFORMW 「甘いものですかっ！？　……え、あれ、今日そんな特別な日でしたっけ！？」
		CASE -1
			PRINTFORMW 「そんな、なんでもないときに甘いものだなんて、畏れおおいです」
	ENDSELECT
ENDIF
RETURN 1
