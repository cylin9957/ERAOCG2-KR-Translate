TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K2035_605
;家に誘うにはTRAIN_MESSAGEが無い
CALL M_KOJO_MESSAGE_COM_K2035_605_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K2035_605_1
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
