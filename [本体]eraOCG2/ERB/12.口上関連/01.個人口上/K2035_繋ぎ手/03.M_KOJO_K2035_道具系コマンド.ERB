CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K2035_40_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K2035_40_1
#DIM DYNAMIC 호감도修正
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「それ、なんですか？　ピンク色でかわいいですねっ」
		PRINTFORMW 「はぁ、股にあてる……それでどうっっつ！？」
		RETURN 1
	ENDIF
	;基本セット
	;４以上
	IF ABL:TARGET:Ｃ감각 >= 4
		SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「あ゛あ゛あ゛、あ゛あ゛ぁ゛ぁ゛ぁ゛ぁ゛あ゛あ゛あ゛あ゛……❤」
			CASE 1
				PRINTFORMW 「ダメっ❤あなたのっ❤%CALLNAME:MASTER%さんの手でっ❤イきたいのにぃ❤」
			CASE 2
				PRINTFORMW 「い゛ゃぁ❤よわよわクリトリス、許してくださいぃぃい゛❤」
		ENDSELECT
	;２未満
	ELSEIF ABL:TARGET:Ｃ감각 < 2
		SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「う゛わっ！？　……ごめんなさい、変な声でちゃいました」
			CASE 1
				PRINTFORMW 「っ！？　こ、これ、ちょっと慣れませんね」
		ENDSELECT
	;４未満
	ELSEIF ABL:TARGET:Ｃ감각 < 4
		SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「と、止めてください！　ちょっと敏感なところにあたって……い゛っ❤」
				PRINTFORMW 敏感なところにあたりつづける刺激に、%CALLNAME:TARGET%は体を仰け反らせている
			CASE 1
				PRINTFORMW 「ひゃあ❤いぎぃ❤へんなっ、声❤でちゃう……❤」
		ENDSELECT
	ENDIF

ENDIF
RETURN 1

