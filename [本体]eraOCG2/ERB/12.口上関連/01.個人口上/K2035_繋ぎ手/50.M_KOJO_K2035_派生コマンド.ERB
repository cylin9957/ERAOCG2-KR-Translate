;1,シックスナイン
;==================================================
@M_KOJO_MESSAGE_SCOM_K2035_1
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_SCOM_K2035_1_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K2035_1_1
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 「お、お互いに舐め合うんですかっ、なんだかその、えっちぃですね……」
		RETURN 1
	ENDIF
	;基本セット
	;연모
	IF TALENT:연모
				IF ABL:TARGET:정액중독 >= 2
					PRINTFORML 「んっ！　んぢゅっ♪　ぐぽ……っ♪ぐぷぷっ❤」
					PRINTFORMW %CALLNAME:TARGET%はクリトリスを舐められるたびに身を震わせながらも、それ以上に眼の前のチンポを味わうことに夢中だ
				ELSE
					PRINTFORML 「あ゛っ、……んふふっ❤じゅぽっ、ぐちゅちゅ……ッ❤」
					PRINTFORMW 「……ぷはぁ、えへへ♪……お互いに求めあってる感があって、好きかも、これ❤」
				ENDIF
		RETURN 1

	;それ以外
	ELSE
		PRINTFORMW 「んんっ……、その、刺激が、強すぎて、うまくできな──ッ！？」
		RETURN 1
	ENDIF
ENDIF
;未記入
;CALLFORM M_KOJO_MESSAGE_COM_K2035_{SELECTCOM}_1
RETURN 1

