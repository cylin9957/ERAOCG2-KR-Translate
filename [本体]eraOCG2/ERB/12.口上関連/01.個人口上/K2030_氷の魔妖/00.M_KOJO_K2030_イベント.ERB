;--------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------
IF LOCAL
	;空行
	PRINTFORML
	;-------------------------------------------------
	;一日初めての挨拶분기（MAIN/お外どちらも）
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && GROUPMATCH(ARG,1,2,6) && CFLAG:TARGET:인사했음 == 0
		IF TALENT:TARGET:애처
			PRINTFORMW 「おはよう、わたしの%CSTR:TARGET:MASCALL%。……ふふっ♪」
		ELSEIF TALENT:TARGET:굴복됨
				PRINTFORMW 「おはようございますっ、%CSTR:TARGET:MASCALL%❤」
				PRINTFORMW 「おっともう朝でしたね。……朝だったわね」
		ELSEIF TALENT:TARGET:연모 
			PRINTFORMW 「%CSTR:TARGET:MASCALL%か、おはよう。わたしに会うなんて災難ね、こっちからしたら幸運だけど」
		ELSE
			PRINTFORMW 「……ああ、%CSTR:TARGET:MASCALL%か。災難ね、わたしに会うなんて」
		ENDIF
		RETURN 1
	ENDIF

	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORML 「いるわよー」
		PRINTFORMW %CALLNAME:TARGET%はこちらを一瞥し、袖をひらひら振った
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;eraOCG2_V0.40_BT_240518時点では流れないため注意
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 「あら、こんなところにいたのね」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて一緒に入る
	;eraOCG2_V0.40_BT_240518時点では流れないため注意
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML 「あら、%CSTR:TARGET:MASCALL%も来たの」
		PRINTFORMW 「なにバカみたいにほうけているの。入ってきたなら背中でも流しなさい」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて出て行った
	;eraOCG2_V0.40_BT_240518時点では流れないため注意
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORML 「……そ、入るのね。じゃ」
		PRINTFORMW 「ああ、雪女のあとだから浴槽が冷えてないといいわね」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて追い出された
	;未確認
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORMW 「……%CSTR:TARGET:MASCALL%、乙女の諸肌をなんだと思ってるの。家主？　関係ない、でてけ」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で遭遇した
	;eraOCG2_V0.40_BT_240522時点では流れないため注意
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		IF TALENT:TARGET:연모 
			PRINTFORMW 「こんなところで会うなんて、……たまには神に感謝してやってもいいわね」
		ELSE
			PRINTFORMW 「あら、%CSTR:TARGET:MASCALL%。奇遇ね」 
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;他캐릭터と데이트중に遭遇した
	;eraOCG2_V0.40_BT_240522時点では流れないため注意
	;ARG:1にデート상대
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		IF TALENT:TARGET:연모 
			PRINTFORML 「……ふぅん、ま、いいわ。邪魔したらそっちの娘にもわるいしね」
			PRINTFORMW 「……埋め合わせに期待、しとくわ」
		ELSE
			PRINTFORMW 「へぇ、意外と隅に置けないのね」
		ENDIF 
			RETURN 1
	ENDIF
ENDIF
RETURN 1

