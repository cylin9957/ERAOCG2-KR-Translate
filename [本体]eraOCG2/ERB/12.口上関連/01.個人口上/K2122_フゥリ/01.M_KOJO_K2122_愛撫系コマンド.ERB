@M_KOJO_MESSAGE_COM_K2122_0_1
#DIM DYNAMIC 호감도修正
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW "으앗! 주인님……조금만 더 상냥하게……"
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;삽입中
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	;V삽입
	IF LOCAL:1 && TEQUIP:50 == MASTER
		;사정した
		IF NOWEX:MASTER:사정
			IF TALENT:TARGET:6
				IF RAND:5 == 0
				;アラヒメ
					PRINTFORMW "구읏으으응──만약 임신이라도 하면, 절대로 용서하지 않을 테니까……❤"
				ELSE
					PRINTFORMW "으아아앗❤ 오빠 걸로, 자궁이, 가득──"
				ENDIF
			ELSE
				PRINTFORMW "응앗❤하앗❤ 주인님, 안이, 너무 뜨거워──"
			ENDIF
			RETURN 1
		ELSE
			SELECTCASE RAND:3
				CASE 0
					IF TALENT:TARGET:6
						IF RAND:5 == 0
						;アラヒメ
							CALL M_KOJO_MESSAGE_COM_K2122_GROAN_2
						ELSE
							CALL M_KOJO_MESSAGE_COM_K2122_GROAN_3
						ENDIF
					ELSE
						PRINTFORMW "하앗❤ 안에, 들어와……"
					ENDIF
				CASE 1
					CALL M_KOJO_MESSAGE_COM_K2122_GROAN_1
				CASE 2
					PRINTFORMW "하앗❤ 안, 기분 좋아……"
			ENDSELECT
			RETURN 1
		ENDIF
	;A삽입
	ELSEIF LOCAL:1 && TEQUIP:51 == MASTER
		;사정した
		IF NOWEX:MASTER:사정
			IF TALENT:TARGET:6
				IF RAND:5 == 0
					;アラヒメ
					PRINTFORMW "구읏으으으응❤!──감히 장에 싸다니!!"
					;谷道は直腸です、アラヒメは少し분노ます
				ELSE
					PRINTFORMW "으아아앗❤! 오빠, 뱃속이, 데일 것 같아──"
				ENDIF
			ELSE
				PRINTFORMW "으아아앗❤ 배가, 뜨거워──"
			ENDIF
			RETURN 1
		ELSE
			SELECTCASE RAND:3
				CASE 0
					IF TALENT:TARGET:6
						IF RAND:5 == 0
						;アラヒメ
							CALL M_KOJO_MESSAGE_COM_K2122_GROAN_3
						ELSE
							CALL M_KOJO_MESSAGE_COM_K2122_GROAN_2
						ENDIF
					ELSE
						PRINTFORMW "하앗❤ 엉덩이가, 범해져서……"
					ENDIF
				CASE 1
					CALL M_KOJO_MESSAGE_COM_K2122_GROAN_1
				CASE 2
					PRINTFORMW "하앗❤ 엉덩이 구멍이, 기분 좋아……"
			ENDSELECT
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;절정した
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && NOWEX:0
		IF TALENT:TARGET:6
			IF RAND:5 == 0
			;アラヒメ
				PRINTFORMW "으아악, 하앗, 안돼, 싫어싫어싫어싫어──"
				CALL M_KOJO_MESSAGE_COM_K2122_ECSTASY_3
			ELSE
				PRINTFORMW "으아앗❤ 오빠가 만져주니까, 기분 좋은 게, 밀려와서……"
				PRINTFORMW "가❤가❤ 오빠, 가버렷❤!!"
				CALL M_KOJO_MESSAGE_COM_K2122_ECSTASY_2
			ENDIF
		ELSE
				PRINTFORMW "으아앗❤ 주인님의 애무로, 이제……"
				PRINTFORMW "가❤가요❤ 주인님, 가요❤!!"
				CALL M_KOJO_MESSAGE_COM_K2122_ECSTASY_1
		ENDIF 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;ふたなり
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && (TALENT:2 & 3) == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	IF TALENT:TARGET:6
		SELECTCASE RAND:6
			CASE 0
				;アラヒメ
				PRINTFORMW "으아아아~앗❤ 흠, 주인의 여자를 기쁘게 하는 기술, 제법 나쁘지 않구나"
				;唔姆:ふむ
			CASE 1
				PRINTFORMW "오빠……더 만져줘~❤"
			CASE 2
				CALL M_KOJO_MESSAGE_COM_K2122_GROAN_2
			CASE 3
				PRINTFORMW 이미 속속들이 알고 있는 %CALLNAME:TARGET%의 성감대를 애무했다……
			CASE 4
				PRINTFORMW %CALLNAME:TARGET%에게 가장 큰 쾌감을 줄 수 있는 세기로 애무했다……
			CASE 5
				PRINTFORMW "으으응❤ % RAND:2 == 1 ? "귀" # "꼬리" %, 기분 좋아……❤"
		ENDSELECT
	ELSE
		SELECTCASE RAND:5
			CASE 0
				PRINTFORMW "주인님……더, 만져주세요……❤"
			CASE 1
				CALL M_KOJO_MESSAGE_COM_K2122_GROAN_1
			CASE 2
				PRINTFORMW %CALLNAME:TARGET%의 민감한 부분을 신중하게 애무했다……
			CASE 3
				PRINTFORMW %CALLNAME:TARGET%의 민감한 부분을 강하게 애무했다……
			CASE 4
				PRINTFORMW "으으응❤ % RAND:2 == 1 ? "귀" # "꼬리" %는, 아, 안 돼요……❤"
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
