@M_KOJO_MESSAGE_COM_K2122_3_1
#DIM DYNAMIC 호감도修正
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	;基本的にはセクハラと同じです
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
        PRINTFORML "주인님, 거기는……으아앗! 이대로 직접 삽입되면, 조금, 아파요……"
        PRINTFORMW  %CALLNAME:TARGET%의 질내에 손가락을 넣었지만, 전혀 익숙하지 않은 것 같다。
		;小穴=vagina=阴道
	    RETURN 1
	ENDIF
	;-------------------------------------------------
	;절정した
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && NOWEX:0
		IF TALENT:TARGET:6
			IF RAND:5 == 0
			;アラヒメ
				PRINTFORMW "구웃, 하아 하아 하아, 손가락만으로……"
				CALL M_KOJO_MESSAGE_COM_K2122_ECSTASY_3
			ELSE
				PRINTFORMW "%CALLNAME:TARGET%의 안, 엄청 기분 좋아서,
				PRINTFORMW 　아아아앗, 보지가 기분 좋아, 오빠, 오빠――"
			CALL M_KOJO_MESSAGE_COM_K2122_ECSTASY_2
			ENDIF
		ELSE
			PRINTFORMW "아아아앗, 주인님, 거기가, 너무 기분 좋아서, 가, 가버려――"
			CALL M_KOJO_MESSAGE_COM_K2122_ECSTASY_1
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;삽입中
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	;A삽입
	IF LOCAL:1 && TEQUIP:51 == MASTER
	;사정した
		IF NOWEX:MASTER:사정
			IF TALENT:TARGET:6
				IF RAND:5 == 0
					;アラヒメ
					PRINTFORMW "구읏으으으응❤!──감히 장에 싸다니!!"
					;谷道は直腸です、アラヒメは少し분노ます
				ELSE
					PRINTFORMW "으아아앗❤! 오빠, 뱃속이, 데일 것 같아──"
				ENDIF
			ELSE
				PRINTFORMW "으아아앗❤ 배가, 뜨거워──"
			ENDIF
			RETURN 1
		ELSE
			SELECTCASE RAND:3
				CASE 0
					IF TALENT:TARGET:6
						IF RAND:5 == 0
						;アラヒメ
							CALL M_KOJO_MESSAGE_COM_K2122_GROAN_3
						ELSE
							CALL M_KOJO_MESSAGE_COM_K2122_GROAN_2
						ENDIF
					ELSE
						PRINTFORMW "하앗❤ 엉덩이가, 범해져서……"
					ENDIF
				CASE 1
					CALL M_KOJO_MESSAGE_COM_K2122_GROAN_1
				CASE 2
					PRINTFORMW "하앗❤ 엉덩이 구멍이, 기분 좋아……"
			ENDSELECT
			RETURN 1
		ENDIF
	ENDIF
	;基本セット
	IF TALENT:TARGET:6
	;在狐理成为애처之后，荒姬才会加入SEX活动。
		IF RAND:5 == 0
		;アラヒメ
			IF ABL:Ｖ감각  <= 2
				PRINTFORML "응... 갑자기 질에 손가락을 넣지 마라……주인이 %CALLNAME:TARGET%의 반려가 아니었다면……나는 진작에……❤"
				;春心=vagina，古中国语
				PRINTFORMW %CALLNAME:TARGET%의 질내에 손가락을 찔러넣고 휘젓자, 아직 익숙하지 않은지, 조금 이를 악물고 참고 있는 것처럼 보였다……
			ELSEIF ABL:Ｖ감각 <= 4
				PRINTFORML "크아, 크으으으……주인 같은 인간에게, 어째서 나의 질은 여기까지……"
				PRINTFORMW %CALLNAME:TARGET%의 질내에 손가락을 침입시켰다……
				PRINTFORMW 그녀는 질내에 손가락이 들어오는 감각에 익숙해진 것 같다.
			ELSE
				SELECTCASE RAND:4
					CASE 0
						PRINTFORML "크음, 그저 인간, 보잘것없는 인간이다……주인, 손을 멈추지 마라!"
					CASE 1
						CALL M_KOJO_MESSAGE_COM_K2122_GROAN_3
					CASE 2
						PRINTFORML "나는 요호다……쾌락에 빠질 수는……음?"
						PRINTFORMW "어이, 멈추라고는 하지 않았다!"
					CASE 3
						PRINTFORML "오빠──주인, 손가락을 더 움직여라! 꺄아아아……❤"
				ENDSELECT
				SIF TALENT:TARGET:음호
					PRINTFORMW %CALLNAME:TARGET%는 애액으로 뒤덮인 질내에 손가락을 추삽할 때마다 격렬하게 흐트러지며, 신음하고 있다.
