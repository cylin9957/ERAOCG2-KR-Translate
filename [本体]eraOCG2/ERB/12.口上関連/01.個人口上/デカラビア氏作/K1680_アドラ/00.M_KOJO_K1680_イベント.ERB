;==================================================
;朝起きたとき（おはよう구상）
;==================================================
@M_KOJO_EVENT_K1680_2(ARG,ARG:1)
;구상主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------
IF LOCAL
	;空行
	PRINTFORML
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;非同棲かつ상대가 先に起きてる
	;eraOCG2_V0.40_BT_240522時点では流れる상황を作れない
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;TARGET私室にMASTERが入っている（연모時添い寝中취침時のみ？
		;eraOCG2_V0.40_BT_240522時点では、TARGETの私室には入れない
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTER私室にTARGETが入ってきてる(연모無しでも入ってくる可能性有り
		;eraOCG2_V0.40_BT_240522時点では、MASTER의수면＝1日の終了のため、実質流れない
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ상대가 起きている
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
PRINTFORMW 「안녕 %CALLNAME:MASTER% 오늘도 하루 힘내자」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ상대가 寝ている
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
PRINTFORMW 「응으음… 괜찮아… 제대로 일어날게.」
		RETURN 1
	ENDIF
	[SKIPSTART]
	;-------------------------------------------------
	;MASTERが起きていて、상대가 起きてきた
	;添い寝時TARGET起床はこちら（TARGET쇠약or添い寝中TARGET睡魔負け後그대로 MASTERが起き続けた上で상대가 起床した場合限定
	;eraOCG2_V0.40_BT_240522時点では流れることを確認できなかったため주의
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;-------------------------------------------------
		;非同棲
		;eraOCG2_V0.40_BT_240522時点では、非同棲時に寝るとTARGETの自室に帰ってしまう。주의
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;同棲中
		;記入チェック（=0, 非표시、1, 표시）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	[SKIPEND]
	;その他
	PRINTFORMW   
ENDIF
RETURN 1

