;==================================================
;0,デュエルスタート！
;==================================================
@M_KOJO_DUEL_K2624_0(カードID,ARG:1)
#DIM DYNAMIC 호감도修正
#DIM DYNAMIC カードID
#DIMS DYNAMIC カード名
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	SELECTCASE RAND:3 + 1
		CASE 1
			PRINTFORMW 「자、받아보시지!」
			PRINTFORMW 「나의 팬 서비스를!」
		CASE 2
			PRINTFORMW 「흥!」
			PRINTFORMW 「하아아아아――!」	
		CASE 3
			PRINTFORMW 「대전 상대 찾기가 정말 힘들어」
			PRINTFORMW 「어쨌든 팬이 너무 많아서 말이야」
	ENDSELECT
ENDIF

;==================================================
;1,モンスター소환
;例：%カード名%を소환！
;==================================================
@M_KOJO_DUEL_K2624_1(カードID,ARG:1)
#DIM DYNAMIC 호감도修正
#DIM DYNAMIC カードID
#DIMS DYNAMIC カード名
TRYCALLFORM CARDNAME_{カードID}("名前")
カード名 = %RESULTS%
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF カードID == 24064
		PRINTFORMW 「나오너라!」
		PRINTFORMW 「지옥에서 온 사자 운명의 실을 조종하는 인형!」
		PRINTFORMW 「기믹・퍼핏－리틀・솔저즈!」
	ELSE
		SELECTCASE RAND:2 + 1
			CASE 1
				PRINTFORMW 「자、한다!」
				PRINTFORMW 「몬스터를 소환!」
			CASE 2
				PRINTFORMW 「보아라!」
				PRINTFORMW 「%カード名%을 공격 표시로 소환!」
		ENDSELECT
	ENDIF
ENDIF


;==================================================
;2,モンスター특수 소환
;==================================================
@M_KOJO_DUEL_K2624_2(カードID,소환出処)
#DIM DYNAMIC 호감도修正
#DIM DYNAMIC カードID
#DIM DYNAMIC 소환出処
#DIMS DYNAMIC カード名
TRYCALLFORM CARDNAME_{カードID}("名前")
カード名 = %RESULTS%
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF カードID == 24064
		PRINTFORMW 「나오너라!」
		PRINTFORMW 「지옥에서 온 사자 운명의 실을 조종하는 인형!」
		PRINTFORMW 「기믹・퍼핏－리얼・솔저즈!」
	ELSE
		SELECTCASE 소환出処
			;손패から
			CASE 召種손패
				PRINTFORMW 「자、와라!」
				PRINTFORMW 「몬스터를 특수 소환!」
			;마법함정ゾーンから。　キリ番・シェイプシスターなどを出す場合
			CASE 召種마함
				PRINTFORMW
			;덱から
			CASE 召種덱
				PRINTFORMW 「나오너라! %カード名%!」
			;EX덱から
			CASE 召種EX덱
				PRINTFORMW 「자、와라!」
				PRINTFORMW 「몬스터를 특수 소환!」
			;묘지から
			CASE 召種묘지
				PRINTFORMW 「자、와라!」
				PRINTFORMW 「몬스터를 특수 소환!」
			;제외状態から
			CASE 召種제외
				PRINTFORMW
				PRINTFORMW
			;主にトークン生成
			CASE 10
				PRINTFORMW
		ENDSELECT
	ENDIF
ENDIF

;==================================================
;3,특수モンスター소환
