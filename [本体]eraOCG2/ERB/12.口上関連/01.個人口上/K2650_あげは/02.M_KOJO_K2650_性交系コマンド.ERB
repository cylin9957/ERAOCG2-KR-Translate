@M_KOJO_MESSAGE_COM_K2650_68
CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K2650_68_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K2650_68_1
#DIM DYNAMIC 호감도修正
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;처녀
		IF TCVAR:파과 == 1
			CALL M_KOJO_MESSAGE_COM_K2650_SHOJO_BREAK
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;삽입계속
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:V삽입관리플래그 == MASTER
		;前回사정한
		IF TCVAR:질내사정플래그 
			CALL M_KOJO_MESSAGE_COM_K2650_SEXCHANGE_2
			RETURN 1
		ELSE
			CALL M_KOJO_MESSAGE_COM_K2650_SEXCHANGE_1
			RETURN 1
		ENDIF
	ENDIF
	호감도修正 = 3
	SIF TALENT:TARGET:애처
		호감도修正 += 0
	SIF TALENT:TARGET:연모 == 2
		호감도修正 += 1
	SIF TALENT:TARGET:연모 >= 1
		호감도修正 += 1
	IF TALENT:TARGET:연모 >= 1
		SELECTCASE RAND:3
			CASE 1
PRINTFORMW 「응츄웁♡ 키스하면서…앙♡ 더 찔러줘어…♡」
			CASE 2
PRINTFORMW 「하앙♡ 안아줘…더 세게…앗♡ 좋아아…♡」
			CASE 3
PRINTFORMW 「앙♡ %CALLNAME:MASTER%의 얼굴…가까워어…♡ 부끄럽지만…좋아아…♡」
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
			CASE 1
PRINTFORMW 「이런 거 싫어어…」
			CASE 2
PRINTFORMW 「으응긋! 그렇게 안지 마아…」
			CASE 3
PRINTFORMW 「으으윽! …얼굴 돌려…!」
		ENDSELECT
	ENDIF
	RETURN 1
ENDIF
RETURN 1

