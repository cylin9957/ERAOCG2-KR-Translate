#DIM DYNAMIC 호감도修正
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	호감도修正 = 3
	SIF TALENT:TARGET:애처
		호감도修正 += 0
