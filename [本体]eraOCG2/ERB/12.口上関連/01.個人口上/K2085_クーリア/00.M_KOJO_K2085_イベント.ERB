;==================================================
@M_KOJO_EVENT_K2085_1(ARG,ARG:1)
;구상主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
IF LOCAL:1 && ARG == 1
PRINTFORMW 「안녕, 지휘자」
PRINTFORMW 쿨리아는 가볍게 인사했다.
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
PRINTFORMW 「뜻밖이네, 지휘자」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが목욕に入ってきて一緒に入る
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
PRINTFORMW 「흠… 나도 들어갈까」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが목욕に入ってきて出て行った
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
PRINTFORMW 「읏… 이건… 실례할게」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが목욕に入ってきて追い出された
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
PRINTFORMW 「……다음엔 조심해」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;외출先で遭遇した
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
PRINTFORMW  「이런 곳에서 만나다니」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;他캐릭터と데이트중に遭遇した
	;ARG:1に데이트상대
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		PRINTFORMW  
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MAIN_MAP上で一日最初の挨拶
	;注、このあと通常の遭遇구상、ARG==1 OR ARG==2に接続します
	;記入チェック（=0, 非표시、1, 표시）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 8
		PRINTFORMW  
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;외출先で一日最初の挨拶
	;ARG:1に데이트상대、ARG:1が0の場合は非데이트
	;注、このあと通常の외출先で遭遇した구상、ARG==6 OR ARG==7に接続します
	;記入チェック（=0, 非표시、1, 표시）
