	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF CFLAG:우후후
			;우후후中に眠くて帰る
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
