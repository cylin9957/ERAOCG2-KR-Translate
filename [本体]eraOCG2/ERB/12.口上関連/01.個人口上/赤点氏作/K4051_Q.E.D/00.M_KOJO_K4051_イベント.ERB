		IF ARG:1 == 0
			;애널
			PRINTFORMW 
		ELSEIF ARG:1 == 1
			;【엉덩이광】취득
			PRINTFORMW 
		ENDIF
	ENDSELECT
	PRINTL 
ENDIF
RETURN 1
