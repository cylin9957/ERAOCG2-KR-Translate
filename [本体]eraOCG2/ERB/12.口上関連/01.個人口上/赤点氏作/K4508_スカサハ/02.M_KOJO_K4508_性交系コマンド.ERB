CALL TRAIN_MESSAGE
CALL M_KOJO_MESSAGE_COM_K4508_60_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K4508_60_1
#DIM DYNAMIC 호감도修正
;-------------------------------------------------
;정상위
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小분기
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		;처녀
		IF TCVAR:파과 == 1
			CALL M_KOJO_MESSAGE_COM_K4508_SHOJO_BREAK
			RETURN 1
		ELSE
			PRINTFORMW 「～～～っ❤　は、入ったか……？❤」
			RETURN 1
		ENDIF
	ENDIF
	;-------------------------------------------------
	;삽입継続
	;삽입したままこの体位に切り替える
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:V삽입관리플래그 == MASTER
		;前回사정した
		IF TCVAR:질내사정플래그 
			CALL M_KOJO_MESSAGE_COM_K4508_SEXCHANGE_2
			RETURN 1
		ELSE
			CALL M_KOJO_MESSAGE_COM_K4508_SEXCHANGE_1
			RETURN 1
		ENDIF
	ENDIF
	;基本セット
	호감도修正 = 3
	SIF TALENT:TARGET:애처
		호감도修正 += 0
	SIF TALENT:TARGET:연모 == 2
		호감도修正 += 0
	SIF TALENT:TARGET:연모 >= 1
		호감도修正 += 0
	SELECTCASE RAND:호감도修正 + 1
		CASE 1
			PRINTFORMW 「マスターのが、삽입って……っ❤　あ、あぁ……❤　気持ち、良いぞ……っ❤」
		CASE 2
			PRINTFORMW 「う、動くな……っ❤　思った以上に、大きくて……っ！❤　ひゃああっ！？❤　待て、待てぇぇっ❤❤」
		CASE 3
			PRINTFORMW 「マスターっ❤　もっとぉ……っ❤　もっと、激しく動いてくれっ❤　生意気雑魚マンコ、壊してくれぇっ❤❤❤」
	ENDSELECT
ENDIF
RETURN 1

