		CASE 4
			PRINTFORMW 
		CASE 5
			PRINTFORMW 
		;親愛
		CASE 6
			PRINTFORMW 
		CASE 7
			PRINTFORMW 
		;애처
		CASE 8
			PRINTFORMW 
	ENDSELECT
ENDIF
