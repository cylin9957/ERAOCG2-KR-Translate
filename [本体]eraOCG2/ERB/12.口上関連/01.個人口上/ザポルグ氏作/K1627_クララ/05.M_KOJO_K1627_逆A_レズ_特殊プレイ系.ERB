	;正真正銘처녀상실
	IF TCVAR:파과 == 1
	;再生처녀상실
	ELSEIF TCVAR:파과 == 2
	;無自覚非처녀상실
	ELSEIF TCVAR:파과 == -1
	ENDIF
ENDIF

