	IF LOCAL:1 && ARG == 9
		;ARG:1と데이트중
		IF ARG:1
		;一人で외출
		ELSE
		ENDIF
		PRINTFORMW  
		RETURN 1
	ENDIF
ENDIF
RETURN 1

