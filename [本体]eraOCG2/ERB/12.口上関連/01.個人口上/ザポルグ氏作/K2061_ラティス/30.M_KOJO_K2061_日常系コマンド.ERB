@M_KOJO_MESSAGE_COM_K2061_302_1
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	RESULT = RAND:3
	PRINTFORM %CALLNAME:TARGET%의
	SELECTCASE RESULT
		CASE 0
			PRINT 꼬리
		CASE 1
			PRINT 머릿결
		CASE 2
			PRINT 뿔
	ENDSELECT
	PRINTL 을 쓰다듬었다
	IF TFLAG:SELECTCOM분기 == 1
		PRINTFORML %CALLNAME:TARGET%는 편안해 보인다…
	ELSEIF TFLAG:SELECTCOM분기 == -1
		PRINTFORML …조금 너무 강했나보다…
	ENDIF
	WAIT
	PRINTFORML
	SELECTCASE TFLAG:SELECTCOM분기
		CASE 1
			SELECTCASE RESULT
				CASE 0
					PRINTFORML 「아앙……♪　그 만지는 법、좋습니다……♪」
					PRINTFORMW %CALLNAME:TARGET%는 꼬리에서 힘을 빼고 있다……
				CASE 1
					PRINTFORML 「…………♪」
					PRINTFORMW %CALLNAME:TARGET%는 눈을 감고 있다……
				CASE 2
					PRINTFORMW 「응…… 조금、쑥스럽네요……❤」
			ENDSELECT
		CASE 0
			SELECTCASE RESULT
				CASE 0
					PRINTFORMW 「윽…… 꼬리는 기본적으로 예민하니까、상냥하게 건드려주세요♪」
				CASE 1
					PRINTFORMW 「손가락이 얽히지 않게、스윽 흘리듯이…… 그래요、그런 느낌입니다……♪」
				CASE 2
					PRINTFORMW 「후훗♪　아니요、조금 당황했을 뿐이니까、부디 계속해 주세요♪」
			ENDSELECT
		CASE -1
			PRINTFORMW 「윽……！　%CSTR:TARGET:MASCALL%、여성의 취급에는 충분히 주의해 주세요？」
	ENDSELECT
ENDIF
RETURN 1
