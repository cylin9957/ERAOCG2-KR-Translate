@M_KOJO_MESSAGE_COM_K2061_306_1
;-------------------------------------------------
;記入チェック（=0, 非표시、1, 표시）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;記入チェック（=0, 非표시、1, 표시）
	;질내사정횟수분기
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && TCVAR:TARGET:질내사정횟수 > 0
		SELECTCASE TCVAR:TARGET:질내사정횟수
			CASE 1 TO 5
