@MAT_EVENT_CHECK(듀얼리스트,カード名,使用素材名前)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC カード名
#DIMS REF 使用素材名前
#DIM DYNAMIC カウンタ

; PRINTFORML 소환カード名:%カード名%
; PRINTFORML オプション_特定素材이벤트:{オプション_特定素材이벤트}

; FOR カウンタ,0,10
;     PRINTFORMW 사용소재이름:{カウンタ}=%使用素材名前:カウンタ%
; NEXT

SIF オプション_特定素材이벤트 != 1
    RETURN

;単体지정
IF MATCH(カード名, "엑소시스터 미카에리스")
    IF MATCH(使用素材名前, "엑소시스터 에리스")
        CALL MAT_EVENT_ACT(듀얼리스트,"엑소시스터 에리스 변신")
    ENDIF
ENDIF

IF MATCH(カード名, "엑소시스터 카스피텔")
    IF MATCH(使用素材名前, "엑소시스터 스텔라")
        CALL MAT_EVENT_ACT(듀얼리스트,"엑소시스터 스텔라 변신")
    ENDIF
ENDIF

IF MATCH(カード名, "엑소시스터 아소필")
    IF MATCH(使用素材名前, "엑소시스터 소피아")
        CALL MAT_EVENT_ACT(듀얼리스트,"엑소시스터 소피아 변신")
    ENDIF
ENDIF

IF MATCH(カード名, "엑소시스터 지브린느")
    IF MATCH(使用素材名前, "엑소시스터 이렌느")
        CALL MAT_EVENT_ACT(듀얼리스트,"엑소시스터 이렌느 변신")
    ENDIF
ENDIF

IF MATCH(カード名, "엑소시스터 루키파르")
    IF MATCH(使用素材名前, "엑소시스터 마르파")
        CALL MAT_EVENT_ACT(듀얼리스트,"엑소시스터 마르파 변신")
    ENDIF
ENDIF

IF MATCH(カード名, "엑소시스터 미카에리스")
    IF !MATCH(使用素材名前, "엑소시스터 에리스")
        SIF MATCH(使用素材名前, "크라이시스터 에리스")
            CALL MAT_EVENT_ACT(듀얼리스트,"크라이시스터 에리스 변신")
    ENDIF
ENDIF

IF MATCH(カード名, "엑소시스터 카스피텔")
    IF !MATCH(使用素材名前, "엑소시스터 스텔라")
        SIF MATCH(使用素材名前, "크라이시스터 스텔라")
            CALL MAT_EVENT_ACT(듀얼리스트,"크라이시스터 스텔라 변신")
    ENDIF
ENDIF

IF MATCH(カード名, "엑소시스터 아소필")
    IF !MATCH(使用素材名前, "엑소시스터 소피아")
        SIF MATCH(使用素材名前, "크라이시스터 소피아")
            CALL MAT_EVENT_ACT(듀얼리스트,"크라이시스터 소피아 변신")
    ENDIF
ENDIF

IF MATCH(カード名, "엑소시스터 지브린느")
    IF !MATCH(使用素材名前, "엑소시스터 이렌느")
        SIF MATCH(使用素材名前, "크라이시스터 이렌느")
            CALL MAT_EVENT_ACT(듀얼리스트,"크라이시스터 이렌느 변신")
    ENDIF
ENDIF

IF MATCH(カード名, "데먼스미스의 크라이시스터")
    IF MATCH(使用素材名前, "마를 새기는 데먼스미스")
        CALL MAT_EVENT_ACT(듀얼리스트,"데먼스미스의 크라이시스터 소환")
    ENDIF
ENDIF

IF MATCH(カード名, "카타르시스터 마르파스")
    IF MATCH(使用素材名前, "크라이시스터 마르파")
        IF MATCH(使用素材名前, "카타르시스터 에리스텔라")
            IF MATCH(使用素材名前, "카타르시스터 소피렌느")
                CALL MAT_EVENT_ACT(듀얼리스트,"마르파스 소환")
            ENDIF
        ENDIF
    ENDIF
ENDIF


IF MATCH(カード名, "바르모니카의 음란신－러시비어")
    IF MATCH(使用素材名前:0, "서큐베 바르모니카")
        IF MATCH(使用素材名前:1, "셀레트리체 바르모니카")
            CALL MAT_EVENT_ACT(듀얼리스트,"러시비어 소환")
        ENDIF
    ENDIF
ENDIF

;複数지정
IF MATCH(使用素材名前, "알버스의 낙윤")
    IF MATCH(使用素材名前, "드래그마의 성녀 에클레시아") || MATCH(使用素材名前, "순백의 성녀 에클레시아")
        CALL MAT_EVENT_ACT(듀얼리스트,"알버스＋에클레시아")
    ENDIF
ENDIF

IF MATCH(使用素材名前, "데스피아의 도화 알베르") || MATCH(使用素材名前, "더 비스테드 알베르")
    IF MATCH(使用素材名前, "드래그마의 성녀 에클레시아") || MATCH(使用素材名前, "순백의 성녀 에클레시아") || MATCH(使用素材名前, "혁의 성녀 에클레시아")
        CALL MAT_EVENT_ACT(듀얼리스트,"알베르＋에클레시아")
    ENDIF
ENDIF


IF MATCH(カード名, "카타르시스터 에리스텔라")
    IF MATCH(使用素材名前, "크라이시스터 에리스") || MATCH(使用素材名前, "엑소시스터 에리스")
        IF MATCH(使用素材名前, "크라이시스터 스텔라") || MATCH(使用素材名前, "엑소시스터 스텔라")
            CALL MAT_EVENT_ACT(듀얼리스트,"에리스텔라 소환")
        ENDIF
    ENDIF
ENDIF

IF MATCH(カード名, "카타르시스터 소피렌느")
    IF MATCH(使用素材名前, "크라이시스터 소피아") || MATCH(使用素材名前, "엑소시스터 소피아")
        IF MATCH(使用素材名前, "크라이시스터 이렌느") || MATCH(使用素材名前, "엑소시스터 이렌느")
            CALL MAT_EVENT_ACT(듀얼리스트,"소피렌느 소환")
        ENDIF
    ENDIF
ENDIF



RETURN

@MAT_EVENT_ACT(듀얼리스트,実行コマンド)
#DIMS DYNAMIC 実行コマンド
#DIMS DYNAMIC 듀얼리스트



SELECTCASE 実行コマンド

    CASE "엑소시스터 에리스 변신"
        PRINTFORML
        PRINTFORML 에리스 「아멘트！」
        PRINTFORML 엑소시스터 에리스는 미카엘의 가호로 변신한다！
        PRINTFORMW

    CASE "엑소시스터 스텔라 변신"
        PRINTFORML
        PRINTFORML 스텔라 「아멘트！」
        PRINTFORML 엑소시스터 스텔라는 카스피텔의 가호로 변신한다！
        PRINTFORMW

    CASE "엑소시스터 소피아 변신"
        PRINTFORML
        PRINTFORML 소피아 「아멘트！」
        PRINTFORML 엑소시스터 소피아는 아나엘의 가호로 변신한다！
        PRINTFORMW

    CASE "엑소시스터 이렌느 변신"
        PRINTFORML
        PRINTFORML 이렌느 「아멘트！」
        PRINTFORML 엑소시스터 이렌느는 지브릴의 가호로 변신한다！
        PRINTFORMW

    CASE "엑소시스터 마르파 변신"
        PRINTFORML
        PRINTFORML 마르파 「아멘트」
        PRINTFORML 엑소시스터 마르파는 루시퍼의 가호로 변신한다！
        PRINTFORMW


    CASE "크라이시스터 에리스 변신"
        PRINTFORML
        PRINTFORML 에리스 「아멘트！」
        PRINTFORML 크라이시스터 에리스는 미카엘의 가호로 변신한다！
        PRINTFORML 에리스 「읏……！」
        PRINTFORML 하지만 미카엘의 빛은 칙칙하다…
        PRINTFORMW

    CASE "크라이시스터 스텔라 변신"
        PRINTFORML
        PRINTFORML 스텔라「아멘트！」
        PRINTFORML 크라이시스터 스텔라는 카스피엘의 가호로 변신한다！
        PRINTFORML 스텔라「읏……！」
        PRINTFORML 하지만 카스피엘의 빛은 칙칙하다…
        PRINTFORMW

    CASE "크라이시스터 소피아 변신"
        PRINTFORML
        PRINTFORML 소피아 「아멘트！」
        PRINTFORML 크라이시스터 소피아는 아나엘의 가호로 변신한다！
        PRINTFORML 소피아 「읏……！」
        PRINTFORML 하지만 아나엘의 빛은 칙칙하다…
        PRINTFORMW

    CASE "크라이시스터 이렌느 변신"
        PRINTFORML
        PRINTFORML 이렌느 「아멘트！」
        PRINTFORML 크라이시스터 이렌느는 지브릴의 가호로 변신한다！
        PRINTFORML 이렌느 「읏……！」
        PRINTFORML 하지만 지브릴의 빛은 칙칙하다…
        PRINTFORMW


    CASE "알버스＋에클레시아"
        PRINTFORM
        PRINTFORML 알버스 「에클레시아！」
        PRINTFORML 에클레시아 「알버스！」
        PRINTFORML 알버스와 에클레시아가 손을 잡은 순간、두 사람은 빛을 뿜으며 합쳐졌다！
        PRINTFORMW

	CASE "알베르＋에클레시아"
        PRINTFORM
        PRINTFORML 에클레시아 「미안해、알버스。이것도 마스터의 명령이니까」
        PRINTFORML 에클레시아 「저……알베르와 합체합니다」
        PRINTFORMW


	CASE "에리스텔라 소환"
        PRINTFORML
        SETCOLOR C_PINK
        PRINTFORML 『악령 아스모데우스』가 빛의 고리로 변하자、
        PRINTFORML 마치 도망칠 수 없도록 포위하는 형태로 펼쳐지며、
        PRINTFORML 싱크로 소재가 된 에리스와 스텔라를 빛의 점으로 변화시킨다…
        PRINTFORMW 악령에 저항하지 못하고 마음이 무너진 에리스와 스텔라는 서로 몸을 가까이 했다…！
        RESETCOLOR
        PRINTFORML
        CALL EROTIC_MOVE("全身凌辱",NO_TO_CHARANUM(2100),-1,500)
        CALL EROTIC_MOVE("全身凌辱",NO_TO_CHARANUM(2101),-1,500)
        PRINTFORML
        SETCOLOR C_PINK
        PRINTFORML 눈부신 빛 속에서 에리스와 스텔라 두 사람의 특징이 군데군데 보인다
        PRINTFORML 큰 키에 고혹적인 몸매의 타락한 구원소녀가 모습을 드러내자、
        PRINTFORMW 당신에게 공손하게 예를 올리고、대전상대 쪽으로 몸을 돌렸다 …！
        RESETCOLOR
        PRINTFORML
		SETVAR @"%듀얼리스트%_MZ_言発:現필드(듀얼리스트)", 1

	CASE "소피렌느 소환"
        PRINTFORML
        SETCOLOR C_PINK
        PRINTFORML 『악령 마르코시어스』가 빛의 고리로 변하자、
        PRINTFORML 마치 도망칠 수 없도록 포위하는 형태로 펼쳐지며、
        PRINTFORML 싱크 로소재가 된 소피아와 시렌느를 빛의 점으로 변화시킨다…
        PRINTFORMW 악령에 저항하지 못하고 마음이 무너진 소피아와 이렌느는 서로 몸을 가까이 했다…！
        RESETCOLOR
        PRINTFORML
        CALL EROTIC_MOVE("全身凌辱",NO_TO_CHARANUM(2102),-1,500)
        CALL EROTIC_MOVE("全身凌辱",NO_TO_CHARANUM(2103),-1,500)
        PRINTFORML
        SETCOLOR C_PINK
        PRINTFORML 눈부신 빛 속에서 소피아와 이렌느 두사람의 특징이 군데군데 보인다
        PRINTFORML 큰 키에 고혹적인 몸매의 타락한 구원소녀가 모습을 드러내자、
        PRINTFORMW 당신의 오른손을 잡고 손등에 키스하고、대전상대 쪽으로 몸을 돌렸다…！
        RESETCOLOR
        PRINTFORML
		SETVAR @"%듀얼리스트%_MZ_言発:現필드(듀얼리스트)", 1


    CASE "마르파스 소환"
        PRINTFORML
        SETCOLOR C_PINK
        PRINTFORML 크라이시스터 마르파는 마르파스의 가호로 변신하며 다른 시스터를 말려들게 했다！
        RESETCOLOR
        PRINTFORML
        CALL EROTIC_MOVE("全身레즈애무",NO_TO_CHARANUM(2100),NO_TO_CHARANUM(2104),500)
        CALL EROTIC_MOVE("全身레즈애무",NO_TO_CHARANUM(2101),NO_TO_CHARANUM(2104),500)
        CALL EROTIC_MOVE("全身레즈애무",NO_TO_CHARANUM(2102),NO_TO_CHARANUM(2104),500)
        CALL EROTIC_MOVE("全身레즈애무",NO_TO_CHARANUM(2103),NO_TO_CHARANUM(2104),500)
		SETVAR @"%듀얼리스트%_MZ_言発:現필드(듀얼리스트)", 1


    CASE "러시비아 소환"
        PRINTFORML
        SETCOLOR C_PINK
        PRINTFORML 음마의 목소리에 꼬드겨진 천마의 성선희, 셀레트리체 바르모니카는 개방적으로 되었다！
        RESETCOLOR
        PRINTFORML
		SETVAR @"%듀얼리스트%_MZ_言発:現필드(듀얼리스트)", 1

    CASE "데먼스미스의 크라이시스터 소환"
        PRINTFORML
        SETCOLOR C_PINK
        PRINTFORML 데먼스미스가 레퀴엠에 크라이시스터 몬스터를 집어넣고 나타났다！
        RESETCOLOR
        PRINTFORML

ENDSELECT


RETURN
