		CALL SOURCE_CALC_쾌Ｖ(조교대상, 実行役대상, (150 * 実보정) / 100)
		CALL SOURCE_CALC_노출(조교대상, 実行役대상, (50 * 実보정) / 100)
		CALL SOURCE_CALC_접촉(조교대상, 実行役대상, (100 * 実보정) / 100)
		痛みチェック = ((100 * 実보정) / 100) * PAIN_CHECK_V(조교대상) / 1000
		CALL SOURCE_CALC_고통(조교대상, 実行役대상, 痛みチェック)
	
	CASE "後穴凌辱"
		IF 조교대상 != -1
			EXP:조교대상:Ａ경험 += (実보정 / 100)
		ENDIF
		CALL SOURCE_CALC_쾌Ａ(조교대상, 実行役대상, (200 * 実보정) / 100)
		CALL SOURCE_CALC_노출(조교대상, 実行役대상, (50 * 実보정) / 100)
		CALL SOURCE_CALC_접촉(조교대상, 実行役대상, (100 * 実보정) / 100)
		痛みチェック = ((250 * 実보정) / 100) * PAIN_CHECK_A(조교대상) / 1000
		CALL SOURCE_CALC_고통(조교대상, 実行役대상, 痛みチェック)

	CASE "애널解し"
		IF 조교대상 != -1
			EXP:조교대상:Ａ경험 += (実보정 / 100)
		ENDIF

		CALL SOURCE_CALC_쾌Ａ(조교대상, 実行役대상, (100 * 実보정) / 100)
		CALL SOURCE_CALC_고통(조교대상, 実行役대상, (150 * 実보정) / 100)

	CASE "両穴凌辱"
		IF 조교대상 != -1
			EXP:조교대상:이상경험 += (実보정 / 100)
			EXP:조교대상:Ｖ경험 += (実보정 / 100)
			EXP:조교대상:Ａ경험 += (実보정 / 100)
		ENDIF
		CALL SOURCE_CALC_쾌Ｖ(조교대상, 実行役대상, (100 * 実보정) / 100)
		CALL SOURCE_CALC_쾌Ａ(조교대상, 実行役대상, (200 * 実보정) / 100)
		CALL SOURCE_CALC_노출(조교대상, 実行役대상, (50 * 実보정) / 100)
		CALL SOURCE_CALC_접촉(조교대상, 実行役대상, (100 * 実보정) / 100)
