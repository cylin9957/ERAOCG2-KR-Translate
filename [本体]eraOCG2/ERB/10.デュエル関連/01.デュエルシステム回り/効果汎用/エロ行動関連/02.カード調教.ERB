@EROTIC_MOVE(実行コマンド, 조교대상 = -1, 実行役대상 = -1, 実보정 = 100,連続実行)
#DIMS DYNAMIC 実行コマンド
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 実行役대상
#DIM DYNAMIC 実보정
#DIM DYNAMIC 痛みチェック
#DIM DYNAMIC 절정カウント前
#DIM DYNAMIC 절정カウント後
#DIM DYNAMIC 절정횟수
#DIM DYNAMIC 절정カウント前B
#DIM DYNAMIC 절정カウント後B
#DIM DYNAMIC 절정횟수B
#DIM DYNAMIC 連続実行
#DIM DYNAMIC 試行횟수

SIF 조교대상 == MASTER補助
	조교대상 = MASTER
SIF 実行役대상 == MASTER補助
	実行役대상 = MASTER

認識阻害 = 1
IF 조교대상 >= 0
	PRINTL
	PRINTFORML 【%実行コマンド%】
	PRINTFORML %NAME:조교대상%은 조교의 영향을 받아버렸습니다。
	EXP:조교대상:듀얼조교경험 ++
ENDIF
IF 조교대상 != -1
	절정カウント前 = EXP:조교대상:절정경험
ENDIF
IF 実行役대상 != -1
	절정カウント前B = EXP:実行役대상:절정경험
ENDIF
$INPUT_LOOP
IF 조교대상 != -1 && 実行役대상 != -1
	IF TALENT:조교대상:성별 & 1 && TALENT:実行役대상:성별 & 1
		EXP:조교대상:레즈경험 += (実보정 / 100)
		EXP:実行役대상:레즈경험 += (実보정 / 100)
	ENDIF
	RELATION:조교대상:実行役대상 += (実보정 / 100)
ENDIF

SELECTCASE 実行コマンド
	CASE "Ｃ単発"
		CALL SOURCE_CALC_쾌Ｃ(조교대상, 実行役대상, (100 * 実보정) / 100)
	CASE "Ｖ単発"
		CALL SOURCE_CALC_쾌Ｖ(조교대상, 実行役대상, (100 * 実보정) / 100)
	CASE "Ａ単発"
		CALL SOURCE_CALC_쾌Ａ(조교대상, 実行役대상, (100 * 実보정) / 100)
	CASE "Ｂ単発"
		CALL SOURCE_CALC_쾌Ｂ(조교대상, 実行役대상, (100 * 実보정) / 100)
	CASE "Ｍ単発"
		CALL SOURCE_CALC_쾌Ｍ(조교대상, 実行役대상, (100 * 実보정) / 100)
	CASE "노출単発"
		CALL SOURCE_CALC_노출(조교대상, 実行役대상, (100 * 実보정) / 100)

	CASE "全身애무"
		CALL SOURCE_CALC_쾌Ｃ(조교대상, 実行役대상, (200 * 実보정) / 100)
		CALL SOURCE_CALC_쾌Ｖ(조교대상, 実行役대상, (200 * 実보정) / 100)
		CALL SOURCE_CALC_쾌Ｂ(조교대상, 実行役대상, (200 * 実보정) / 100)
		CALL SOURCE_CALC_쾌Ｍ(조교대상, 実行役대상, (200 * 実보정) / 100)

		CALL SOURCE_CALC_접촉(조교대상, 実行役대상, (250 * 実보정) / 100)
		CALL SOURCE_CALC_노출(조교대상, 実行役대상, (200 * 実보정) / 100)
		CALL SOURCE_CALC_정복(実行役대상, 조교대상, (150 * 実보정) / 100)

	CASE "묘상捕縛"
		IF 조교대상 != -1
			EXP:조교대상:이상경험 += (実보정 / 100)
			EXP:조교대상:정음경험 += (実보정 / 100)
			EXP:조교대상:방뇨경험 += (実보정 / 100)
		ENDIF
		CALL SOURCE_CALC_쾌Ｃ(조교대상, 実行役대상, (150 * 実보정) / 100)
		CALL SOURCE_CALC_쾌Ｖ(조교대상, 実行役대상, (150 * 実보정) / 100)
		CALL SOURCE_CALC_쾌Ａ(조교대상, 実行役대상, (150 * 実보정) / 100)
		CALL SOURCE_CALC_쾌Ｂ(조교대상, 実行役대상, (150 * 実보정) / 100)
		CALL SOURCE_CALC_쾌Ｍ(조교대상, 実行役대상, (150 * 実보정) / 100)

		CALL SOURCE_CALC_고통(조교대상, 実行役대상, (200 * 実보정) / 100)
		CALL SOURCE_CALC_노출(조교대상, 実行役대상, (100 * 実보정) / 100)
		CALL SOURCE_CALC_구속(조교대상, 実行役대상, (200 * 実보정) / 100)
		CALL SOURCE_CALC_불결(조교대상, 実行役대상, (300 * 実보정) / 100)

	CASE "丸飲み"
		IF 조교대상 != -1
			EXP:조교대상:이상경험 += (実보정 / 100)
		ENDIF
		CALL SOURCE_CALC_고통(조교대상, 実行役대상, (200 * 実보정) / 100)
		CALL SOURCE_CALC_구속(조교대상, 実行役대상, (400 * 実보정) / 100)
		CALL SOURCE_CALC_불결(조교대상, 実行役대상, (300 * 実보정) / 100)

	CASE "생체파츠"
		CALL SOURCE_CALC_쾌Ｃ(조교대상, 実行役대상, (100 * 実보정) / 100)
		CALL SOURCE_CALC_쾌Ｖ(조교대상, 実行役대상, (100 * 実보정) / 100)
		CALL SOURCE_CALC_쾌Ａ(조교대상, 実行役대상, (100 * 実보정) / 100)
		CALL SOURCE_CALC_쾌Ｂ(조교대상, 実行役대상, (100 * 実보정) / 100)
		CALL SOURCE_CALC_쾌Ｍ(조교대상, 実行役대상, (100 * 実보정) / 100)
		CALL SOURCE_CALC_고통(조교대상, 実行役대상, (50 * 実보정) / 100)
