@CARD_REMOVAL_BASIC(듀얼리스트,효과範囲,場所,除去種,修正値,反転플래그)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC 대상者

#DIMS DYNAMIC 효과範囲
#DIMS DYNAMIC 除去種

;묘지、패、덱、제외
#DIM DYNAMIC カードID
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード,10
#DIM DYNAMIC 魔選択カード,10

#DIM DYNAMIC 対選択カード,10
#DIM DYNAMIC 対魔選択カード,10


#DIM DYNAMIC 選択カード_ID,10
#DIM DYNAMIC 実손패セット,200

#DIM DYNAMIC カウンタ
#DIM DYNAMIC カウンタB
#DIM DYNAMIC 修正値
#DIM DYNAMIC 攻撃力修正
#DIMS DYNAMIC 발동ゾーン
#DIMS DYNAMIC 대상カード名
#DIM DYNAMIC 대상カードID

#DIM DYNAMIC 現在最大攻撃力
#DIM DYNAMIC ダメージ量
#DIM DYNAMIC 反転플래그
#DIM DYNAMIC 마법함정플래그
#DIM DYNAMIC 裏側플래그
#DIM DYNAMIC 攻撃力手渡
#DIM DYNAMIC 除去成功数
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 複数カウンタ
VARSET 효과추가종류除去보존
VARSET 효과추가カテ보존

FOR カウンタ,0,10
	효과추가종류除去보존:カウンタ = 효과추가종류:カウンタ
NEXT
효과추가カテ보존 = %효과추가カテ%

カードID = 処理中_효과ID

IF 효과条件複数 == 100
	RETURN 0
ELSEIF INRANGE(효과条件複数,50,89)
	효과条件複数 = 90
ENDIF

VARSET 現在最大攻撃力, -1
VARSET 選択カード, -1
VARSET 他参照選択カード, -1
VARSET 他参照選択カードID,-1

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 反転플래그 == 1
	대상者 = %듀얼리스트%
ELSE
	대상者 = %対面者%
ENDIF

$CHOICE_LOOP
IF !GROUPMATCH(효과範囲, "全場単体", "필드単体", "마함単体", "필드묘지単体",  "両全場単体", "両필드単体", "両마함単体")
CUSTOMDRAWLINE —
PRINTFORML 【제거효과확인】 
CUSTOMDRAWLINE —
PRINTL
ENDIF
SELECTCASE 효과範囲
;――――――――――――――――――――――――――――――その他―――――――――――――――――――――――――
	CASE "戦闘破壊"
		;ザポルグ追記20241227
		IF 除去種 == "제외"
			CALL CARD_DISPOSE(대상者,"필드",場所,제외行き)
		ELSE
			CALL CARD_DISPOSE(대상者,"필드",場所,戦闘破壊行き)
		ENDIF
	CASE "손패破壊"
		VARSET カウンタB
		FOR カウンタ,0,紙束最大値
			SIF GETVAR(@"%대상者%_손패:カウンタ") == -1
				CONTINUE
			実손패セット:カウンタB = カウンタ
			カウンタB ++
		NEXT


		IF カウンタB >= 1
			$INPUT_HAND_LOOP
			選択カード = RAND:カウンタB
			選択カード = 実손패セット:選択カード
			SIF GETVAR(@"%대상者%_손패:選択カード") == -1
				GOTO INPUT_HAND_LOOP
			IF 除去種 == "손패"
				CALL CARD_DISPOSE(대상者,"손패",選択カード,손패行き)
			ELSEIF 除去種 == "덱"
				CALL CARD_DISPOSE(대상者,"손패",選択カード,덱行き)
			ELSEIF 除去種 == "제외"
				CALL CARD_DISPOSE(대상者,"손패",選択カード,1)
			ELSEIF 除去種 == "묘지"
				CALL CARD_DISPOSE(대상者,"손패",選択カード)
			ENDIF
		ENDIF
	CASE "손패全体"
		IF 손패数(대상者) != 0
			FOR カウンタ,0,紙束最大値
				SIF GETVAR(@"%대상者%_손패:カウンタ") == -1
					CONTINUE
				CALL CARD_DISPOSE(대상者,"손패",カウンタ)
			NEXT
		ENDIF

	CASE "손패지정"
		IF 손패数(대상者) != 0
			選択カード = 場所
			IF 除去種 == "제외"
				CALL CARD_DISPOSE(대상者,"손패",選択カード,1)
			ELSE
				CALL CARD_DISPOSE(대상者,"손패",選択カード)
			ENDIF
		ENDIF
;――――――――――――――――――――――――――――――全体破壊―――――――――――――――――――――――――
	CASE "필드全体"
		FOR カウンタ,0,5
			CALL CARD_REMOVAL_ABLE(대상者,듀얼리스트,"全体",カウンタ,除去種,カードID)
			SIF RESULT != 1
				CONTINUE
			選択カード:カウンタ = カウンタ
		NEXT
		FOR カウンタ,0,5
			SIF 選択カード:カウンタ == -1
				CONTINUE
			CALL CARD_REMOVAL_AFTERMATH(대상者,듀얼리스트,選択カード:カウンタ,除去種,カードID,修正値)
			除去成功数 ++
		NEXT
		RESULT:1 = 除去成功数
		RETURN 1
	CASE "両필드全体"
		FOR カウンタ,0,5
			IF 효과条件他所 > 0
				SIF カウンタ == 場所
					CONTINUE
			ENDIF
			CALL CARD_REMOVAL_ABLE(듀얼리스트,듀얼리스트,"全体",カウンタ,除去種,カードID)
			SIF RESULT != 1
				CONTINUE
			選択カード:カウンタ = カウンタ
		NEXT
		VARSET 対選択カード, -1
		FOR カウンタ,0,5
			CALL CARD_REMOVAL_ABLE(対面者,듀얼리스트,"全体",カウンタ,除去種,カードID)
			SIF RESULT != 1
				CONTINUE
			対選択カード:カウンタ = カウンタ
		NEXT


		FOR カウンタ,0,5
			SIF (選択カード:カウンタ) == -1
				CONTINUE
			CALL CARD_REMOVAL_AFTERMATH(듀얼리스트,듀얼리스트,選択カード:カウンタ,除去種,カードID,修正値)
			攻撃力手渡 += RESULT:1
			除去成功数 ++
		NEXT
		FOR カウンタ,0,5
			SIF (対選択カード:カウンタ) == -1
				CONTINUE
			CALL CARD_REMOVAL_AFTERMATH(対面者,듀얼리스트,対選択カード:カウンタ,除去種,カードID,修正値)
			攻撃力手渡 += RESULT:1
			除去成功数 ++
		NEXT

		RESULT:2 = 攻撃力手渡
		RESULT:1 = 除去成功数
		RETURN 1
	CASE "마함全体"
		VARSET 魔選択カード, -1
		FOR カウンタ,0,5
			CALL CARD_REMOVAL_ABLE(대상者,듀얼리스트,"全体",カウンタ,除去種,カードID,1)
			SIF RESULT != 1
				CONTINUE
			選択カード:カウンタ = カウンタ
		NEXT

		FOR カウンタ,0,5
			SIF (選択カード:カウンタ) == -1
				CONTINUE
			CALL CARD_REMOVAL_AFTERMATH(대상者,듀얼리스트,選択カード:カウンタ,除去種,カードID,修正値,1)
			除去成功数 ++
		NEXT
		RESULT:1 = 除去成功数
		RETURN 1
	CASE "両마함全体"
		VARSET 魔選択カード, -1
		FOR カウンタ,0,5
			CALL CARD_REMOVAL_ABLE(듀얼리스트,듀얼리스트,"全体",カウンタ,除去種,カードID,1)
			SIF RESULT != 1
				CONTINUE
			魔選択カード:カウンタ = カウンタ
		NEXT
		VARSET 対魔選択カード, -1
		FOR カウンタ,0,5
			CALL CARD_REMOVAL_ABLE(対面者,듀얼리스트,"全体",カウンタ,除去種,カードID,1)
			SIF RESULT != 1
				CONTINUE
			対魔選択カード:カウンタ = カウンタ
		NEXT

		FOR カウンタ,0,5
			SIF 魔選択カード:カウンタ == -1
				CONTINUE
			CALL CARD_REMOVAL_AFTERMATH(듀얼리스트,듀얼리스트,魔選択カード:カウンタ,除去種,カードID,修正値,1)
			除去成功数 ++
		NEXT
		FOR カウンタ,0,5
			SIF 対魔選択カード:カウンタ == -1
				CONTINUE
			CALL CARD_REMOVAL_AFTERMATH(対面者,듀얼리스트,対魔選択カード:カウンタ,除去種,カードID,修正値,1)
			除去成功数 ++
		NEXT

		RESULT:1 = 除去成功数
		RETURN 1
	CASE "全場全体"
		FOR カウンタ,0,5
			CALL CARD_REMOVAL_ABLE(대상者,듀얼리스트,"全体",カウンタ,除去種,カードID)
			SIF RESULT != 1
				CONTINUE
			選択カード:カウンタ = カウンタ
		NEXT
		VARSET 魔選択カード, -1
		FOR カウンタ,0,5
			CALL CARD_REMOVAL_ABLE(대상者,듀얼리스트,"全体",カウンタ,除去種,カードID,1)
			SIF RESULT != 1
				CONTINUE
			魔選択カード:カウンタ = カウンタ
		NEXT

		FOR カウンタ,0,5
			SIF (選択カード:カウンタ) == -1
				CONTINUE
			CALL CARD_REMOVAL_AFTERMATH(대상者,듀얼리스트,選択カード:カウンタ,除去種,カードID,修正値)
		NEXT
		FOR カウンタ,0,5
			SIF (魔選択カード:カウンタ) == -1
				CONTINUE
			CALL CARD_REMOVAL_AFTERMATH(대상者,듀얼리스트,魔選択カード:カウンタ,除去種,カードID,修正値,1)
		NEXT

	CASE "両全場全体"
		FOR カウンタ,0,5
			IF 효과条件他所 > 0
				SIF カウンタ == 場所
					CONTINUE
			ENDIF
			CALL CARD_REMOVAL_ABLE(듀얼리스트,듀얼리스트,"全体",カウンタ,除去種,カードID)
			SIF RESULT != 1
				CONTINUE
			選択カード:カウンタ = カウンタ
		NEXT
		VARSET 対選択カード, -1
		FOR カウンタ,0,5
			CALL CARD_REMOVAL_ABLE(対面者,듀얼리스트,"全体",カウンタ,除去種,カードID)
			SIF RESULT != 1
				CONTINUE
			対選択カード:カウンタ = カウンタ
		NEXT

		VARSET 魔選択カード, -1
		FOR カウンタ,0,5
			CALL CARD_REMOVAL_ABLE(듀얼리스트,듀얼리스트,"全体",カウンタ,除去種,カードID,1)
			SIF RESULT != 1
				CONTINUE
			魔選択カード:カウンタ = カウンタ
		NEXT
		VARSET 対魔選択カード, -1
		FOR カウンタ,0,5
			CALL CARD_REMOVAL_ABLE(対面者,듀얼리스트,"全体",カウンタ,除去種,カードID,1)
			SIF RESULT != 1
				CONTINUE
			対魔選択カード:カウンタ = カウンタ
		NEXT


		FOR カウンタ,0,5
			SIF (選択カード:カウンタ) == -1
				CONTINUE
			CALL CARD_REMOVAL_AFTERMATH(듀얼리스트,듀얼리스트,選択カード:カウンタ,除去種,カードID,修正値)
			除去成功数 ++
		NEXT
		FOR カウンタ,0,5
			SIF (魔選択カード:カウンタ) == -1
				CONTINUE
			CALL CARD_REMOVAL_AFTERMATH(듀얼리스트,듀얼리스트,魔選択カード:カウンタ,除去種,カードID,修正値,1)
			除去成功数 ++
		NEXT
		FOR カウンタ,0,5
			SIF (対選択カード:カウンタ) == -1
				CONTINUE
			CALL CARD_REMOVAL_AFTERMATH(対面者,듀얼리스트,対選択カード:カウンタ,除去種,カードID,修正値)
			除去成功数 ++
		NEXT
		FOR カウンタ,0,5
			SIF (対魔選択カード:カウンタ) == -1
				CONTINUE
			CALL CARD_REMOVAL_AFTERMATH(対面者,듀얼리스트,対魔選択カード:カウンタ,除去種,カードID,修正値,1)
			除去成功数 ++
		NEXT
		RESULT:1 = 除去成功数
		RETURN 1
;――――――――――――――――――――――――――――――単体破壊―――――――――――――――――――――――――
	CASE "全場単体", "필드単体", "마함単体", "필드묘지単体",  "両全場単体", "両필드単体", "両마함単体" 
			CALL RESETAREA
		IF 효과範囲 == "全場単体"
			효과範囲상대필드 = 1
			효과範囲상대마함 = 1
		ELSEIF 효과範囲 == "필드単体"
			효과範囲상대필드 = 1
		ELSEIF 효과範囲 == "마함単体"
			효과範囲상대마함 = 1
		ELSEIF 효과範囲 == "필드묘지単体"
			효과範囲상대필드 = 1
			효과範囲상대묘지 = 1
		ELSEIF 효과範囲 == "両全場単体"
			효과範囲필드 = 1
			효과範囲마함 = 1
			효과範囲상대필드 = 1
			효과範囲상대마함 = 1
		ELSEIF 효과範囲 == "両필드単体"
			효과範囲필드 = 1
			효과範囲상대필드 = 1
		ELSEIF 효과範囲 == "両마함単体"
			효과範囲마함 = 1
			효과範囲상대마함 = 1
		ENDIF
		CALL CARD_REMOVAL_ADV(듀얼리스트,"単体除去",場所,除去種,修正値,反転플래그)
		IF 除去種 == "選択"
			;選択したカード
			選択カード = RESULT:1
			IF GROUPMATCH(효과範囲,"필드単体","마함単体")
				選択カード = 選択カード - 2000
			ENDIF
			;カードID
			RESULT:99 = 他参照選択カードID
			RESULT:1 = 選択カード
			RETURN 1
		ENDIF
		SIF RESULT == 0
			RETURN 0
			
		RESULT:2 = 選択カード_ID
		RETURN 1





	CASE "全場単体", "필드単体", "마함単体", "필드묘지単体"
		VARSET 選択カード, -1
		
		$CHOICE_LOOP_1
		VARSET 現在最大攻撃力, -1
		VARSET 条件달성
		IF 효과条件複数
			FOR カウンタ,0,複数カウンタ
				효과条件選択済:カウンタ = 選択カード:カウンタ 
			NEXT
		ENDIF
		IF 효과範囲 == "필드単体" || 효과範囲 == "全場単体" || 효과範囲 == "필드묘지単体"
			FOR カウンタ,0,5
				IF 효과条件複数 > 0
					SIF MATCH(효과条件選択済, カウンタ)
						CONTINUE
				ENDIF
				CALL CARD_REMOVAL_ABLE(대상者,듀얼리스트,"単体",カウンタ,除去種,カードID)
				IF RESULT == 0 
					CONTINUE
				ELSEIF RESULT == 2 && 듀얼리스트 == "상대"
					CONTINUE
				ELSEIF RESULT == 1
					条件달성 = 1
				ENDIF
				IF 듀얼리스트 == "자신"
					VARSET 대상カードID
					VARSET 裏側플래그
					裏側플래그 = GETVAR(@"%대상者%_MZ_表:カウンタ")
					대상カードID = GETVAR(@"%대상者%_필드:カウンタ")
					IF 대상者 != 듀얼리스트
						SETCOLOR C_GRAY
						IF GETVAR(@"%대상者%_MZ_破耐:カウンタ") >= 1 && 除去種 == "묘지"
							CALLFORM CARD_KANPRINT("상대필드",カウンタ,대상カードID,1,裏側플래그)
						ELSEIF GETVAR(@"%대상者%_MZ_対耐:カウンタ") >= 1
							CALLFORM CARD_KANPRINT("상대필드",カウンタ,대상カードID,2,裏側플래그)
						ELSE
							TRYCALLFORM CARD_{カードID}, "종류"
							IF GETVAR(@"%대상者%_MZ_モ耐:カウンタ") >= 1 && INRANGE(RESULT, 0,15)
								CALLFORM CARD_KANPRINT("상대필드",カウンタ,대상カードID,3,裏側플래그)
							ELSEIF GETVAR(@"%대상者%_MZ_魔耐:カウンタ") >= 1 && INRANGE(RESULT, 20,30)
								CALLFORM CARD_KANPRINT("상대필드",カウンタ,대상カードID,4,裏側플래그)
							ELSEIF GETVAR(@"%대상者%_MZ_함정耐:カウンタ") >= 1 && INRANGE(RESULT, 30,40)
								CALLFORM CARD_KANPRINT("상대필드",カウンタ,대상カードID,5,裏側플래그)
							ELSE
								RESETCOLOR
								CALL CARD_SETCOLOR(대상者,"필드",カウンタ,裏側플래그)
								CALLFORM CARD_KANPRINT("상대필드",カウンタ,대상カードID,0,裏側플래그)
							ENDIF
						ENDIF
					ELSE
						CALL CARD_SETCOLOR(대상者,"필드",カウンタ)
						CALLFORM CARD_KANPRINT("필드",カウンタ,대상カードID)
					ENDIF
					RESETCOLOR
				ELSEIF 듀얼리스트 == "상대"
					IF  GETVAR(@"%대상者%_MZ_攻:カウンタ") > 現在最大攻撃力
						現在最大攻撃力 =  GETVAR(@"%대상者%_MZ_攻:カウンタ")
						TRYCALLFORM CARDNAME_{GETVAR(@"%대상者%_필드:カウンタ")}, "名前"
						選択カード:複数カウンタ = カウンタ
					ENDIF
				ENDIF
			NEXT
		ENDIF
		IF 효과範囲 == "마함単体" || 효과範囲 == "全場単体"
			FOR カウンタ,0,5
				IF 효과条件複数 > 0
					SIF MATCH(효과条件選択済, カウンタ + 10)
						CONTINUE
				ENDIF
				CALL CARD_REMOVAL_ABLE(대상者,듀얼리스트,"単体",カウンタ,除去種,カードID,1)
