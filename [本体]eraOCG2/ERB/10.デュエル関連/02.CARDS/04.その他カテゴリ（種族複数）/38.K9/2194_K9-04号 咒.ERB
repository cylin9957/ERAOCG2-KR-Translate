@CARDNAME_2194,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = K9-04호 노로이
	CASE "略称"
		RESULTS = K9 노로이
	CASE "カテゴリ"
		RESULTS:0 = K9
ENDSELECT


@CARD_2194(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 기계족
	CASE "レベル"
		RETURN 5
	CASE "攻撃力"
		RETURN 2200
	CASE "守備力"
		RETURN 1500
	CASE "성별"
		RETURN 牝性
	CASE "추가日"
		RETURN 250726
ENDSELECT
RETURN 0

@CARD_EXPLANATION_2194(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ②③의 효과는 각각 １턴에 １번밖에 사용할 수 없다。
CALLFORM CARD_ABLE(0,효과소환)
PRINTL ①：상대의 패가 ２장 이상인 경우、이 카드는 릴리스 없이 소환할 수 있다。
CALLFORM CARD_ABLE(2,소환時)
PRINTL ②：이 카드가 소환된 경우에 발동할 수 있다。
PRINTL 덱에서 기계족이외의 "K9" 몬스터 １장을 특수 소환한다。
;PRINTL 이 턴、자신은「K9」몬스터밖에 EX덱에서 특수 소환할 수 없다。
CALLFORM CARD_ABLE(3,필드起動)
PRINTL ③：자신 필드의 앞면 표시의 "K9" 카드 １장을 묘지로 보내고 발동할 수 있다。
PRINTL 상대의 패를 전부 확인한다。

@CARDSUMMON_AA_2194


@CARDCAN_2194(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,2194)
SIF RESULT == 1
	RETURN 0

IF 종류 == 효과소환
	CALL FULLRESET
	효과範囲상대손패 = 1
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 2
		RETURN 0
	RETURN 1
ELSEIF 종류 == 소환時
	SIF 발동済_2(듀얼리스트,2194)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	CALLFORM 효과条件종족_ADV(기계족,"以外")
	효과条件レベル = 5
	효과条件カテゴリ = K9
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드起動
	SIF 발동済_3(듀얼리스트,2194)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF 듀얼리스트 == "상대" ;CPUは使わない
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件カテゴリ = K9
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲상대손패 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_2194(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 選択カード


IF 종류 == 효과소환
	RETURN 1
ELSEIF 종류 == 소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,2194,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		CALLFORM 효과条件종족_ADV(기계족,"以外")
		효과条件レベル = 5
		효과条件カテゴリ = K9
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,2194,3)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲마함 = 1
		효과条件カテゴリ = K9
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲상대손패 = 1
			효과条件任意 = 1
			CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ENDIF



