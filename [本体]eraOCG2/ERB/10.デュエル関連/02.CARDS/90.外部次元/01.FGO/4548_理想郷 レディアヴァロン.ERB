@CARDNAME_4548,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 이상향 레이디 아발론
	CASE "略称"
		RESULTS = 프린
	CASE "カテゴリ"
		RESULTS:0 = 영령
		RESULTS:1 = 프리텐더
		RESULTS:2 = FATE
ENDSELECT


@CARD_4548(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "外部者"
		RETURN 1
	CASE "종류"

		RETURN 싱크로
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 미확인족
	CASE "レベル"
		RETURN 8
	CASE "攻撃力"
		RETURN 2200
	CASE "守備力"
		RETURN 2800
	CASE "성별"
		RETURN 牝性
	CASE "추가日"
		RETURN 250908
ENDSELECT
RETURN 0

@CARDSUMMON_4548(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要レベル数 = 8
素材条件素材数起動 = 1

IF EX参照 == 0 || EX参照 == 1
	素材条件튜너 = 1
ELSEIF EX参照 >= 10
	素材条件튜너 = -1
ENDIF
IF EX参照 == 1
	CALL SYNCAN(듀얼리스트,소환カード,4548) 
	RETURN RESULT
ENDIF


@CARD_EXPLANATION_4548(종류)
#DIM DYNAMIC 종류

PRINTL 튜너 １장＋튜너 이외의 몬스터 １장 이상
PRINTL 이 카드명의 ①②의 효과는 각각 １턴에 １번밖에 사용할 수 없다。
CALLFORM CARD_ABLE(1,EX소환時)
PRINTL ①：이 카드가 S소환에 성공한 경우에 발동할 수 있다。
PRINTL 턴 종료시까지 상대 필드의 몬스터는 효과를 발동할 수 없다。
CALLFORM CARD_ABLE(2,필드起動,フリー誘発)
PRINTL ②：서로의 메인 페이즈에 발동할 수 있다。
PRINTL 자신 / 상대 묘지의 카드를 ２장 덱으로 되돌린다。
PRINTL 그 후、자신 필드에 전사족 / 마법사족 / 사이킥족 몬스터가 존재하는 경우、
PRINTL 추가로 상대 필드의 몬스터 １장을 덱으로 되돌린다。
;終わりなく永遠に見ていたい！
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ③：이 카드가 필드 / 묘지에 존재하고、
PRINTL 자신 필드에 공격력 ２５００이상인 전사족 / 마법사족 / 사이킥족 몬스터중 어느 것인가가 존재하는 한、
PRINTL 서로에게 전투 / 효과에 의한 데미지를 받지 않는다。


@CARDCAN_4548(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,4548)
SIF RESULT == 1
	RETURN 0
IF 종류 == EX소환時
	SIF 발동済_1(듀얼리스트,1618)
		RETURN 0

	RETURN 1
ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_2(듀얼리스트,1618)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과範囲상대묘지 = 1
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 2
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF !GROUPMATCH(ゾーン,"필드","묘지")
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종족:0 = 마법사족
	효과条件종족:1 = 전사족
	효과条件종족:2 = 사이킥족
	효과条件以攻撃力 = 2500
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_4548(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 素材数
#DIM DYNAMIC 대상モンスター_ID,5
#DIM DYNAMIC 炎起動
#DIM DYNAMIC 水起動
#DIM DYNAMIC 風起動
#DIM DYNAMIC 地起動

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == EX소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4548,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"発無",1)
	ENDIF
ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,12060,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과範囲상대묘지 = 1
		효과条件行先플래그 = 덱行き
		효과条件複数 = 2
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲필드 = 1
			효과条件종족:0 = 마법사족
			효과条件종족:1 = 전사족
			효과条件종족:2 = 사이킥족
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			IF RESULT == 1
				CALL FULLRESET
				효과範囲상대필드 = 1
				CALL CARD_REMOVAL_ADV(듀얼리스트,"単体除去",場所,"덱")
			ENDIF
		ENDIF
	ENDIF
ELSEIF 종류 == 지속효과
	CALL SET_PLAYER_INFO(듀얼리스트,"ダメージ無効",3)
	CALL SET_PLAYER_INFO(듀얼리스트,"효과ダメージ無効",3)
	CALL SET_PLAYER_INFO(듀얼리스트,"ダメージ無効",3,1)
	CALL SET_PLAYER_INFO(듀얼리스트,"효과ダメージ無効",3,1)
ENDIF
