VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 게이트 오브 스카이 스카사하=스카디
	CASE "略称"
		RESULTS = 스카디
	CASE "カテゴリ"
		RESULTS:0 = 영령
		RESULTS:1 = 캐스터
		RESULTS:2 = FATE
ENDSELECT


@CARD_4509(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 링크
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 마법사족
	CASE "レベル"
		RETURN 3
	CASE "攻撃力"
		RETURN 2700
	CASE "守備力"
		RETURN 2000
	CASE "성별"
		RETURN 牝性
ENDSELECT


RETURN 0


@CARD_EXPLANATION_4509(종류)
