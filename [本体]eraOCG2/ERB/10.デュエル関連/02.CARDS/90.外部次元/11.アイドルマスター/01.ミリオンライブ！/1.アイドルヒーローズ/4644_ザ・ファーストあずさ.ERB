@CARDNAME_4644,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 아이돌 히어로 더 퍼스트 아즈사
	CASE "略称"
		RESULTS = The 1st 아즈사
	CASE "カテゴリ"
		RESULTS:0 = I•HERO
		RESULTS:1 = HERO
		RESULTS:2 = 밀리언 라이브!
		RESULTS:4 = 미우라 아즈사
ENDSELECT

@CARD_4644(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 1
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 전사족
	CASE "レベル"
		RETURN 7
	CASE "攻撃力"
		RETURN 2600
	CASE "守備力"
		RETURN 2000
	CASE "성별"
		RETURN 牝性
	CASE "추가日"
		RETURN 250601
ENDSELECT
RETURN 0

@CARD_EXPLANATION_4644(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②③의 효과는 각각 １턴에 １번밖에 사용할 수 없다.
CALL CARD_ABLE(1,손패起動)
PRINTL ①：자신 메인 페이즈에 발동할 수 있다.
PRINTL 패에서 이 카드를 버리고, 덱에서 이 카드명 이외의 "I•HERO" 몬스터 １장을 패에 넣는다.
PRINTL 그 후, 패를 １장 버린다.
PRINTL 이 효과의 발동 후, 턴 종료시까지, 자신은 융합 몬스터밖에 EX덱에서 특수 소환할 수 없다.
CALL CARD_ABLE(2,효과後誘発)
PRINTL ②：자신 필드에 "I•HERO" 융합 몬스터가 소환 / 특수 소환되었을 경우에, 상대 필드의 앞면 표시 카드 １장을 대상으로 하여 발동할 수 있다.
PRINTL 패의 이 카드를 상대에게 보여주고, 턴 종료시까지, 대상 카드의 효과를 무효화한다
PRINTL 이 효과의 발동 후, 턴 종료시까지, 자신은 "I•HERO" 몬스터밖에 특수 소환할 수 없다.

@CARDCAN_4644(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,4644)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,4644)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = I•HERO
	효과条件固名前 = 아이돌 히어로 더 퍼스트 아즈사
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 효과後誘発
	SIF 발동済_2(듀얼리스트,4644)
		RETURN 0
	SIF ゾーン != "손패"
		RETURN 0
	CALL FULLRESET
	효과条件カテゴリ = I•HERO
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"소환全般")
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件除去種 = 無効
	효과条件표시 = 表側표시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_4644(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4644,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = I•HERO
		효과条件固名前 = 아이돌 히어로 더 퍼스트 아즈사
		효과条件종류 = モン全般
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲손패 = 1
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
		CALL FULLRESET
		EX소환可能종류 = 융합
		CALL CARD_EX_RESTRICTION(듀얼리스트)
	ENDIF
ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4644,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲상대필드 = 1
		효과範囲상대마함 = 1
		효과条件표시 = 表側표시
		CALL CARD_REMOVAL_ADV(듀얼리스트,"単体除去",場所,"無効")
		CALL CARD_SS_RESTRICTION(듀얼리스트,"I•HERO")
	ENDIF
ENDIF