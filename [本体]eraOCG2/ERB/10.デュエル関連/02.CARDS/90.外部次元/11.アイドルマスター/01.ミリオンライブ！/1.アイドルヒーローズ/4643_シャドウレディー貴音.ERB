@CARDNAME_4643,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 아이돌 히어로 섀도우 레이디 타카네
	CASE "略称"
		RESULTS = S. 레이디 타카네
	CASE "カテゴリ"
		RESULTS:0 = I•HERO
		RESULTS:1 = HERO
		RESULTS:2 = 밀리언 라이브!
		RESULTS:4 = 시죠 타카네
ENDSELECT

@CARD_4643(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "외부자"
		RETURN 1
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 전사족
	CASE "レベル"
		RETURN 5
	CASE "攻撃力"
		RETURN 1800
	CASE "守備力"
		RETURN 1800
	CASE "성별"
		RETURN 牝性
	CASE "추가日"
		RETURN 250601
ENDSELECT
RETURN 0

@CARD_EXPLANATION_4643(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②의 효과는 각각 1턴에 1번밖에 사용할 수 없다.
CALL CARD_ABLE(1,효과後誘発)
PRINTL ①：상대가 마법 / 함정 카드의 효과를 발동했을 경우에 발동할 수 있다.
PRINTL 패의 이 카드를 묘지로 보내고、덱에서 "I•HERO" 몬스터 １장을 패에 넣는다。
PRINTL 그 후、상대 필드에만 몬스터가 존재하는 경우、
PRINTL 자신의 패 / 묘지에서 "I•HERO" 융합 몬스터 카드에 의해 정해진 융합 소재 몬스터를 제외함으로써、
PRINTL 그 융합 몬스터 １장을 ＥＸ덱에서 융합 소환할 수 있다。
PRINTL 이 때、２장째 융합 소재는 묘지로 보낸 이 카드여야 한다。
PRINTL 이 효과의 발동 후、다음 턴의 종료시까지 자신은 "I•HERO" 몬스터밖에 특수 소환할 수 없다。
CALL CARD_ABLE(2,ターン終了時)
PRINTL ②：자신 / 상대 엔드 페이즈에、자신 필드에 "I•HERO" 융합 몬스터가 존재하는 경우에 발동할 수 있다。
PRINTL 묘지의 이 카드를 패로 되돌린다。

@CARDCAN_4643(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,4643)
SIF RESULT == 1
	RETURN 0

IF 종류 == 효과後誘発
	SIF 발동済_1(듀얼리스트,4643)
		RETURN 0
	SIF ゾーン != "손패"
		RETURN 0
	CALL FULLRESET
	효과条件종류 = 마함全般
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"효과발동",1)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲EX덱 = 1
	효과条件レベル = 4
	효과条件カテゴリ = I•HERO
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン終了時
	SIF 발동済_2(듀얼리스트,4643)
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = I•HERO
	효과条件종류 = 융합
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン開始時予約
	RETURN 1
ENDIF


@CARDEFFECT_4643(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

#DIM DYNAMIC カード추가先
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4643,1)
	IF RESULT == 0
		カード추가先 = 現묘지数(듀얼리스트)
		CALL FULLRESET
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件レベル = 4
		효과条件カテゴリ = I•HERO
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1 && GETVAR(@"%듀얼리스트%_제외NG置換") < 1
			条件달성 = 1
			CALL FULLRESET
			효과範囲상대필드 = 1
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			SIF RESULT != 1
				条件달성 = 0
			CALL FULLRESET
			효과範囲필드 = 1
			CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
			SIF RESULT == 1
				条件달성 = 0
			IF 条件달성 > 0
				CALL RESETAREA_MAT
				素材範囲묘지 = 1
				素材範囲손패 = 1
				CALL FULLRESET
				효과条件カテゴリ = I•HERO
				효과条件종류 = 융합
				효과条件소환可 = 1
				強制素材場所２ = 지정場所(カード추가先,"묘지")
				효과条件任意 = 1
				CALL CARD_CHOOSE_BASIC(듀얼리스트,"EX덱",ゾーン,場所)
				選択カード = RESULT
				IF 選択カード != 9999
					TRYCALLFORM CARDSUMMON_{GETVAR(@"%듀얼리스트%_EX덱:{選択カード}")}(듀얼리스트,選択カード)
					CALL FUSSUMMON(듀얼리스트,選択カード,제외行き)
				ENDIF
			ENDIF
		ENDIF
		CALL CARD_EX_RESTRICTION(듀얼리스트,"I•HERO")
		CALL CARD_RESERV(듀얼리스트,4643,スタン予約)
	ENDIF
ELSEIF 종류 == ターン終了時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,4643,2)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,제외行き)
	ENDIF
ELSEIF 종류 == ターン開始時予約
	CALL CARD_EX_RESTRICTION(듀얼리스트,"I•HERO")
ENDIF