@CARDNAME_300205,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 트랩 앵커
	CASE "略称"
		RESULTS = 트랩 앵커
	CASE "カテゴリ"
		RESULTS:0 = 부스트
ENDSELECT


@CARD_300205(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN システム
ENDSELECT

RETURN 0

@CARD_EXPLANATION_300205(종류)
#DIM DYNAMIC 종류

PRINTL 부스트는 １턴에 １번밖에 발동할 수 없다。
CALLFORM CARD_ABLE(1,부스트起動)
PRINTL ①：스피드 카운터를 ２개 소비하고 발동할 수 있다。
PRINTL 자신은 묘지에서 함정 카드 １장을 패에 넣는다。

@CARDCAN_300205(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,300205)
SIF RESULT == 1
	RETURN 0

IF 종류 == 부스트起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF 발동済_1(듀얼리스트,300205)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_SpＣ") < 2
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件종류 = 함정全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_300205(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC カウンタB
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択肢

IF 종류 == 부스트起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,300205,1)
	IF RESULT == 0
		CALL SET_PLAYER_INFO(듀얼리스트,"SpＣ",-2,0,1)
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件종류 = 함정全般
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF
