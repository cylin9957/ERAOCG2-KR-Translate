@CARDNAME_79618,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 호색한 용병
	CASE "略称"
		RESULTS = 호색한 용병
	CASE "カテゴリ"
		RESULTS:0 = 없음
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_79618(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
	CASE "추가日"
		RETURN 250903
ENDSELECT

RETURN 0

@CARD_EXPLANATION_79618(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
CALL CARD_ABLE(0,마함起動,손패起動)
PRINTL ①：상대의 패를 확인하고、그 안에서 １장을 고른다。
PRINTL 그 카드의 성별에 따라 이하의 효과를 적용한다。
PRINTL ●암컷：그 카드를 자신의 패에 넣는다
PRINTL ●암컷 이외：그 카드를 주인의 덱 맨 아래로 되돌리고、서로는 １장 드로우 한다。
CALL TEXT_DECORATION("ROGUE")


@CARDCAN_79618(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79618)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_DRAW_CHECK
	CALL FULLRESET
	효과範囲상대손패 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_79618(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 조교대상
#DIMS DYNAMIC 対面者
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79618)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲상대손패 = 1
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT != 9999
			選択カード_ID = RESULT:99
			選択カード = RESULT:50
			TRYCALLFORM CARD_{選択カード_ID}("성별")
			IF RESULT == 牝性
				CALL CARD_ADD(듀얼리스트,"상대손패","손패",選択カード)
				PRINTFORML
				TRYCALLFORM CARDNAME_{選択カード_ID}("名前")
				SETCOLOR C_PINK
				PRINTFORML 호색한 용병에게 붙잡힌%RESULTS%는、전신을 만져졌다！
				CALL CARD_EROTICS(選択カード_ID)
				IF RESULT >= 0
					조교대상 = RESULT
					CALL EROTIC_MOVE("全身愛撫",조교대상,-1,100)
					CALL EROTIC_MOVE("全身愛撫",조교대상,-1,100)
					CALL EROTIC_MOVE("全身愛撫",조교대상,-1,100)
				ENDIF
				RESETCOLOR
			ELSE
				CALL CARD_DISPOSE(対面者,"손패",選択カード,덱行き)
				IF RESULT == 1
					CALL CARDDRAW_BASIC(対面者,1)
					CALL CARDDRAW_BASIC(듀얼리스트,1)
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ENDIF

