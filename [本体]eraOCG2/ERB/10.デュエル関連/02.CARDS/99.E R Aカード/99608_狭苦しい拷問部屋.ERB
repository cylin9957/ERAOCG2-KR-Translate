@CARDNAME_99608,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 좁아터진 고문방
	CASE "略称"
		RESULTS = Ｈ방
	CASE "カテゴリ"
		RESULTS:0 = 없음
		RESULTS:2 = E.R.A
ENDSELECT


@CARD_99608(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常함정
	CASE "추가日"
		RETURN 250910
ENDSELECT

RETURN 0

@CARD_EXPLANATION_99608(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
CALL CARD_ABLE(1,마함起動,フリー誘発)
PRINTL ①：필드 위의 몬스터 ２장을 대상으로 하고 발동할 수 있다。
PRINTL 그 몬스터들을 턴 종료시까지 제외한다。
PRINTL 

SETCOLOR C_GRAY
PRINTL 아니야、나는 그저……발동하는것만으로 ２장의 몬스터를 제외하는 강력한 カード를 만들고싶었을 뿐이야……
RESETCOLOR
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_99608(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 選択カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,99608)
SIF RESULT == 1
	RETURN 0


IF 종류 == 마함起動 || 종류 == フリー誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 3
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT < 2
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_99608(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC 대상者

#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 대상
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 효과蓄積
#DIMS DYNAMIC 操作者

#DIM DYNAMIC 選択カードA
#DIM DYNAMIC 選択カードB
#DIM DYNAMIC 選択カードA_ID
#DIM DYNAMIC 選択カードB_ID
#DIM DYNAMIC 대상A
#DIM DYNAMIC 대상B
#DIM DYNAMIC カード성별A
#DIM DYNAMIC カード성별B
#DIM DYNAMIC 성교횟수A
#DIM DYNAMIC 성교횟수B
#DIMS DYNAMIC 대상呼称A
#DIMS DYNAMIC 대상呼称B




CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 마함起動 || 종류 == フリー誘発
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,99608)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)

		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲상대필드 = 1
		효과条件複数 = 2
		CALL CARD_CHOOSE_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT != 9999
			選択カードA = RESULT:0
			選択カードB = RESULT:1
			;A情報취득
			CALL CARD_SELECT(듀얼리스트,選択カードA)
			CALL CARD_EROTICS_FIELD(대상者（CS）,"필드",選択カード（CS）)
			IF RESULT != -1
				대상A = RESULT
				대상呼称A = %CALLNAME:RESULT%
			ELSE
				대상A = -1
				TRYCALLFORM CARDNAME_{選択カードID（CS）}, "名前"
				대상呼称A = %RESULTS%
			ENDIF
			カード성별A = GETVAR(@"%대상者（CS）%_MZ_性:選択カード（CS）")
			성교횟수A = GETVAR(@"%대상者（CS）%_MZ_攻:選択カード（CS）")
			;3000/500 = 3 
			TIMES 성교횟수A , 0.005
			;제외処理
			CALL CARD_SELECT(듀얼리스트,選択カードA)
			CALL CARD_DISPOSE(대상者（CS）,参照先（CS）,選択カード（CS）,제외行き,1)

			;B情報취득
			CALL CARD_SELECT(듀얼리스트,選択カードB)
			CALL CARD_EROTICS_FIELD(대상者（CS）,"필드",選択カード（CS）)
			IF RESULT != -1
				대상B = RESULT
				대상呼称B = %CALLNAME:RESULT%
			ELSE
				대상B = -1
				TRYCALLFORM CARDNAME_{選択カードID（CS）}, "名前"
				대상呼称B = %RESULTS%
			ENDIF
			カード성별B = GETVAR(@"%대상者（CS）%_MZ_性:選択カード（CS）")
			성교횟수B = GETVAR(@"%대상者（CS）%_MZ_攻:選択カード（CS）")
			TIMES 성교횟수B , 0.005
			;제외処理
			CALL CARD_SELECT(듀얼리스트,選択カードB)
			CALL CARD_DISPOSE(대상者（CS）,参照先（CS）,選択カード（CS）,제외行き,1)



			;無性は何もなし
			IF カード성별A <= 0 || カード성별B <= 0
			;男同士は何もなし
			ELSEIF カード성별A == 2 && カード성별B == 2
			;男（/ふたなり）＆女性はエッチ開始（男性主導権）
			ELSEIF カード성별A >= 2 && GROUPMATCH(カード성별B,1,3)
				PRINTFORMW %대상呼称A%와 %대상呼称B%는 좁아터진 방에 갇혔다。
				PRINTFORMW 둘만 있는 공간에서 탈출을 시도하지만... 방에 달콤한 향기가 퍼지자, 서로 본능을 억제하지 못하게 되었다。
				CALL EROTIC_MOVE("前穴凌辱",대상B,대상A,300,성교횟수A)
			;여성&남(또는 후타나리)은 에치 시작(여성 주도권)
			ELSEIF カード성별B >= 2 && GROUPMATCH(カード성별A,1,3)
				PRINTFORMW %대상呼称A%와 %대상呼称B%는 좁아터진 방에 갇혔다。
				PRINTFORMW 둘만 있는 공간에서 탈출을 시도하지만... 방에 달콤한 향기가 퍼지자, 서로 본능을 억제하지 못하게 되었다。
				CALL EROTIC_MOVE("逆レイプ",대상A,대상B,300,성교횟수B)
			ELSEIF カード성별A == 1 && カード성별B == 1
				PRINTFORMW %대상呼称A%와 %대상呼称B%는 좁아터진 방에 갇혔다。
				PRINTFORMW 둘만 있는 공간에서 탈출을 시도하지만... 방에 달콤한 향기가 퍼지자, 서로 본능을 억제하지 못하게 되었다。			
				CALL EROTIC_MOVE("全身레즈애무",대상B,대상A,300,성교횟수A)
			ENDIF


		ENDIF
	ENDIF
ENDIF



