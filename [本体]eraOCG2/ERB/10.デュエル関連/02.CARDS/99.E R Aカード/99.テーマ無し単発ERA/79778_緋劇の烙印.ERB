@CARDNAME_79778,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 비극의 낙인
	CASE "略称"
		RESULTS = 비극의 낙인
	CASE "カテゴリ"
		RESULTS:0 = 낙인
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_79778(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
	CASE "生成カード"
		RETURN 1884
	CASE "추가日"
		RETURN 250825
ENDSELECT

RETURN 0

@CARD_EXPLANATION_79778(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL 이 카드명의 ①②의 효과는 각각 １턴에 １번밖에 사용할 수 없다。
CALL CARD_ABLE(1,마함起動,손패起動)
PRINTL ①：덱밖에서 상대의 묘지에 "알버스의 낙윤" １장을 생성하고、
PRINTL 덱에서 레벨 ４인 마법사족 / 빛속성 / 암컷 몬스터 １장을 패에 넣는다。
CALL CARD_ABLE(2,ターン終了時)
PRINTL ②：자신 / 상대의 엔드 페이즈에 발동할 수 있다.
PRINTL 자신 묘지의 마법사족 / 빛속성 / 암컷 몬스터 １체를 제외하고, 묘지의 이 카드를 패에 넣는다。
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_79778(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 条件달성
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79778)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,79778)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件속성 = 빛속성
	효과条件종족 = 마법사족
	효과条件성별 = 牝性
	효과条件レベル = 4
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン終了時
	SIF 발동済_2(듀얼리스트,79778)
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件속성 = 빛속성
	효과条件종족 = 마법사족
	효과条件성별 = 牝性
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_79778(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%



IF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79778,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		SETCOLOR C_RED
		PRINTFORML >>——MESSAGE:——————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————>>>
		PRINTFORML
		PRINTFORML 알버스는 힘을 다해 쓰러졌다.
		PRINTFORML
		CALL FULLRESET
		CALL CARD_GENERATE_GY(対面者,1884)
		PRINTFORML 
		RESETCOLOR
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件속성 = 빛속성
		효과条件종족 = 마법사족
		효과条件성별 = 牝性
		효과条件レベル = 4
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ELSEIF 종류 == ターン終了時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79778,2)
	IF RESULT == 0
		SETCOLOR C_RED
		PRINTFORML >>——MESSAGE:——————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————>>>
		PRINTFORML
		PRINTFORML 성녀들은 공허한 눈으로 기도를 바친다……
		PRINTFORML 
		RESETCOLOR
		CALL FULLRESET
		효과条件속성 = 빛속성
		효과条件종족 = 마법사족
		효과条件성별 = 牝性
		효과条件行先플래그 = 제외行き
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"묘지",ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,손패行き)
		ENDIF
	ENDIF
ENDIF

