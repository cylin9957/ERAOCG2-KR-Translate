@CARDCAN_79891(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 조교대상


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79891)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,79891)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = 광서의 기계
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 攻撃時誘発
	SIF 발동済_2(듀얼리스트,79891)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
		CALL RESETAREA_MAT
	素材範囲필드 = 1
	素材範囲상대필드 = 1
	素材範囲묘지 = 1
	素材範囲상대묘지 = 1
	CALL FULLRESET
	효과範囲EX덱 = 1
	효과条件종류 = 융합
	효과条件カテゴリ = 광서의 기계
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_79891(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 選択結果
#DIM DYNAMIC 捕縛대상
#DIM DYNAMIC 捕縛場所,紙束最大値
#DIM DYNAMIC 捕縛場所B
#DIM DYNAMIC 捕縛候補,紙束最大値
#DIM DYNAMIC カウンタB
#DIM DYNAMIC 절정횟수
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 묘지送り位置

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79891,1)
	IF RESULT == 0
	CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = 광서의 기계
		CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			CALL CARD_ADD_GY_BASIC(듀얼리스트,"손패",ゾーン,場所)
			IF RESULT == 1
				FOR カウンタ,0,現山札数(対面者)
					捕縛대상 = GETVAR(@"%対面者%_덱:カウンタ") ;0→カウンタ
					SIF 捕縛대상 == -1 ;念のため-1の場合は次へ
						CONTINUE
					CALLFORM CARD_{捕縛대상},"성별"
					SIF RESULT == 無性 || RESULT == 雄性
						CONTINUE
						CALL CARD_EROTICS(捕縛대상)
					SIF RESULT < 1 ;CSV持ち以外は제외
						CONTINUE
					捕縛候補:カウンタB = 捕縛대상 ;牝性を候補に格納
					捕縛場所:カウンタB = カウンタ
					カウンタB++ ;候補数加算
				NEXT
				SIF カウンタB < 1 ;カウンタが0以下=候補0
					RETURN
				묘지送り位置 = (RAND:カウンタB)
				捕縛대상 = 捕縛候補:묘지送り位置 ;カウンタBまでの乱数で捕縛候補の中からランダムに捕縛대상を決定
				捕縛場所B = 捕縛場所:묘지送り位置
				CALL CARD_EROTICS(捕縛대상) ;必ずCARD_EROTICSを挟む
				조교대상 = RESULT
				IF 조교대상 > 0 ;조교대상条件はあなたを想定しないなら>0で
					SELECTCASE RAND:4
						CASE 0
							PRINTFORML
							PRINTFORML 강력한 기계병기를 움직이기 위한 양분을 얻기 위해, 암컷들은 가공되어 버린 육체를 계속해서 범해지고 있다,
							PRINTFORML 포획되어 있는 %CALLNAME:조교대상% 도 문득 그 1개체가 되어버렸음을 깨닫게 된다.
							PRINTFORMW 기계성의 시설 중 하나인 밀폐된 미약이 채워진 캡슐 안에서, %CALLNAME:조교대상% 는 모욕당하고 있다……
						CASE 1
							PRINTFORML
							PRINTFORML 대형의 광서의 기계를 가동시키기 위한 에너지를 얻기 위해, %CALLNAME:조교대상% 를 포함한 암육은 계속해서 범해지고 있었다
							PRINTFORML 절정과 발광을 반복하며, 정신이 부서질 것 같으면 억지로 보강되어 제정신을 유지하게 된다.
							PRINTFORMW %CALLNAME:조교대상% 는, 끝나지 않는 성적 유린에 절정과 신음소리를 되돌려, 광서의 기계에 바쳐지고 있다……
						CASE 2
							PRINTFORML
							PRINTFORML %CALLNAME:조교대상% 는 대형의 광서의 기계를 작동시키기 위한 에너지원으로 만들기 위해,
							PRINTFORML 사지을 절단당하고, 기계성의 시설 안에서 끝없이 범해지기 위한 암홀로 조립되어 버리고 말았다.
							PRINTFORMW %CALLNAME:조교대상% 를 포함한 수많은 암홀들의 신음소리가 기계성에 울려퍼진다……

						CASE 3
							PRINTFORML
							PRINTFORML 이미 포획되어 있던 암컷의 1개체가 기계병기로 손에 옮겨진다
							PRINTFORML 팔과 다리를 절단당하고, 고문을 당하며 계속해서 움직이지 않으면 안되는 그런 암육.
							PRINTFORMW %CALLNAME:조교대상%, 기계에 조립된 암컷의 복부에 그 안이 새겨져 있었다.
					ENDSELECT
					FOR カウンタ,0,4
						PRINTFORML 
						CALL MADORGASM_RAND_ROB_TXT(1,0)
						CALL MADORGASM_RAND_ROB_TXT(1,3)
						PRINTFORML
						CALL EROTIC_MOVE("全身凌辱",조교대상,MASTER,500)
					NEXT
					CALL MADORGASM_RAND_ROB_TXT(1,5);うるさい鳴き声
					PRINTFORML %CALLNAME:조교대상%는 몸 곳곳에서 체액을 흘리며, 눈물까지 흘리면서 필사적으로 구원을 요청하고 있다。
					PRINTFORMW 그 대신에 패에 더해진 광서의 기계는 움직이기 위한 양분을 얻어 언제든지 움직일 수 있는 것 같다, 듀얼의 판면에 놓아도 효과를 발휘할 것이다
				
				ENDIF
			CALL CARD_DISPOSE(対面者,"덱",捕縛場所B)
			ENDIF
		ENDIF
	ENDIF

ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79891,2)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL RESETAREA_MAT
		素材範囲필드 = 1
		素材範囲상대필드 = 1
		素材範囲묘지 = 1
		素材範囲상대묘지 = 1
		CALL FULLRESET
		효과条件종류 = 융합
		효과条件カテゴリ = 광서의 기계
		효과条件소환可 = 1
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"EX덱",ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
			TRYCALLFORM CARDSUMMON_{GETVAR(@"%듀얼리스트%_EX덱:{選択カード}")}(듀얼리스트,選択カード)
			CALL FUSSUMMON(듀얼리스트, 選択カード)
		ENDIF
	ENDIF
ENDIF




