@CARDNAME_79891,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 이쿠기어 슬레이브 커스텀 플랜
	CASE "略称"
		RESULTS = 광서암타계획
	CASE "カテゴリ"
		RESULTS:0 = 광서의 기계
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_79891(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
	CASE "추가日"
		RETURN 250706
ENDSELECT

RETURN 0

