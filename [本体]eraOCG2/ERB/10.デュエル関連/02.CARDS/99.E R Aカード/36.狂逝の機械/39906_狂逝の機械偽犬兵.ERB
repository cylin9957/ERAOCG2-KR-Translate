CALL TEXT_DECORATION("ROGUE")


@CARDCAN_39906(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 조교대상

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,39906)
SIF RESULT == 1
	RETURN 0

IF 종류 == 효과後誘発
	SIF 발동済_1(듀얼리스트,39906)
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件성별 = 牝性	
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"특수소환",1)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 필드起動
	SIF 발동済_2(듀얼리스트,39906)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
    SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
    CALL FULLRESET
	효과範囲손패 = 1
	효과範囲묘지 = 1
    효과条件종류 = モン全般
	효과条件カテゴリ = 광서의 기계
	효과条件소환可 = 1
    CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
    SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 攻撃後強制誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 戦闘場所(듀얼리스트,1) == 5
	RETURN 0
	SIF 戦闘場所(듀얼리스트) != 場所
	RETURN 0
    CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件표시 = 表側표시
	효과条件성별 = 성별在
	SIF RESULT != 1
		RETURN 0
	RETURN 1
	
ELSEIF 종류 == 攻撃後誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 戦闘場所(듀얼리스트) != 場所
		RETURN 0
	SIF 直前戦闘破壊ID(듀얼리스트,1) == 0
		RETURN 0	
	CALLFORM CARD_{直前戦闘破壊ID(듀얼리스트,1)}("성별")
	SIF RESULT != 牝性
		RETURN 0
	RETURN 1

ELSEIF 종류 == ターン終了時強制
	SIF ゾーン != "필드"
		RETURN 0
	SIF	GETVAR(@"%듀얼리스트%_MZ_汎数:場所") > 0
		RETURN 1
	SIF	GETVAR(@"%듀얼리스트%_MZ_変化元ID:場所") >= 0
		RETURN 1
	RETURN 1
ENDIF


@CARDEFFECT_39906(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 捕縛대상
#DIM DYNAMIC 捕縛候補,紙束最大値
#DIM DYNAMIC 捕縛場所,紙束最大値
#DIM DYNAMIC 捕縛場所B
#DIM DYNAMIC カウンタB
#DIM DYNAMIC 묘지送り位置
#DIM DYNAMIC 選択カード_ID

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39906,1)
	IF RESULT == 0
	효과範囲필드 = 1
        효과条件성별 = 牝性
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			CALL CARD_SP_SUMMON(듀얼리스트, "묘지", 場所)
			;範囲지정は不要
			IF RESULT == 1 ;一応「소환できた」を後続条件に
		CALL FULLRESET
				FOR カウンタ,0,現山札数(対面者)
					捕縛대상 = GETVAR(@"%対面者%_덱:カウンタ") ;0→카운터
					SIF 捕縛대상 == -1 ;혹시 -1인 경우는 다음으로
						CONTINUE
					CALLFORM CARD_{捕縛대상},"성별"
					SIF RESULT == 無性 || RESULT == 雄性
						CONTINUE
					CALL CARD_EROTICS(捕縛대상)
					SIF RESULT < 1 ;CSV를 가진 것 이외는 제외
						CONTINUE
					捕縛候補:カウンタB = 捕縛대상 ;암컷을 후보에 저장
					捕縛場所:カウンタB = カウンタ
					カウンタB++ ;후보 수 가산
				NEXT
				SIF カウンタB < 1 ;카운터가 0 이하 = 후보 0
					RETURN
				捕縛대상 = 捕縛候補:(RAND:カウンタB) ;카운터B까지의 난수로 포박 후보 중에서 랜덤으로 포박 대상을 결정
				捕縛대상 = 捕縛候補:묘지送り位置
				捕縛場所B = 捕縛場所:묘지送り位置
				CALL CARD_EROTICS(捕縛대상) ;반드시 CARD_EROTICS를 끼워 넣는다
				조교대상 = RESULT
				IF 조교대상 > 0 ;조교대상 조건은 당신을 상정하지 않는다면 >0으로
					CALL SET_FIELD_INFO(듀얼리스트,"필드",カード소환先,"変化元ID",捕縛대상,上書きモード)
					SELECTCASE RAND:4
						CASE 0
							PRINTFORML
							PRINTFORMW 기계성에 침입한 자들 앞에 한 마리의 기계병이 나타난다.
							PRINTFORMW 네 발로 달리는, 사람의 몸을 쉽게 덮을 수 있을 정도의 거대한 몸집을 가진 톱니바퀴로 움직이는 기계의 짐승,
							PRINTFORML 그 복부에 해당하는 부위에는, 손발을 잃은 %CALLNAME:조교대상%가 조립되어 있었다.
							PRINTFORMW 즈츄, 즈츄... 하고 복부가 부풀어 오를 정도의 굵기와 길이를 가진 인공 생식기로 손발뿐만 아니라 두 구멍도 기계의 짐승에 연결되어,
							PRINTFORML 등에는 살덩이 몸에 달린 철제 삽입구에서 삽입된 수많은 관을 통해 최음제가 흘러들고,
							PRINTFORMW 머리 윗부분을 덮는 세뇌용 바이저에서 지지, 지지지, 하는 소리가 계속 울린다……
						CASE 1
							PRINTFORML
							PRINTFORML 필드에 사람을 덮을 만큼의 톱니바퀴와 철 덩어리 같은 체구를 가진, 네 발로 달리는 기계의 짐승.
							PRINTFORMW 물론, 이 기계에도 그 배 부분에 결합되듯이 한 명의 암컷이 부착되어 있었다.
							PRINTFORMW %CALLNAME:조교대상%이, 사지를 잃은 모습으로, 머리를 세뇌 장치로 덮고, 배 속에 미독을 주입당한 모습이다.
							PRINTFORMW 「와옹♡왕앗, 와옹”♡오”♡와옹”♡왕”♡왕”응”♡」
							PRINTFORML 머리가 가려져 있기 때문에 그것을 본 침입자들은 그게 누구인지 알 수 없다.
							PRINTFORMW 「와앗♡왕♡왕응”와옹”♡♡와옹”♡왕”응”♡왕♡와오오오오옹”♡」
							PRINTFORML 그 암컷 개는 도움을 구하듯, 매달리듯 개 짖는 소리를 내며, 절정하고 조수를 뿜는 모습을 드러내고 있었다…
						CASE 2
							PRINTFORML
							PRINTFORML
							PRINTFORML 어느새 %CALLNAME:조교대상%은 네 발로 돌아다니는 짐승형 기계에 결합되어 있었다.
							PRINTFORMW 손발이 잘리고, 육체는 음란한 의미의 문양과 음문으로 뒤덮여, 머리에는 세뇌를 위한 장치가 결합되어 있다.
							PRINTFORMW 그래서 %CALLNAME:조교대상%은 개처럼 짖는 것밖에 할 수 없다.
							PRINTFORMW 「오옷”♡왕”♡와앙”♡와앗, 오옷읏오오”♡왕”♡왕응”♡」
							PRINTFORMW 항상 음부에 물려진 짐승의 생식기를 본뜬 육봉은, 질육의 깊은 곳에 있는 육의 꿀단지를 계속 두드리고,
							PRINTFORMW 부슉♡ 부슉♡ 부슉…, %CALLNAME:조교대상%은 애액과 조수를 뿌리며 그 냄새를 계속 퍼뜨리고 있다…
						
						CASE 3
							PRINTFORML
							PRINTFORML
							PRINTFORML 길을 잃었을 때 잡혔던 것인지, 아니면 이미 잡혀 있었던 것인지, 이유는 확실하지 않지만,
							PRINTFORMW %CALLNAME:조교대상%은 네 발로 돌아다니는 짐승형 기계에 결합된 채, 신음하며 괴롭힘당하는 육덩어리가 되어 있었다.
							PRINTFORMW 머리에 부착된 세뇌 장치에서 머리로 다양한 신호가 흘러가는 소리, 암컷 구멍이 비틀려 질육이 깎이는 듯 피스톤 장치로 찔리면서,
							PRINTFORMW %CALLNAME:조교대상%은 그 세뇌의 영향과 고통에서 오는 쾌락으로 개처럼 짖는 소리를 계속 내고 있다
							PRINTFORMW 「오옷”♡와옹”♡♡♡오”♡오옷”♡오오오오”♡♡와옹♡♡♡왕응n♡와아오옹”♡♡♡」
							PRINTFORMW 몸에 붙어 있는 기계가 움직일수록 그 괴롭힘은 강해지고, 그 쾌락에서 발출되는 신호가
							PRINTFORMW 몸속을 돌며, 주입된 나노머신이 활성화되어, 더 많은 에너지를 기계에 계속 보내고 있다.
							PRINTFORMW %CALLNAME:조교대상%은 순조롭게 암컷 육체로서의 역할을 다하고 있는 듯하다……
					ENDSELECT

				ELSE
					PRINTFORML
					PRINTFORMW 광서의 기계위견병이 특수 소환되었습니다
				ENDIF
				CALL CARD_DISPOSE(対面者,"덱",捕縛場所B)
			ENDIF
		ENDIF
	ENDIF
ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39906,2)
    IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,제외行き)
		CALL FULLRESET
		효과範囲손패 = 1
        효과範囲묘지 = 1
        효과条件종류 = モン全般
		효과条件カテゴリ = 광서의 기계
		효과条件固名前 =  이쿠기어 웨어 독 골렘
		CALL CARD_SP_SUMMON_ADV (듀얼리스트,ゾーン,場所)
			IF RESULT == 1
				CALL SET_FIELD_INFO(듀얼리스트,"필드",カード소환先,"発情",2,1)
				CALL SET_FIELD_INFO(듀얼리스트,"필드",カード소환先,"薬Ｃ",1)
		ENDIF
	ENDIF

ELSEIF 종류 == 攻撃後強制誘発
CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39906,3)
	IF RESULT == 0
		CALL FULLRESET
         효과条件성별 = 성별在
         CALL STATCHANGE_EFFECT(対面者,"필드",選択カード,"発情",1)
	ENDIF

ELSEIF 종류 == 攻撃後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,39904)
		IF RESULT == 0
			CALL FULLRESET
            CALL CARD_EROTICS(直前戦闘破壊ID(듀얼리스트,1))
		IF RESULT != -1
			조교대상 = RESULT
			SELECTCASE RAND:3
				CASE 0
				PRINTFORMW 기계 위견병의 외장에 억지로 땅에 짓눌려버린 %CALLNAME:조교대상%는 상처 입은 몸 그대로 기계 위견병.
				PRINTFORMW 앞발로 상반신을 완전히 땅에 짓누르듯이 잡혀, 하반신을 위견병에게 잡힌 암컷이 %CALLNAME:조교대상%의 두 구멍을 핥고 최음제를 주입하며 핥아 바른다,
				PRINTFORMW 완전히 발정되어 하반신이 젖은 %CALLNAME:조교대상%는, 하반신에 짐승의 생식기를 본뜬 것을 눌러대며, 앞으로 당할 일을 알아차리고 얼굴이 창백해진다……
				CASE 1
				PRINTFORMW 전투에서 쓰러져, 몸에 두른 것도 너덜너덜해진 %CALLNAME:조교대상%는 기계 위견병에게 덮쳐지듯 짓눌려버린다.
				PRINTFORMW 위견병의 머리뿐만 아니라, 조립된 암컷도 %CALLNAME:조교대상%가 입고 있는 것을 입으로 물어뜯어 뜯어낸다,
				PRINTFORMW 전부 뜯겨 아무것도 걸치지 않은 %CALLNAME:조교대상%에게, 위견병의 짐승 생식기가 눌러진다……
