@CARDNAME_39906,参照先
;여기서 지정 카드의 이름, 약칭을 반환할 예정
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 이쿠기어 웨어 독 골렘
	CASE "略称"
		RESULTS = 광서의위견병
	CASE "カテゴリ"
		RESULTS:0 = 광서의 기계
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_39906(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 기계족
	CASE "レベル"
		RETURN 6
	CASE "攻撃力"
		RETURN 2600
	CASE "守備力"
		RETURN 1500
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

