@CARDNAME_79619,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 섹슈얼 아레나
	CASE "略称"
		RESULTS = 이성투기장
	CASE "カテゴリ"
		RESULTS:0 = 없음
		RESULTS:1 = E.R.A
ENDSELECT


@CARD_79619(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN フィールド
	CASE "추가日"
		RETURN 250917
ENDSELECT

RETURN 0

@CARD_EXPLANATION_79619(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
PRINTL 자신 필드는 "이성투기장"이 된다。
PRINTL 이 카드의 발동、효과에 대해서、상대는 카드의 효과를 발동할 수 없다。
CALL CARD_ABLE(0,필드起動,손패起動)
PRINTL ①：이 카드의 발동시의 효과 처리로서、필드의 무성 몬스터를 전부 주인의 덱 가장 아래로 되돌린다。
CALL CARD_ABLE(0,발동強制誘発)
PRINTL ②：무성 몬스터가 효과를 발동했을 때에 발동한다。
PRINTL 그 발동과 효과를 무효로 하고、덱의 가장 아래로 되돌린다。
CALL CARD_ABLE(0,攻守変動)
PRINTL ③：필드가 "이성투기장"인 한、
PRINTL 필드의 무성 몬스터의 공격력 / 수비력은 ０이 된다。
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_79619(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79619)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	IF 종류 == 마함起動
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ENDIF
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	RETURN 1
ELSEIF 종류 == 발동強制誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 1
		RETURN 0
	CALL FULLRESET
	효과条件성별 = 無性
	효과条件종류 = モン全般
	CALL CARD_CHECK_START_DATA_2(듀얼리스트,2)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 攻守変動 || 종류 == 지속효과
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == チェーン可否
	RETURN 1
ENDIF

@CARDEFFECT_79619(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 조교대상
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동＆설치まで
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79619)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
		CALL FULLRESET
		효과条件성별 = 無性
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"両필드全体",場所,"덱")
	ENDIF
ELSEIF 종류 == 발동強制誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79619)
	IF RESULT == 0
		CALL FULLRESET
		효과条件성별 = 無性
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効2",場所,"덱")
	ENDIF
ELSEIF 종류 == 攻守変動
	CALL FULLRESET
	효과条件성별 = 無性
	CALL STATCHANGE_BASIC(듀얼리스트,"필드",場所,-999999999,-999999999)
	CALL STATCHANGE_BASIC(듀얼리스트,"상대필드",場所,-999999999,-999999999)
ELSEIF 종류 == 지속효과
	CALL SET_FIELD_MAGIC_INFO(듀얼리스트, "이성투기장")
ENDIF
