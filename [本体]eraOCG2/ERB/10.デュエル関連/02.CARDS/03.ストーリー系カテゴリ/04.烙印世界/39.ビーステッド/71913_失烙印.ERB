@CARDNAME_71913,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 실낙인
	CASE "略称"
		RESULTS = 실낙인
	CASE "カテゴリ"
		RESULTS:0 = 낙인
ENDSELECT


@CARD_71913(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 지속마법
	CASE "추가日"
		RETURN 250903
ENDSELECT
RETURN 0

@CARD_EXPLANATION_71913(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ②의 효과는 １턴에 １번밖에 사용할 수 없다。
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ①：이 카드가 마법＆함정 존에 존재하는 한、
PRINTL 자신이 융합 몬스터를 특수 소환할 때에 상대는 마법 / 함정 / 몬스터의 효과를 발동할 수 없다。
CALLFORM CARD_ABLE(2,효과後誘発)
PRINTL ②：자신이 EX덱에서 융합 몬스터를 특수 소환한 경우에 발동할 수 있다。
PRINTL 덱에서 "알버스의 낙윤" 또는 "알버스" 몬스터 １장을 패에 넣는다。


@CARDCAN_71913(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 条件달성
#DIM DYNAMIC EX条件달성
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 효과使用불가,10
CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,71913)
SIF RESULT == 1
	RETURN 0


IF 종류 == 지속효과
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 1
		RETURN 0
	SIF FINAL_SUMMON_TARGET(듀얼리스트,0) == 5
		RETURN 0
	TRYCALLFORM CARD_{GETVAR(@"%듀얼리스트%_필드:{FINAL_SUMMON_TARGET(듀얼리스트,0)}")},"종류"
	SIF !MATCH(RESULT,융합)
		RETURN 0
	RETURN 1

ELSEIF 종류 == 효과後誘発
	SIF 발동済_2(듀얼리스트,71913)
		RETURN 0
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 1
		RETURN 0
	SIF FINAL_SUMMON_TARGET(듀얼리스트,0) == 5
		RETURN 0
	TRYCALLFORM CARD_{GETVAR(@"%듀얼리스트%_필드:{FINAL_SUMMON_TARGET(듀얼리스트,0)}")},"종류"
	SIF !MATCH(RESULT,융합)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_召種:{FINAL_SUMMON_TARGET(듀얼리스트,0)}") != 召種EX덱
		RETURN 0
	RETURN 1
ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	IF 종류 == 마함起動
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ENDIF
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_71913(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC EX参照
#DIM DYNAMIC 효과使用불가,10
#DIM DYNAMIC 素材数
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択済カード
#DIM DYNAMIC EX条件달성
;융합소환
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 지속효과
	CALL SET_PLAYER_INFO(듀얼리스트,"특수阻害中止",3)
ELSEIF  종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,71913,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件スーパー名前 = 알버스의 낙윤
		효과条件カテゴリ = 알버스
		효과条件종류 = モン全般
		CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
	ENDIF

ELSEIF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,71913)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
	ENDIF
ENDIF
