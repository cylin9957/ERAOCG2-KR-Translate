@CARDNAME_34050,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 클락 와이반
	CASE "略称"
		RESULTS = Ｃ 와이반
	CASE "カテゴリ"
		RESULTS:0 = 없음
ENDSELECT


@CARD_34050(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 바람속성
	CASE "종족"
		RETURN 사이버스족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1800
	CASE "守備力"
		RETURN 1000
	CASE "生成カード"
		RETURN 134050
	CASE "추가日"
		RETURN 250908
ENDSELECT

RETURN 0

@CARD_EXPLANATION_34050(종류)
#DIM DYNAMIC 종류

PRINTL このカード名の①の효과は１ターンに１度しか使用できない。
CALLFORM CARD_ABLE(1,소환時,특수소환時)
PRINTL ①：このカードが소환・특수소환に成功した場合に발동できる。
PRINTL このカードの攻撃力を半分にし、자신フィールドに「클락 토큰」１体を특수소환する。


@CARDCAN_34050(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,34050)
SIF RESULT == 1
	RETURN 0
IF 종류 == 소환時 || 종류 == 특수소환時
	SIF 발동済_1(듀얼리스트,34050)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,134050,"")
	SIF RESULT == 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_34050(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 選択カード

IF 종류 == 소환時 || 종류 == 특수소환時
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,34050,1)
	IF RESULT == 0
		攻撃力修正 = (GETVAR(@"%듀얼리스트%_MZ_攻:場所") / 2)
		CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"永攻",-攻撃力修正)
		CALL FULLRESET
		CALL CARD_GENERATE(듀얼리스트,134050)
	ENDIF
ENDIF



