@CARDNAME_57116,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 프록시 F 매지션
	CASE "略称"
		RESULTS = PF 매지션
	CASE "カテゴリ"
		RESULTS:0 = 없음
ENDSELECT


@CARD_57116(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 링크
	CASE "속성"
		RETURN 화염속성
	CASE "종족"
		RETURN 사이버스족
	CASE "レベル"
		RETURN 2
	CASE "攻撃力"
		RETURN 1400
	CASE "守備力"
		RETURN 1100
	CASE "추가日"
		RETURN 250909
ENDSELECT

RETURN 0
@CARDSUMMON_57116(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照

CALL RESETCONDITION_MAT
必要링크数 = 2
素材条件종류 = 효과全般

IF EX参照 == 1
	CALL LINCAN(듀얼리스트,소환カード,57116) 
	RETURN RESULT
ENDIF

@CARD_EXPLANATION_57116(종류)
#DIM DYNAMIC 종류

PRINTL 효과モンスター２体
PRINTL このカード名の①②の효과はそれぞれ１ターンに１度しか使用できない。
CALL CARD_ABLE(1,필드起動)
PRINTL ①：자신メインフェイズに발동できる。
PRINTL 자신フィールドから、융합モンスターカードによって決められた융합素材モンスターを묘지へ送り、
PRINTL その융합モンスター１体をEX덱から융합소환する。
CALL CARD_ABLE(2,효과後誘発)
PRINTL ②：융합モンスターが융합소환された場合に발동できる。
PRINTL 손패から攻撃力１０００以下のモンスター１体を특수소환する。

@CARDCAN_57116(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 必要カード
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,57116)
SIF RESULT == 1
	RETURN 0

IF 종류 == 필드起動
	SIF 발동済_1(듀얼리스트,57116)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL RESETAREA_MAT
	素材範囲필드 = 1
	CALL FULLRESET
	효과範囲EX덱 = 1
	효과条件종류 = 융합
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 효과後誘発
	SIF 발동済_2(듀얼리스트,57116)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과条件종류 = 융합
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"소환全般")
	SIF RESULT != 1
		RETURN 0

	CALL FULLRESET
	효과範囲손패 = 1
	효과条件攻撃力 = 1
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	RETURN 1
ENDIF


@CARDEFFECT_57116(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID 


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,57116,1)
	IF RESULT == 0
		CALL RESETAREA_MAT
		素材範囲필드 = 1
		CALL FULLRESET
		효과条件종류 = 융합
		효과条件소환可 = 1	
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"EX덱",ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
			TRYCALLFORM CARDSUMMON_{GETVAR(@"%듀얼리스트%_EX덱:{選択カード}")}(듀얼리스트,選択カード)
			CALL FUSSUMMON(듀얼리스트, 選択カード)
		ENDIF
	ENDIF
ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,57116,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과条件攻撃力 = 1
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF
