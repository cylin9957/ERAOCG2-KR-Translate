@CARDNAME_24335,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 사운드 워리어 기타스
	CASE "略称"
		RESULTS = 기타스
	CASE "カテゴリ"
		RESULTS:0 = 사운드 워리어
ENDSELECT


@CARD_24335(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 바람속성
	CASE "종족"
		RETURN 기계족
	CASE "レベル"
		RETURN 3
	CASE "攻撃力"
		RETURN 1500
	CASE "守備力"
		RETURN 100
	CASE "分割"
		RETURN 펜듈럼
	CASE "추가日"
		RETURN 250914
ENDSELECT

RETURN 0

@CARD_EXPLANATION_24335(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("P_MAGIC")
PRINTL 이 카드명의 ①②의 Ｐ효과는 각각 １턴에 １번밖에 사용할 수 없다。
CALLFORM CARD_ABLE(1,마함起動)
PRINTL ①：자신의 마법＆함정 존에 이 카드명 이외의 Ｐ몬스터 카드가 존재하는 경우에발동할 수 있다。
PRINTL 패 / ＥＸ덱에서 레벨６ 이하에、ＥＸ몬스터 이외의 몬스터 １장을 특수 소환한다。
CALLFORM CARD_ABLE(2,마함起動)
PRINTL ②：패를 １장 버리고 발동할 수 있다。
PRINTL 덱에서 "사운드 워리어 기타스" 이외의 "사운드 워리어" 몬스터 １장을 특수 소환한다。
CALL TEXT_DECORATION("P_MAGIC_2")
PRINTL 이 카드명의 ①의 몬스터 효과는 １턴에 １번밖에 사용할 수 없다。
CALLFORM CARD_ABLE(3,소환時)
PRINTL ①：이 카드가 소환에 성공한 경우에발동할 수 있다。
PRINTL 자신 묘지의 "사운드 워리어"몬스터 １체를 특수소환한다。

@CARDCAN_24335(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 발동불가,4

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,24335)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0

	SIF 발동済_1(듀얼리스트,24335)
		발동불가:0 = 1
	CALL FULLRESET
	효과範囲마함 = 1
	효과条件分割 = 펜듈럼
	효과条件표시 = 表側표시
	효과条件他所 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		발동불가:0 = 1
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲EX덱 = 1
	효과条件종류 = EX以外全般
	CALLFORM 효과条件レベル_ADV(6,"以下")
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		발동불가:0 = 1
	
	SIF 발동済_2(듀얼리스트,24335)
		발동불가:1 = 1
	SIF 손패数(듀얼리스트) < 1
		발동불가:1 = 1
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 사운드 워리어
	효과条件固名前 = 사운드 워리어 기타스
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		발동불가:1 = 1
	
	SIF 발동불가:0 == 1 && 발동불가:1 == 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 손패起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		RETURN 0
	CALL FULLRESET
	효과範囲마함 = 1
	효과条件名前 = 사운드 워리어 기타스
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1 && 듀얼리스트 != "자신"
		RETURN 0
	RETURN 1

ELSEIF 종류 == 소환時
	SIF 발동済_3(듀얼리스트,24335)
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = 사운드 워리어
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_24335(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 발동불가,4

IF 종류 == 마함起動
	SIF 발동済_1(듀얼리스트,24335)
		발동불가:0 = 1
	CALL FULLRESET
	효과範囲마함 = 1
	효과条件分割 = 펜듈럼
	효과条件표시 = 表側표시
	효과条件他所 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		발동불가:0 = 1
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲EX덱 = 1
	효과条件종류 = EX以外全般
	CALLFORM 효과条件レベル_ADV(6,"以下")
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		발동불가:0 = 1
	
	SIF 발동済_2(듀얼리스트,24335)
		발동불가:1 = 1
	SIF 손패数(듀얼리스트) < 1
		발동불가:1 = 1
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 사운드 워리어
	효과条件固名前 = 사운드 워리어 기타스
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		발동불가:1 = 1
	
	IF 발동불가:0 != 1 && 발동불가:1 != 1
		CALL YN_CHOICE(듀얼리스트,"①：패 / ＥＸ덱에서 레벨６ 이하를 특수 소환한다。","②：패를 버리고 덱에서「사운드 워리어」몬스터를 특수 소환한다。")
	ELSEIF 발동불가:0 != 1
		RESULT = 0
	ELSEIF 발동불가:1 != 1
		RESULT = 1
	ENDIF

	IF RESULT == 0
		CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,24335,1)
		IF RESULT == 0
			CALL FULLRESET
			효과範囲손패 = 1
			효과範囲EX덱 = 1
			효과条件종류 = EX以外全般
			CALLFORM 효과条件レベル_ADV(6,"以下")
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	
	ELSEIF RESULT == 1
		CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,24335,2)
		IF RESULT == 0
			CALL FULLRESET
			효과範囲손패 = 1
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
			IF RESULT == 1
				CALL FULLRESET
				효과範囲덱 = 1
				효과条件カテゴリ = 사운드 워리어
				효과条件固名前 = 사운드 워리어 기타스
				CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
			ENDIF
		ENDIF
	ENDIF

ELSEIF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,24335)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
	ENDIF

ELSEIF 종류 == 소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,24335,3)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件カテゴリ = 사운드 워리어
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF

