	CASE "名前"
		RESULTS = 음양사 타오타오
	CASE "略称"
		RESULTS = 음양사 타오타오
	CASE "カテゴリ"
		RESULTS:0 = 없음
ENDSELECT


@CARD_1664(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 미확인족
	CASE "レベル"
		RETURN 2
	CASE "攻撃力"
		RETURN 900
	CASE "守備力"
		RETURN 600
	CASE "튜너"
		RETURN 1
	CASE "성별"
		RETURN 牝性
	CASE "추가日"
		RETURN 250804
ENDSELECT

RETURN 0

@CARD_EXPLANATION_1664(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ②의 효과는 １턴에 １번밖에 사용할 수 없다。
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ①：이 카드가 몬스터와 전투를 행한 경우、그 ２장은 그 전투에선 파괴되지 않는다。
CALLFORM CARD_ABLE(2,효과後誘発)
PRINTL ②：자신이 전투 / 효과로 데미지를 받았을 경우에 발동할 수 있다。
PRINTL 자신 패 / 묘지에서 레벨 ３ 이상의 미확인족 몬스터 １장을 특수 소환한다。
PRINTL 받은 데미지가 ２０００ 이상인 경우、특수 소환할 몬스터를 덱 / ＥＸ덱에서 선택할 수 있다。

@CARDCAN_1664(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,1664)
SIF RESULT == 1
	RETURN 0

IF 종류 == 효과後誘発
	SIF 발동済_2(듀얼리스트,1664)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	CALL CARD_CHECK_FIN_BIT(듀얼리스트,"被弾者")
	SIF RESULT != 1
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_変動値") <= 0
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲묘지 = 1
	IF GETVAR(@"%듀얼리스트%_変動値") >= 2000
		효과範囲덱 = 1
		효과範囲EX덱 = 1
	ENDIF
	효과条件종족 = 미확인족
	CALLFORM 효과条件レベル_ADV(3,"以上")
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	SIF 戦闘場所(듀얼리스트,1) == 5
		RETURN 0
	SIF 戦闘場所(듀얼리스트) != 場所
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_1664(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 戦闘カード
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,1664,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲묘지 = 1
		IF GETVAR(@"%듀얼리스트%_変動値") >= 2000
			효과範囲덱 = 1
			효과範囲EX덱 = 1
		ENDIF
		효과条件종족 = 미확인족
		CALLFORM 효과条件レベル_ADV(3,"以上")
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF

ELSEIF 종류 == 지속효과
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"戦耐")
	CALL STATCHANGE_EFFECT(対面者,"自身",戦闘場所(듀얼리스트,1),"戦耐")
ENDIF
