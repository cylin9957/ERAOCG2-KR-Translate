@CARDNAME_73606,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 라그랑주 오르비트
	CASE "略称"
		RESULTS = Lagrange orrbit
	CASE "カテゴリ"
		RESULTS:0 = 없음
		RESULTS:1 = 게임 오리지널
ENDSELECT


@CARD_73606(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
	CASE "レア度"
		RETURN ULR
	CASE "추가日"
		RETURN 250909
ENDSELECT

RETURN 0

@CARD_EXPLANATION_73606(종류)
#DIM DYNAMIC 종류

PRINTL このカード名の①②の효과はそれぞれ１ターンに１度しか使用できない。
CALLFORM CARD_ABLE(1,마함起動 ,손패起動)
PRINTL ①：자신の손패・フィールドから미확인족モンスター１体を제외して발동できる。
PRINTL 자신は덱から２枚ドローする。
CALLFORM CARD_ABLE(2,묘지起動)
PRINTL ②：묘지のこのカードを제외して발동できる。
PRINTL 자신はフィールドの［カード名の異なるモンスター（表側표시）の数×３００］ＬＰ回復する。
PRINTL この효과で１５００以上回復した場合、さらに자신の묘지・제외状態の미확인족モンスター１体を攻撃표시で특수소환できる。

@CARDCAN_73606(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,73606)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,73606)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲필드 = 1
	효과条件종족 = 미확인족
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL CARD_DRAW_CHECK
	RETURN 1

ELSEIF 종류 == 묘지起動
	SIF 발동済_2(듀얼리스트,73606)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件표시 = 表側표시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_73606(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 回復量修正
#DIM DYNAMIC 選択カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73606,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲필드 = 1
		효과条件종족 = 미확인족
		효과条件종류 = モン全般
		효과条件行先플래그 = 제외行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL CARDDRAW_BASIC(듀얼리스트,2)
		ENDIF
	ENDIF

ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,73606,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲상대필드 = 1
		효과条件표시 = 表側표시
		효과条件固名前起動 = 1
		효과条件CNT = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		回復量修正 = RESULT * 300

		CALL LIFE_CHANGE(듀얼리스트,"増加","효과",回復量修正)
		IF 回復量修正 >= 1500
			CALL FULLRESET
			효과範囲묘지 = 1
			효과範囲제외 = 1
			효과条件종족 = 미확인족
			효과条件任意 = 1
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所,"攻撃표시")
		ENDIF
	ENDIF
ENDIF

