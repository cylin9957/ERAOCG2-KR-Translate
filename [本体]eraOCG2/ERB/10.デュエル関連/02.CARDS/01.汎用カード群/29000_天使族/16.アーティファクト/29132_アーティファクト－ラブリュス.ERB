@CARDNAME_29132,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 아티팩트－라브류스
	CASE "略称"
		RESULTS = ＡＦ라브류스
	CASE "カテゴリ"
		RESULTS:0 = 아티팩트
ENDSELECT


@CARD_29132(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 천사족
	CASE "レベル"
		RETURN 5
	CASE "攻撃力"
		RETURN 2300
	CASE "守備力"
		RETURN 1000
	CASE "추가日"
		RETURN 250912
ENDSELECT
RETURN 0

@CARD_EXPLANATION_29132(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ③의 효과는 １턴에 １번밖에 사용할 수 없다。
CALLFORM CARD_ABLE(0,손패起動)
PRINTL ①：자신 메인 페이즈에 발동할 수 있다。
PRINTL 패의 이 카드를 자신의 마법＆함정 존에 세트한다。
PRINTL 그 후、자신 필드에 뒷면 표시 카드가 ２장 이상 존재하는 경우、
PRINTL 덱에서 이 카드명 이외의 "아티팩트" 몬스터 １장을 패에 넣을 수 있다。
CALLFORM CARD_ABLE(0,戦破壊→묘지)
PRINTL ②：마법＆함정 존에 세트된 이 카드가 상대 턴에 파괴되어 묘지로 보내진 경우에 발동할 수 있다。
PRINTL 이 카드를 특수 소환한다。
CALLFORM CARD_ABLE(3,특수소환時,효과後誘発)
PRINTL ③：이 카드가 상대 턴에 특수 소환된 경우、
PRINTL 또는 이 카드가 패에 존재하고、자신의 "아티팩트" 카드가 파괴된 경우에 발동할 수 있다。
PRINTL 패에서 "아티팩트" 몬스터 １장을 특수 소환한다。


@CARDSUMMON_AA_29132


@CARDCAN_29132(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,29132)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		RETURN 0
	RETURN 1

ELSEIF 종류 == 戦破壊→묘지
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") <= 1
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,29132,"묘지")
	SIF RESULT == 1
		RETURN 0
	SIF ターンプレイヤー == 듀얼리스트
		RETURN 0
	SIF 묘지送り종류 != "破壊"
		RETURN 0
	RETURN 1

ELSEIF 종류 == 특수소환時 || 종류 == 효과後誘発
	SIF 발동済_3(듀얼리스트,29132)
		RETURN 0
	IF 종류 == 특수소환時
		SIF ターンプレイヤー == 듀얼리스트
			RETURN 0

	ELSEIF 종류 == 효과後誘発
		SIF ゾーン != "손패"
			RETURN 0
		CALL FULLRESET
		효과条件カテゴリ = 아티팩트
		CALL CARD_CHECK_FIN_DATA(듀얼리스트,"破壊全般")
		SIF RESULT != 1
			RETURN 0
	ENDIF
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件カテゴリ = 아티팩트
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_29132(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 選択カード

IF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,29132)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き,1)
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲마함 = 1
		효과条件표시 = 裏側표시
		효과条件CNT = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT >= 2
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件カテゴリ = 아티팩트
			효과条件종류 = モン全般
			효과条件固名前 = 아티팩트－라브류스
			효과条件任意 = 1
			CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF

ELSEIF 종류 == 戦破壊→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,29132)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件名前 = 아티팩트－라브류스
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF

ELSEIF 종류 == 특수소환時 || 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,29132,3)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과条件カテゴリ = 아티팩트
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF



