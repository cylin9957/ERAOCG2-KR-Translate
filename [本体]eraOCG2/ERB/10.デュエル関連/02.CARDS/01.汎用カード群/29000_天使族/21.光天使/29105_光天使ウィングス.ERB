@CARDNAME_29105,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 홀리 라이트닝 윙스
	CASE "略称"
		RESULTS = 홀리 라이트닝 윙스
	CASE "カテゴリ"
		RESULTS:0 = 홀리 라이트닝
ENDSELECT


@CARD_29105(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 천사족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1200
	CASE "守備力"
		RETURN 1800
	CASE "추가日"
		RETURN 250823
ENDSELECT
RETURN 0

@CARD_EXPLANATION_29105(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①の효과は１턴에 １번밖에 사용할 수 없다。
CALLFORM CARD_ABLE(1,효과後誘発)
PRINTL ①：이 카드가 일반 드로우 이외의 방법으로 패에 더해진 경우에 발동할 수 있다。
PRINTL 이 턴 자신은 일반 소환에 더하여 한 번만 천사족 몬스터를 소환할 수 있다。
CALLFORM CARD_ABLE(2,소환時)
PRINTL ②：이 카드가 소환에 성공한 경우에 발동할 수 있다。
PRINTL 패에서 레벨 ４ 빛속성 몬스터 １체를 특수 소환한다。

@CARDCAN_29105(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,29105)
SIF RESULT == 1
	RETURN 0


IF 종류 == 효과後誘発
	SIF 발동済_1(듀얼리스트,29105)
		RETURN 0
	SIF ドローフェイズ == 1
		RETURN 0
	SIF ゾーン != "손패"
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,29105,"손패")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과条件名前 = 홀리 라이트닝 윙스
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"손패취득+D")
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 소환時
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件속성 = 빛속성
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 短紋章起動
	RETURN 1
ENDIF


@CARDEFFECT_29105(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,29105,1)
	IF RESULT == 0
		CALL CARD_RESERV(듀얼리스트,29105,短紋章予約)
	ENDIF
ELSEIF 종류 == 소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,29105,2)
		IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과条件속성 = 빛속성
		효과条件レベル = 4
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ELSEIF 종류 == 短紋章起動
	CALL FULLRESET
	효과条件종족 = 천사족
	효과条件レベル = 4
	CALL STATCHANGE_HAND(듀얼리스트,"全体",場所,"召可")
ENDIF


