@CARDNAME_50192,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = Em그림자화가 섀도우 메이커
	CASE "略称"
		RESULTS = Em그림자화가
	CASE "カテゴリ"
		RESULTS:0 = 엔터메이지
ENDSELECT


@CARD_50192(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 엑시즈
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 마법사족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 2600
	CASE "守備力"
		RETURN 1000
	CASE "성별"
		RETURN 雄性
	CASE "추가日"
		RETURN 250829
ENDSELECT
RETURN 0

@CARDSUMMON_50192(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照


CALL RESETCONDITION_MAT
必要素材数 = 3
素材条件レベル = 5
素材条件종족 = 마법사족

IF EX参照 == 1
	CALL XYZCAN(듀얼리스트,소환カード,50192) 
	RETURN RESULT
ENDIF

@CARD_EXPLANATION_50192(종류)
#DIM DYNAMIC 종류

PRINTL レベル５モンスター×３
PRINTL このカードは１ターンに１度、자신フィールドのランク４の마법사족モンスターの上に重ねてX소환する事もできる。
PRINTL このカード名の①の효과は１ターンに１度しか使用できない。
CALLFORM CARD_ABLE(1,필드起動)
PRINTL ①：このカードのX素材を１つ取り除き、덱から「Em」モンスター１体を묘지へ送って발동できる。
PRINTL 덱から「RUM」마법カード１枚を손패に加える。
CALLFORM CARD_ABLE(0,발동誘発)
PRINTL ②：상대がカードの효과を발동した時にこのカードのＸ素材を１つ取り除いて발동できる。
PRINTL このカードのX素材を１つ取り除き、EX덱から「Em그림자화가 섀도우 메이커」１体を특수소환する。
PRINTL その後、このカードにX素材が存在しない場合、자신の묘지から「Em」モンスター１体を특수소환する。


@CARDCAN_50192(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 必要カード
#DIM DYNAMIC 条件달성
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,50192)
SIF RESULT == 1
	RETURN 0

IF 종류 == EX덱起動
	SIF 발동済_1(듀얼리스트,50192)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_EX_CHECK(듀얼리스트,50192)
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件レベル = 4
	효과条件종족 = 마법사족
	효과条件重ねてＸ = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드起動
	SIF 발동済_2(듀얼리스트,50192)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_Ｘ:場所") <= -1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = RUM
	효과条件종류 = 마법全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 엔터메이지
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	RETURN 1


ELSEIF 종류 == 발동誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_Ｘ:場所") <= -1
		RETURN 0
	CALL FULLRESET
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲EX덱 = 1
	효과条件名前 = Em그림자화가 섀도우 메이커
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_50192(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 攻撃力修正
IF 종류 == EX덱起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,50192,1)
	IF RESULT == 0
		CALL RESETCONDITION_MAT
		必要素材数 = 1

		素材条件종류 = 엑시즈
		素材条件レベル = 4
		素材条件종족 = 마법사족
		重ねてＸ = 1
		TRYCALLFORM XYZSUMMON(듀얼리스트,場所)
	ENDIF

ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,50192,2,X消費1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = 엔터메이지
		효과条件종류 = モン全般
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件カテゴリ = RUM
			효과条件종류 = 마법全般
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,50192,0,X消費1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲EX덱 = 1
		효과条件名前 = Em그림자화가 섀도우 메이커
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)

		IF GETVAR(@"%듀얼리스트%_MZ_Ｘ:場所") <= -1
			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件カテゴリ = 엔터메이지
			효과条件任意 = 1
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ENDIF