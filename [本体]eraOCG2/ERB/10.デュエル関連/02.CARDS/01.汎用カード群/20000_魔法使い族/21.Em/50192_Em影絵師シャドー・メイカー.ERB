@CARDNAME_50192,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = Em그림자화가 섀도우 메이커
	CASE "略称"
		RESULTS = Em그림자화가
	CASE "カテゴリ"
		RESULTS:0 = 엔터메이지
ENDSELECT


@CARD_50192(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 엑시즈
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 마법사족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 2600
	CASE "守備力"
		RETURN 1000
	CASE "성별"
		RETURN 雄性
	CASE "추가日"
		RETURN 250829
ENDSELECT
RETURN 0

@CARDSUMMON_50192(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照


CALL RESETCONDITION_MAT
必要素材数 = 3
素材条件レベル = 5
素材条件종족 = 마법사족

IF EX参照 == 1
	CALL XYZCAN(듀얼리스트,소환カード,50192) 
	RETURN RESULT
ENDIF

@CARD_EXPLANATION_50192(종류)
#DIM DYNAMIC 종류

PRINTL 레벨５ 몬스터×３
PRINTL 이 카드는 １턴에 １번、자신 필드의 랭크４인 마법사족 몬스터 위에 겹쳐서 X소환할 수 있다。
PRINTL 이 카드명의 ①의 효과는 １턴에 １번밖에 사용할 수 없다。
CALLFORM CARD_ABLE(1,필드起動)
PRINTL ①：이 카드의 X소재를 １개 제거하고、덱에서 "Em" 몬스터 １장을 묘지로 보내고 발동할 수 있다。
PRINTL 덱에서 "RUM" 마법 카드 １장을 패에 넣는다
CALLFORM CARD_ABLE(0,발동誘発)
PRINTL ②：상대가 카드의 효과를 발동했을 때에 이 카드의 Ｘ소재를 １개 제거하고 발동할 수 있다。
PRINTL 이 카드의 X소재를 １개 제거하고、EX덱에서 "Em그림자화가 섀도우 메이커" １장을 특수 소환한다。
PRINTL 그 후、이 카드에 X소재가 존재하지 않는 경우、자신의 묘지에서 "Em" 몬스터 １장을 특수 소환한다。


@CARDCAN_50192(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 必要カード
#DIM DYNAMIC 条件달성
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,50192)
SIF RESULT == 1
	RETURN 0

IF 종류 == EX덱起動
	SIF 발동済_1(듀얼리스트,50192)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL CARD_EX_CHECK(듀얼리스트,50192)
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件レベル = 4
	효과条件종족 = 마법사족
	효과条件重ねてＸ = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드起動
	SIF 발동済_2(듀얼리스트,50192)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_Ｘ:場所") <= -1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = RUM
	효과条件종류 = 마법全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 엔터메이지
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	RETURN 1


ELSEIF 종류 == 발동誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_Ｘ:場所") <= -1
		RETURN 0
	CALL FULLRESET
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲EX덱 = 1
	효과条件名前 = Em그림자화가 섀도우 메이커
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_50192(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 攻撃力修正
IF 종류 == EX덱起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,50192,1)
	IF RESULT == 0
		CALL RESETCONDITION_MAT
		必要素材数 = 1

		素材条件종류 = 엑시즈
		素材条件レベル = 4
		素材条件종족 = 마법사족
		重ねてＸ = 1
		TRYCALLFORM XYZSUMMON(듀얼리스트,場所)
	ENDIF

ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,50192,2,X消費1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = 엔터메이지
		효과条件종류 = モン全般
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件カテゴリ = RUM
			효과条件종류 = 마법全般
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,50192,0,X消費1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲EX덱 = 1
		효과条件名前 = Em그림자화가 섀도우 메이커
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)

		IF GETVAR(@"%듀얼리스트%_MZ_Ｘ:場所") <= -1
			CALL FULLRESET
			효과範囲묘지 = 1
			효과条件カテゴリ = 엔터메이지
			효과条件任意 = 1
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ENDIF