@CARDNAME_50193,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = Em트래피즈 포스 위치
	CASE "略称"
		RESULTS = Em트래피즈위치
	CASE "カテゴリ"
		RESULTS:0 = 엔터메이지
ENDSELECT


@CARD_50193(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 융합
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 마법사족
	CASE "レベル"
		RETURN 7
	CASE "攻撃力"
		RETURN 2400
	CASE "守備力"
		RETURN 1800
	CASE "성별"
		RETURN 牝性
	CASE "추가日"
		RETURN 250829
ENDSELECT

RETURN 0

@CARD_EXPLANATION_50193(종류)
#DIM DYNAMIC 종류

PRINTL "Em" 몬스터×２
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ①：이 카드가 몬스터 존에 존재하는 한、자신 필드의 "Em"몬스터는 파괴내성과 대상내성을 얻는다。
PRINTL ②：자신 필드에 이 카드 이외의 "Em" 몬스터가 존재하는 한、이 카드는 공격 회피를 얻는다。
CALLFORM CARD_ABLE(3,攻撃時誘発)
PRINTL ③：자신의 "Em" 몬스터가 상대 몬스터와 전투를 진행하는 공격 선언 시에발동할 수 있다。
PRINTL 그 상대 몬스터의 공격력은 ６００내리고、상대에게６００데미지를 준다。

@CARDSUMMON_50193(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要素材数 = 2
素材条件カテゴリ = 엔터메이지


IF EX参照 == 1
	CALL FUSCAN(듀얼리스트,소환カード,50193) 
	RETURN RESULT
ENDIF


@CARDCAN_50193(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,50193)
SIF RESULT == 1
	RETURN 0


IF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0	
	RETURN 1
ELSEIF 종류 == 攻撃時誘発
	SIF ゾーン != "필드" 
		RETURN 0
	TRYCALLFORM CARDNAME_{戦闘カードID(듀얼리스트)}("カテゴリ")
	SIF RESULTS != "엔터메이지"
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_50193(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 選択カード

IF 종류 == 지속효과
	CALL FULLRESET
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"破耐")
	CALL FULLRESET
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"対耐")

	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 엔터메이지
	효과条件他所 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL FULLRESET
		CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"攻避")
	ENDIF
ELSEIF 종류 == 攻撃時誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,50193)
	IF RESULT == 0
		選択カード = 戦闘場所(듀얼리스트,1)
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"自動処理",選択カード,"永攻DOWN",600)	
		CALL LIFE_CHANGE(듀얼리스트,"減少","효과",600,1)
	ENDIF
ENDIF
