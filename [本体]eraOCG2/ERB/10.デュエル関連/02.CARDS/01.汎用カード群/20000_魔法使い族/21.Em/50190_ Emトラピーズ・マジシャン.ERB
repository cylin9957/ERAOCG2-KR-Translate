@CARDNAME_50190,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = Em트래피즈 매지션
	CASE "略称"
		RESULTS = Em트래피즈
	CASE "カテゴリ"
		RESULTS:0 = 엔터메이지
ENDSELECT


@CARD_50190(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 엑시즈
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 마법사족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 2500
	CASE "守備力"
		RETURN 2000
	CASE "성별"
		RETURN 雄性
	CASE "추가日"
		RETURN 250829
ENDSELECT

RETURN 0

@CARDSUMMON_50190(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
#DIM DYNAMIC カウンタ

CALL RESETCONDITION_MAT
必要素材数 = 2
素材条件レベル = 4
素材条件종족 = 마법사족
IF EX参照 == 1
	CALL XYZCAN(듀얼리스트,소환カード,50190) 
	RETURN RESULT
ENDIF



@CARD_EXPLANATION_50190(종류)
#DIM DYNAMIC 종류

PRINTL 마법사족レベル４モンスター×２
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ①：このカードがモンスターゾーンに存在する限り、자신は戦闘・효과ダメージを受けない。
CALLFORM CARD_ABLE(2,마함起動,フリー誘発)
PRINTL ②：자신・상대のメインフェイズ１に１度、このカードのX素材を１つ取り除き、
PRINTL フィールドの他の表側攻撃표시モンスター１体を대상として발동できる。
PRINTL このターン、そのモンスターは２回攻撃でき、ターン終了時に破壊される。
CALLFORM CARD_ABLE(0,破壊→묘지 ,戦闘→묘지)
PRINTL ③：このカードが戦闘または효과で破壊された場合に발동できる。
PRINTL 덱から「Em」モンスター１体を選んで특수소환する。


@CARDCAN_50190(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 必要カード
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성
#DIMS DYNAMIC 装備者
#DIM DYNAMIC 装備先

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL 装備판정(듀얼리스트,場所,ゾーン)
装備者 = %RESULTS%
装備先 = RESULT

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,50190)
SIF RESULT == 1
	RETURN 0


IF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1

ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_Ｘ:場所") <= -1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件표시 = 表側표시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 엔터메이지
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_50190(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM 選択カード


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 지속효과
	CALL SET_PLAYER_INFO(듀얼리스트,"ダメージ無効",3)
	CALL SET_PLAYER_INFO(듀얼리스트,"효과ダメージ無効",3)
ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,50190,10,X消費1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲필드 = 1
		효과範囲상대필드 = 1
		효과条件표시 = 表側표시
		CALL CARD_GRANT_ADV(듀얼리스트,ゾーン,場所,"戦＋破壊",1)
	ENDIF
ELSEIF 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,50190,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = 엔터메이지
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF
