@CARDNAME_25235,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 요선수 반강편
	CASE "略称"
		RESULTS = 반강편
	CASE "カテゴリ"
		RESULTS:0 = 요선수
ENDSELECT

@CARD_25235(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 바람속성
	CASE "종족"
		RETURN 야수족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 800
	CASE "守備力"
		RETURN 1200
	CASE "성별"
		RETURN 雄性
	CASE "추가日"
		RETURN 260000
ENDSELECT
RETURN 0

@CARD_EXPLANATION_25235(종류)
#DIM DYNAMIC 종류

PRINTL このカード名の①②の효과はそれぞれ１ターンに１度しか使用できない。
CALLFORM CARD_ABLE(1,발동誘発)
PRINTL ①：자신フィールドに「요선수」カード以外の表側표시のカードが存在せず、상대がモンスターの효과を발동した時、
PRINTL このカードを손패から捨てて발동できる。
PRINTL その발동を無効にする。
PRINTL このターン、자신の「요선수」カードの효과の발동に対して상대は효과を발동できない。
CALLFORM CARD_ABLE(2,필드起動)
PRINTL ②：자신フィールドに他の「요선수」モンスターが存在する場合に발동できる。
PRINTL 자신は１枚ドローする。
CALLFORM CARD_ABLE(0,ターン終了時強制)
PRINTL ③：このカードを소환したターンのエンドフェイズに발동する。
PRINTL このカードを持ち主の손패に戻す。

@CARDCAN_25235(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,25235)
SIF RESULT == 1
	RETURN 0

IF 종류 == 발동誘発
	SIF 발동済_1(듀얼리스트,25235)
		RETURN 0
	SIF ゾーン != "손패"
		RETURN 0
	CALL FULLRESET
	효과条件종류 = モン全般
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件표시 = 表側표시
	효과条件固カテゴリ = 요선수
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드起動
	SIF 발동済_2(듀얼리스트,25235)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件표시 = 表側표시
	효과条件カテゴリ = 요선수
	효과条件他所 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL CARD_DRAW_CHECK
	RETURN 1
ELSEIF 종류 == ターン終了時強制
	SIF ゾーン != "필드"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_T1召種:場所") != 召種通常
		RETURN 0
	RETURN 1
ELSEIF 종류 == 短紋章起動
	RETURN 1
ENDIF


@CARDEFFECT_25235(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

#DIM DYNAMIC 選択カードID

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,25235,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"無効")
		CALL CARD_RESERV(듀얼리스트,25235,短紋章予約)
	ENDIF
ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,25235,2)
	IF RESULT == 0
		CALL CARDDRAW_BASIC(듀얼리스트,1)
	ENDIF
ELSEIF 종류 == ターン終了時強制
	CALL CARD_DISPOSE(듀얼리스트,"필드",場所,3)

ELSEIF 종류 == 短紋章起動
	CALL SET_CAT_SOGAI_INFO(듀얼리스트,"요선수")
ENDIF