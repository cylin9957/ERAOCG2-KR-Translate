@CARDNAME_91104,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 싱크로 원웨이
	CASE "略称"
		RESULTS = Syn 원웨이
	CASE "カテゴリ"
		RESULTS:0 = 싱크로
		RESULTS:1 = 파워 오브 펠로우즈
		;「정크 워리어」のカード名が記されたモンスター
ENDSELECT


@CARD_91104(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常함정
	CASE "レア度"
		RETURN RARE
	CASE "추가日"
		RETURN 250906
ENDSELECT

RETURN 0

@CARD_EXPLANATION_91104(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①의 효과는 １턴에 １번밖에 사용할 수 없다。
CALLFORM CARD_ABLE(1,마함起動)
PRINTL ①：자신의 덱 / 묘지에서 레벨１ 튜너 １장을 특수 소환할 수 있다。
PRINTL 특수 소환하지 않았을 경우、대신에 자신의 덱 / 묘지에서 레벨１ 튜너 １장을 패에 넣는다。
CALLFORM CARD_ABLE(2,묘지起動)
PRINTL ②：이 카드가 묘지에 존재하고、"정크 워리어" 또는 "파워 오브 펠로우즈" 몬스터가 자신 필드에 존재하는 경우에 발동할 수 있다。
PRINTL 이 카드를 자신 필드에 세트한다。
PRINTL 이 효과로 세트한 이 카드는 필드에서 벗어난 경우에 제외된다。


@CARDCAN_91104(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,91104)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == フリー誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF 발동済_1(듀얼리스트,91104)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") != 2
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과範囲묘지 = 1
	효과条件튜너 = 1
	효과条件レベル = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 묘지起動
	SIF 발동済_2(듀얼리스트,91104)
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件スーパー名前 = 정크 워리어
	효과条件カテゴリ = 파워 오브 펠로우즈
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_91104(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ

IF 종류 == 마함起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,91104,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과範囲덱 = 1
		효과範囲묘지 = 1
		효과条件튜너 = 1
		효과条件レベル = 1
		효과条件任意 = 1
		CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT != 1
			CALL FULLRESET
			효과範囲덱 = 1
			효과範囲묘지 = 1
			효과条件튜너 = 1
			효과条件レベル = 1
			CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ELSEIF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,91104,2)
	IF RESULT == 0
		CALL FULLRESET
		효과条件名前 = 싱크로 원웨이
		효과추가종류 = 500
		CALL CARD_SET_BASIC(듀얼리스트,"묘지",ゾーン,場所,"裏側표시")
	ENDIF
ENDIF

