@CARDNAME_25704,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 전황의 중－손모
	CASE "略称"
		RESULTS = 손모
	CASE "カテゴリ"
		RESULTS:0 = 전황
ENDSELECT


@CARD_25704(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 물속성
	CASE "종족"
		RETURN 야수족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1800
	CASE "守備力"
		RETURN 1500
	CASE "성별"
		RETURN 雄性
	CASE "レア度"
		RETURN SPR
	CASE "추가日"
		RETURN 250913
ENDSELECT

RETURN 0

@CARD_EXPLANATION_25704(종류)
#DIM DYNAMIC 종류

PRINTL このカード名の②③の효과はそれぞれ１ターンに１度しか使用できない。
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ①：자신フィールドに他の「전황」モンスターが存在する限り、상대はこのカードを攻撃대상に選択できない。
CALLFORM CARD_ABLE(1,필드起動)
PRINTL ②：자신の손패・フィールドからカード１枚を묘지へ送って발동できる。
PRINTL 덱から「전황의 중－손모」以外の「전황」モンスター１体を손패に加える。
CALLFORM CARD_ABLE(2,효과後誘発)
PRINTL ③：「전황의 중－손모」以外の자신の「전황」モンスターの효과が발동した場合、상대フィールドのモンスター１体を대상として발동できる。
PRINTL そのモンスターを持ち主の손패に戻す。



@CARDCAN_25704(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,25704)
SIF RESULT == 1
	RETURN 0

IF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件カテゴリ = 전황
	효과条件他所 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 필드起動
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_1(듀얼리스트,25704)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件他所 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件종류 = モン全般
	효과条件カテゴリ = 전황
	효과条件固名前 = 전황의 중－손모
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 효과後誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_2(듀얼리스트,25704)
		RETURN 0
	CALL FULLRESET
	효과条件종류 = モン全般
	효과条件カテゴリ = 전황
	효과条件固名前 = 전황의 중－손모
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"효과발동")
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ENDIF

@CARDEFFECT_25704(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

IF 종류 == 지속효과
	CALL FULLRESET
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"攻避")

ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,25704,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲필드 = 1
		효과範囲마함 = 1
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과条件종류 = モン全般
			효과条件カテゴリ = 전황
			효과条件固名前 = 전황의 중－손모
			CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
		ENDIF
	ENDIF

ELSEIF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,25704,2)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드単体",場所,"손패")
	ENDIF

ENDIF
