@CARDNAME_52555,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 전황맹장－쌍룡
	CASE "略称"
		RESULTS = 쌍룡
	CASE "カテゴリ"
		RESULTS:0 = 전황
		RESULTS:1 = 전황의덕－유현
ENDSELECT


@CARD_52555(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 링크
	CASE "속성"
		RETURN 바람속성
	CASE "종족"
		RETURN 야수족
	CASE "レベル"
		RETURN 2
	CASE "攻撃力"
		RETURN 1100
	CASE "守備力"
		RETURN 1600
	CASE "성별"
		RETURN 雄性
	CASE "レア度"
		RETURN ULR
	CASE "추가日"
		RETURN 250913
ENDSELECT

RETURN 0

@CARD_EXPLANATION_52555(종류)
#DIM DYNAMIC 종류

PRINTL 바람속성の「전황」モンスターを含む야수족モンスター２体
PRINTL このカードは「전황의덕－유현」としても扱う。
PRINTL このカード名の①③の효과はそれぞれ１ターンに１度しか使用できない。
CALLFORM CARD_ABLE(1,EX소환時)
PRINTL ①：このカードが링크소환に成功した場合に발동できる。
PRINTL 덱から「전황」カード１枚を손패に加える。
CALLFORM CARD_ABLE(0,攻守変動)
PRINTL ②：자신フィールドの「전황」モンスターの攻撃力・守備力は５００アップする。
CALLFORM CARD_ABLE(2,필드起動,フリー誘発)
PRINTL ③：자신の손패・フィールドからカード１枚を묘지へ送り、상대フィールドの表側표시のカード１枚を대상として발동できる。
PRINTL そのカードを持ち主の손패に戻す。この효과は상대ターンでも발동できる。



@CARDSUMMON_52555(듀얼리스트,소환カード,EX参照)
#DIMS DYNAMIC 듀얼리스트
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 소환カード
#DIM DYNAMIC EX参照
;素材条件

CALL RESETCONDITION_MAT
必要링크数 = 2
素材条件종족 = 야수족

IF EX参照 == 0 || EX参照 == 1
	素材条件속성 = 바람속성
	素材条件カテゴリ = 전황
ELSEIF EX参照 >= 10
	素材条件종류 = モン全般
ENDIF


IF EX参照 == 1
	CALL LINCAN(듀얼리스트,소환カード,52555) 
	RETURN RESULT
ENDIF



@CARDCAN_52555(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,52555)
SIF RESULT == 1
	RETURN 0

IF 종류 == EX소환時
	SIF 발동済_1(듀얼리스트,52555)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 전황
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 攻守変動
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1

ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 발동済_2(듀얼리스트,52555)
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과範囲필드 = 1
	효과範囲마함 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과範囲상대마함 = 1
	효과条件표시 = 表側표시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ENDIF

@CARDEFFECT_52555(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 攻撃力修正

IF 종류 == EX소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,52555,1)
	IF RESULT == 0
		CALL FULLRESET
		효과条件カテゴリ = 전황
		CALL CARD_ADD_BASIC(듀얼리스트,"덱",ゾーン,場所)
	ENDIF


ELSEIF 종류 == 攻守変動
	CALL FULLRESET
	효과条件カテゴリ = 전황
	CALL STATCHANGE_BASIC(듀얼리스트,"필드",場所,500,500)

ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,52555,2)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과範囲필드 = 1
		효과範囲마함 = 1
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲상대필드 = 1
			효과範囲상대마함 = 1
			효과条件표시 = 表側표시
			CALL CARD_REMOVAL_ADV(듀얼리스트,"単体除去",場所,"손패")
		ENDIF
	ENDIF

ENDIF

