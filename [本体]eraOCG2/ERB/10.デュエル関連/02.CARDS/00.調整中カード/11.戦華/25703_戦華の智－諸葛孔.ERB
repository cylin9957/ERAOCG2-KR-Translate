@CARDNAME_25703,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 전황의 지－제갈공
	CASE "略称"
		RESULTS = 제갈공
	CASE "カテゴリ"
		RESULTS:0 = 전황
ENDSELECT


@CARD_25703(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 바람속성
	CASE "종족"
		RETURN 야수족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1000
	CASE "守備力"
		RETURN 2000
	CASE "성별"
		RETURN 雄性
	CASE "レア度"
		RETURN RARE
	CASE "추가日"
		RETURN 250913
ENDSELECT

RETURN 0

@CARD_EXPLANATION_25703(종류)
#DIM DYNAMIC 종류

PRINTL このカード名の①の효과は１ターンに１度しか使用できない。
CALLFORM CARD_ABLE(1,효과後誘発)
PRINTL ①：このカードがドロー以外の方法で손패に加わった場合に발동できる。このカードを특수소환する。
CALLFORM CARD_ABLE(2,발동誘発)
CALLFORM CARD_ABLE(3,발동誘発)
PRINTL ①：상대が효과を발동した時に발동できる。
PRINTL 자신フィールドの表側표시の「전황」지속마법・지속함정カード１枚を묘지へ送ってその발동を無効にする。
PRINTL 자신フィールドに「전황의덕－유현」が存在する場合、さらにそのカードを破壊する。
PRINTL （このカード名のこの효과はモンスターの효과、마법・함정カードの효과に対してそれぞれ１ターンに１度しか발동できない）



@CARDCAN_25703(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,25703)
SIF RESULT == 1
	RETURN 0

IF 종류 == 효과後誘発
	SIF ゾーン != "손패"
		RETURN 0
	SIF 발동済_1(듀얼리스트,25703)
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,25703,"손패")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과条件名前 = 전황의 지－제갈공
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"손패취득")
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 발동誘発
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲마함 = 1
	효과条件종류:0 = 지속마법
	효과条件종류:1 = 지속함정
	효과条件カテゴリ = 전황
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	CALL FULLRESET
	효과条件종류 = モン全般
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	IF RESULT == 1
		SIF 발동済_2(듀얼리스트,25703)
			RETURN 0
		RETURN 1
	ENDIF

	CALL FULLRESET
	효과条件종류 = 마함全般
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	IF RESULT == 1
		SIF 발동済_3(듀얼리스트,25703)
			RETURN 0
		RETURN 1
	ENDIF

ENDIF

@CARDEFFECT_25703(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

IF 종류 == 효과後誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,25703,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트, "손패", 場所)
	ENDIF

ELSEIF 종류 == 발동誘発
	CALL FULLRESET
	효과条件종류 = モン全般
	CALL CARD_CHECK_START_DATA(듀얼리스트,1,2)
	IF RESULT == 1
		CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,25703,2)
		IF RESULT == 0
			CALL FULLRESET
			효과範囲마함 = 1
			효과条件종류:0 = 지속마법
			효과条件종류:1 = 지속함정
			효과条件カテゴリ = 전황
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
			IF RESULT == 1
				CALL FULLRESET
				효과範囲필드 = 1
				효과条件カテゴリ = 전황의덕－유현
				CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
				IF RESULT == 0
					CALL FULLRESET
					CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"無効")
					RETURN 1
				ELSE
					CALL FULLRESET
					CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"묘지")
					RETURN 1
				ENDIF
			ENDIF
			RETURN 0
		ENDIF
	ENDIF

	CALL FULLRESET
	효과条件종류 = 마함全般
	CALL CARD_CHECK_START_DATA(듀얼리스트,1,2)
	IF RESULT == 1
		CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,25703,3)
		IF RESULT == 0
			CALL FULLRESET
			효과範囲마함 = 1
			효과条件종류:0 = 지속마법
			효과条件종류:1 = 지속함정
			효과条件カテゴリ = 전황
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
			IF RESULT == 1
				CALL FULLRESET
				효과範囲필드 = 1
				효과条件カテゴリ = 전황의덕－유현
				CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
				IF RESULT == 0
					CALL FULLRESET
					CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"無効")
					RETURN 1
				ELSE
					CALL FULLRESET
					CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"묘지")
					RETURN 1
				ENDIF
			ENDIF
			RETURN 0
		ENDIF
	ENDIF

ENDIF
