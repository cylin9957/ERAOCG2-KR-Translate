@CARDNAME_73220,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 행운 구름
	CASE "略称"
		RESULTS = 행운 구름
	CASE "カテゴリ"
		RESULTS:0 = 운마물
ENDSELECT


@CARD_73220(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
	CASE "추가日"
		RETURN 250917
ENDSELECT

RETURN 0

