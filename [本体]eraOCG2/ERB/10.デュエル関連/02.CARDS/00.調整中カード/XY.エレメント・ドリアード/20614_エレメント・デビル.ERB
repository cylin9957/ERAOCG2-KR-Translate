@CARDNAME_20614,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 엘리멘트 데블
	CASE "略称"
		RESULTS = E 데블
	CASE "カテゴリ"
		RESULTS:0 = 엘리멘트
ENDSELECT


@CARD_20614(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 악마족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1500
	CASE "守備力"
		RETURN 1000
	CASE "추가日"
		RETURN 250915
ENDSELECT

RETURN 0

@CARD_EXPLANATION_20614(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①의 효과는 １턴에 １번밖에 사용할 수 없다。
CALL CARD_ABLE(1,손패起動,묘지起動)
PRINTL ①：패에서 다른 카드를 １장 버리고 발동할 수 있다。
PRINTL 패 및 묘지에서、이 카드를 포함하는 "엘리멘트" 몬스터를１장씩 특수 소환한다。
PRINTL ②：이 카드는 서로의 필드에 특정 속성을 가진 몬스터가 존재하는 경우、이하의 효과를 얻는다。
CALLFORM CARD_ABLE(10,攻撃後誘発)
PRINTL ●바람속성：이 카드의 공격력은 ３００、수비력은 ２００오르고、
PRINTL １턴에１번、이 카드의 공격으로 상대 몬스터를 파괴한 경우、이 카드는 이 턴 １번 더 공격할 수 있다。
CALL CARD_ABLE(0,지속효과)
PRINTL ●땅속성：이 카드의 수비력은 ５００오르고、공격저지를 얻는다。
PRINTL 

@CARDCAN_20614(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,20614)
SIF RESULT == 1
	RETURN 0

IF 종류 == 묘지起動
	SIF 발동済_1(듀얼리스트,20614)
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT <= 1
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件カテゴリ = 엘리멘트
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,20614,"묘지")
	SIF RESULT == 1
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,,"손패")
	SIF RESULT == 1
		RETURN 0
	SIF モンスター数(듀얼리스트) >= 4
		RETURN 0
	RETURN 1

ELSEIF 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,20614)
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT <= 1
		RETURN 0
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件カテゴリ = 엘리멘트
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,20614,"손패")
	SIF RESULT == 1
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,,"묘지")
	SIF RESULT == 1
		RETURN 0
	SIF モンスター数(듀얼리스트) >= 4
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件속성:0 = 땅속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 攻撃後誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	SIF 戦闘場所(듀얼리스트) != 場所
		RETURN 0
	SIF 直前戦闘破壊ID(듀얼리스트,1) == 0
		RETURN 0

	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件속성:0 = 바람속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 攻守変動
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件속성:0 = 바람속성
	효과条件속성:1 = 땅속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_20614(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード


IF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,20614,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲손패 = 1
			효과条件종류 = モン全般
			효과条件カテゴリ = 엘리멘트
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
			IF RESULT == 1
				CALL FULLRESET
				효과範囲묘지 = 1
				효과条件名前 = 엘리멘트 데블
				CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
			ENDIF
		ENDIF
	ENDIF

ELSEIF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,20614,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		효과条件他所 = 1
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲손패 = 1
			효과条件名前 = 엘리멘트 데블
			CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
			IF RESULT == 1
				CALL FULLRESET
				효과範囲묘지 = 1
				효과条件종류 = モン全般
				효과条件カテゴリ = 엘리멘트
				CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
			ENDIF
		ENDIF
	ENDIF

ELSEIF 종류 == 지속효과
		CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"戦阻")

ELSEIF 종류 == 攻撃後誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF 효과발동済(듀얼리스트,場所,ゾーン)
		RETURN 0
	SIF 戦闘場所(듀얼리스트) != 場所
		RETURN 0
	SIF 直前戦闘破壊ID(듀얼리스트,1) == 0
		RETURN 0

	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件속성:0 = 바람속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 攻守変動
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件속성:0 = 땅속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL STATCHANGE_BASIC(듀얼리스트,"自身",場所,0,500)
	ENDIF

	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件속성:0 = 바람속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL STATCHANGE_BASIC(듀얼리스트,"自身",場所,300,200)
	ENDIF
ENDIF
