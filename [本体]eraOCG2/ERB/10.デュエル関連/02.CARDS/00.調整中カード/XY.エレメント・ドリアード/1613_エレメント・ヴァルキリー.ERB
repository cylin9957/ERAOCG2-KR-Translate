@CARDNAME_1613,参照先
;ココで지정카드의 名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 엘리멘트 발키리
	CASE "略称"
		RESULTS = E 발키리
	CASE "カテゴリ"
		RESULTS:0 = 엘리멘트
ENDSELECT


@CARD_1613(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 천사족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1500
	CASE "守備力"
		RETURN 1000
	CASE "성별"
		RETURN 牝性
ENDSELECT

RETURN 0

@CARD_EXPLANATION_1613(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①의 효과는 １턴에 １번밖에 사용할 수 없다。
CALL CARD_ABLE(0,소환時 ,특수소환時)
PRINTL ①: 이 카드가 소환 / 특수 소환에 성공한 경우, 패를 1장 버리고 발동할 수 있다.
PRINTL 덱에서 "엘리멘트" 몬스터 1장과, "엘리멘트" 마법 / 함정 카드 1장을 패에 넣는다.
PRINTL ②: 이 카드는 서로의 필드에 특정 속성을 가진 몬스터가 존재할 경우, 이하의 효과를 얻는다.
CALL CARD_ABLE(0,지속효과)
PRINTL ●화염속성: 이 카드의 공격력은 500 포인트 올리고, 관통 공격을 얻는다.
CALLFORM CARD_ABLE(0,攻撃後強制誘発)
PRINTL ●물속성: 이 카드의 공격력은 200, 수비력은 300 올리고,
PRINTL 이 카드의 전투로 상대 몬스터를 파괴한 경우, 카드를 1장 뽑는다.

@CARDCAN_1613(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,1613)
SIF RESULT == 1
	RETURN 0

IF 종류 == 소환時 || 종류 == 특수소환時
	SIF 발동済_1(듀얼리스트,1613)
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件종류 = モン全般
	효과条件カテゴリ = 엘리멘트
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件종류 = 마함全般
	효과条件カテゴリ = 엘리멘트
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	SIF 손패数(듀얼리스트) == 0
		RETURN 0
	RETURN 1

ELSEIF 종류 == 攻撃後強制誘発
	SIF ゾーン != "필드"
		RETURN 0
	SIF 戦闘場所(듀얼리스트) != 場所
		RETURN 0
	SIF 直前戦闘破壊ID(듀얼리스트,1) == 0
		RETURN 0	

	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件속성 = 물속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 攻守変動
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件속성:0 = 화염속성
	효과条件속성:1 = 물속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件속성:0 = 화염속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_1613(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード



IF 종류 == 소환時 || 종류 == 특수소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,1613,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"손패",ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			효과範囲덱 = 1
			효과条件종류 = モン全般
			효과条件カテゴリ = 엘리멘트
			CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)

			CALL FULLRESET
			효과範囲덱 = 1
			효과条件종류 = 마함全般
			효과条件カテゴリ = 엘리멘트
			CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF
ELSEIF 종류 == 攻撃後強制誘発
	CALL CARDDRAW_BASIC(듀얼리스트,1)

ELSEIF 종류 == 攻守変動
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件속성:0 = 화염속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL STATCHANGE_BASIC(듀얼리스트,"自身",場所,500)
	ENDIF
CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件속성:0 = 물속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL STATCHANGE_BASIC(듀얼리스트,"自身",場所,200,300)
	ENDIF

ELSEIF 종류 == 지속효과
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"貫通")	
ENDIF
