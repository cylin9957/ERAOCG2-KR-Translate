@CARDNAME_20613,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 엘리멘트 솔저
	CASE "略称"
		RESULTS = E 솔저
	CASE "カテゴリ"
		RESULTS:0 = 엘리멘트
ENDSELECT


@CARD_20613(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 전사족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1500
	CASE "守備力"
		RETURN 1000
	CASE "성별"
		RETURN 雄性
	CASE "추가日"
		RETURN 250915
ENDSELECT

RETURN 0

