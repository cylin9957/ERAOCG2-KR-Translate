@CARDNAME_71700,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 엘리멘트 트레일
	CASE "略称"
		RESULTS = 정령이이끄는궤적
	CASE "カテゴリ"
		RESULTS:0 = 엘리멘트
		RESULTS:1 = 정령
		RESULTS:3 = 게임 오리지널
ENDSELECT


@CARD_71700(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN フィールド
	CASE "추가日"
		RETURN 250915
ENDSELECT

RETURN 0

@CARD_EXPLANATION_71700(종류)
#DIM DYNAMIC 종류

PRINTL 자신フィールドは「정령達の導く軌跡」となる。
PRINTL 「정령達の導く軌跡」の③の효과は１ターンに１度しか使用できない。
CALLFORM CARD_ABLE(0,攻守変動)
PRINTL ①：자신フィールドのモンスターの攻撃力・守備力は、
PRINTL 자신フィールドのモンスターの속성の종류×１００、자신フィールドの「정령」モンスターの종류の数×３００アップする。
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ②：자신のフィールド・묘지に存在する「엘리멘트」モンスターの종류に応じて、以下の효과を得る。
PRINTL ●２종류以上：자신フィールドの「엘리멘트」モンスターは대상耐性を得る。
PRINTL ●４종류以上：자신フィールドの「엘리멘트」モンスターは破壊耐性を得る。
PRINTL ●６종류以上：자신フィールドの「엘리멘트」마법・함정カードは대상耐性・破壊耐性を得る。
CALL CARD_ABLE(1,발동誘発)
PRINTL ③：상대がモンスターの효과・마법・함정カードを발동した時、자신・상대フィールドのモンスターの속성の종류が６종류以上の場合に발동できる。
PRINTL 덱・ＥＸ덱から「엘리멘트」モンスター１体を묘지に送り、その발동を無効にし破壊する。

@CARDCAN_71700(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 条件달성
#DIM DYNAMIC カウンタ
#DIMS DYNAMIC 対面者

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,71700)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	IF 종류 == 마함起動
		SIF GETVAR(@"%듀얼리스트%_魔_表:場所") == 1
			RETURN 0
	ENDIF
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	RETURN 1

ELSEIF 종류 == 발동誘発
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") >= 2
		RETURN 0

	SIF 발동済_1(듀얼리스트,71700)
		RETURN 0

	CALL FULLRESET
	CALL CARD_CHECK_START_DATA(듀얼리스트,1)
	SIF RESULT != 1
		RETURN 0

	CALL FULLRESET
	효과範囲덱 = 1
	효과範囲EX덱 = 1
	효과条件カテゴリ = 엘리멘트
	효과条件종류 = モン全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0

	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件속성 = 빛속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		カウンタ += 1

	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件속성 = 바람속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		カウンタ += 1

	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件속성 = 물속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		カウンタ += 1

	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件속성 = 땅속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		カウンタ += 1

	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件속성 = 화염속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		カウンタ += 1

	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件속성 = 어둠속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		カウンタ += 1

	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件속성 = 신속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		カウンタ += 1

	SIF カウンタ < 6
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과	|| 종류 == 攻守変動
	SIF ゾーン != "마함"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_魔_表:場所") >= 2
		RETURN 0
	RETURN 1

ENDIF

@CARDEFFECT_71700(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 攻撃力修正
#DIMS DYNAMIC 対面者


CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 || 종류 == 손패起動
	;カード발동＆설치まで
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,71700)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,마함行き)
	ENDIF

ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,71700,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲덱 = 1
		효과範囲EX덱 = 1
		효과条件カテゴリ = 엘리멘트
		효과条件종류 = モン全般
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"破壊")
		ENDIF
	ENDIF

ELSEIF 종류 == 攻守変動
	;攻撃UP
	CALL FULLRESET
	CALL FIELD_INFO_COUNT(듀얼리스트,"属")
	攻撃力修正 = RESULT * 100
	CALL FULLRESET
	CALL STATCHANGE_BASIC(듀얼리스트,"필드",場所,攻撃力修正,攻撃力修正)

	CALL FULLRESET
	효과範囲필드 = 1
	효과条件종류 = モン全般
	효과条件カテゴリ = 정령
	효과条件固名前起動 = 1
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	攻撃力修正 = RESULT * 300
	CALL FULLRESET
	CALL STATCHANGE_BASIC(듀얼리스트,"필드",場所,攻撃力修正,攻撃力修正)

ELSEIF 종류 == 지속효과
	CALL SET_FIELD_MAGIC_INFO(듀얼리스트, "정령達の導く軌跡")

	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲묘지 = 1
	효과条件カテゴリ = 엘리멘트
	효과条件固名前起動 = 1
	효과条件CNT = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	カウンタ = RESULT
	IF カウンタ >= 2
		CALL FULLRESET
		효과条件カテゴリ = 엘리멘트
	    CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"対耐")
	ENDIF
	IF カウンタ >= 4
		CALL FULLRESET
		효과条件カテゴリ = 엘리멘트
	    CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"破耐")
	ENDIF
	IF カウンタ >= 6
		CALL FULLRESET
		효과条件カテゴリ = 엘리멘트
	    CALL STATCHANGE_EFFECT(듀얼리스트,"마함",場所,"破耐")
	    CALL STATCHANGE_EFFECT(듀얼리스트,"마함",場所,"対耐")
	ENDIF
ENDIF