@CARDNAME_20612,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 엘리멘트 사우루스
	CASE "略称"
		RESULTS = E 사우루스
	CASE "カテゴリ"
		RESULTS:0 = 엘리멘트
ENDSELECT


@CARD_20612(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 비늘야수족
	CASE "レベル"
		RETURN 4
	CASE "攻撃力"
		RETURN 1500
	CASE "守備力"
		RETURN 1000
	CASE "추가日"
		RETURN 250915
ENDSELECT

RETURN 0

@CARD_EXPLANATION_20612(종류)
#DIM DYNAMIC 종류

PRINTL このカード名の①の효과は１ターンに１度しか使用できない。
CALL CARD_ABLE(1,묘지起動)
PRINTL ①：손패から他のカードを１枚捨てて발동できる。
PRINTL 묘지のこのカードを특수소환し、덱から「엘리멘트」モンスター１体を손패に加える。
CALL CARD_ABLE(0,지속효과)
PRINTL ②：このカードはお互いのフィールドに特定の속성を持つモンスターが存在する場合、以下の효과を得る。
PRINTL ●화염속성：このカードの攻撃力は５００アップし、貫通攻撃を得る。
PRINTL ●땅속성：このカードの守備力は５００アップし、攻撃阻害を得る。
PRINTL 

@CARDCAN_20612(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 条件달성

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,20612)
SIF RESULT == 1
	RETURN 0

IF 종류 == 묘지起動
	SIF 발동済_1(듀얼리스트,20612)
		RETURN 0
	SIF 손패数(듀얼리스트) == 0
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,20612,"묘지")
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件종류 = モン全般
	효과条件カテゴリ = 엘리멘트
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件속성:0 = 화염속성
	효과条件속성:1 = 땅속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 攻守変動
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件속성:0 = 화염속성
	효과条件속성:1 = 땅속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ENDIF


@CARDEFFECT_20612(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIMS DYNAMIC 対面者
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード


IF 종류 == 묘지起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,20612,1)
	IF RESULT == 0
		CALL FULLRESET
		효과範囲손패 = 1
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			CALL CARD_SP_SUMMON(듀얼리스트,ゾーン,場所)

			CALL FULLRESET
			효과範囲덱 = 1
			효과条件종류 = モン全般
			효과条件カテゴリ = 엘리멘트
			CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
		ENDIF
	ENDIF

ELSEIF 종류 == 지속효과
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件속성:0 = 화염속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"貫通")
	ENDIF

	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件속성:0 = 땅속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"戦阻")
	ENDIF

ELSEIF 종류 == 攻守変動
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件속성:0 = 땅속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL STATCHANGE_BASIC(듀얼리스트,"自身",場所,0,500)
	ENDIF

	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件속성:0 = 화염속성
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL STATCHANGE_BASIC(듀얼리스트,"自身",場所,500,0)
	ENDIF
ENDIF
