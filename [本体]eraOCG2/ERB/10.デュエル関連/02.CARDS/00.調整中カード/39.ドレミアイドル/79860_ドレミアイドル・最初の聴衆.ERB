@CARDNAME_79860,参照先
;ココで지정卡片の名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 도레미아이돌 최초의 청중
	CASE "略称"
		RESULTS = 최초청중
	CASE "カテゴリ"
		RESULTS:0 = 도레미코드
		RESULTS:1 = 도레미아이돌
		RESULTS:2 = E.R.A
ENDSELECT


@CARD_79860(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 지속마법
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 천사족
	CASE "レベル"
		RETURN 1
	CASE "攻撃力"
		RETURN 700
	CASE "守備力"
		RETURN 700
	CASE "分割"
		RETURN 通常モン
	CASE "추가日"
		RETURN 250910
ENDSELECT

RETURN 0

@CARD_EXPLANATION_79860(종류)
#DIM DYNAMIC 종류

CALL CARD_ABLE(1,마함起動,손패起動)
PRINTL ①：상대 필드에 "◯◯최애" 몬스터가 존재하지 않는 경우에 발동할 수 있다、그 후、다음 효과를 적용한다。
PRINTL ●이 카드를 일반 몬스터(천사족 / 땅 / 레벨1 / 공700 / 수700)로서 상대 몬스터 존에 공격 표시로 특수 소환한다。
PRINTL ●덱에서 레벨 3 이하의 "도레미아이돌" P몬스터 1체를 선택하여 자신 마법 / 함정 존에 앞면 표시로 놓는다。
PRINTL ●이 카드를 이 효과로 마법 / 함정 존에 놓인 "도레미아이돌" 카드의 "◯◯최애"로 취급한다。



@CARDCAN_79860(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,79860)
SIF RESULT == 1
	RETURN 0


IF 종류 == 마함起動 || 종류 == 손패起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF 종류 == 손패起動 && 마함数(듀얼리스트) >= 5
		RETURN 0
	SIF モンスター数(対面者) >= 5
		RETURN 0
	CALL FULLRESET
	효과範囲상대필드 = 1
	효과条件カテゴリ:0 = 큐티아최애
	효과条件カテゴリ:1 = 드리미아최애
	효과条件カテゴリ:2 = 엘리티아최애
	효과条件カテゴリ:3 = 팬시아최애
	효과条件カテゴリ:4 = 그레이시아최애
	효과条件カテゴリ:5 = 엔제리아최애
	효과条件カテゴリ:6 = 뷰티아최애
	효과条件カテゴリ:7 = 쿨리아최애
	효과条件カテゴリ:8 = 뮤제시아최애
	효과条件カテゴリ:9 = 도레미 하코오시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 도레미아이돌
	CALL 효과条件レベル_ADV(3, "以下")
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 永紋章起動
	RETURN 1
	
ENDIF



@CARDEFFECT_79860(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択場所
#DIM DYNAMIC 조교대상

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動 || 종류 == 손패起動
	;卡片发动確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,79860,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트,ゾーン,場所,"攻撃표시",1)
		選択場所 = カード소환先
		
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = 도레미아이돌
		효과条件종류 = モン全般
		CALL 효과条件レベル_ADV(3, "以下")
		CALL CARD_SET_ADV(듀얼리스트,ゾーン,場所,"表側표시")
		IF RESULT != 9999
			TRYCALLFORM CARD_{RESULT:1},"レベル"
			SELECTCASE RESULT
				CASE 1
					조교대상 = NO_TO_CHARANUM(2086)
				CASE 2
					조교대상 = NO_TO_CHARANUM(2087)
				CASE 3
					조교대상 = NO_TO_CHARANUM(2088)
			ENDSELECT
			CALL Doremi_fans(듀얼리스트,選択場所,조교대상,"最初聴衆")
		ENDIF	
	ENDIF

ELSEIF 종류 == 永紋章起動

	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件名前 = 신참 도레미아이돌 큐티아
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL FULLRESET
		효과条件カテゴリ:0 = 큐티아최애
		효과条件カテゴリ:1 = 도레미 하코오시
		CALL STATCHANGE_EFFECT(듀얼리스트,"상대필드",場所,"無効")
		CALL STATCHANGE_EFFECT(듀얼리스트,"상대필드",場所,"戦無")
	ENDIF
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件名前 = 신참 도레미아이돌 드리미아
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL FULLRESET
		효과条件カテゴリ:0 = 드리미아최애
		효과条件カテゴリ:1 = 도레미 하코오시
		CALL STATCHANGE_EFFECT(듀얼리스트,"상대필드",場所,"無効")
		CALL STATCHANGE_EFFECT(듀얼리스트,"상대필드",場所,"戦無")
	ENDIF
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
	효과条件名前 = 신참 도레미아이돌 엘리티아
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL FULLRESET
		효과条件カテゴリ:0 = 엘리티아최애
		효과条件カテゴリ:1 = 도레미 하코오시
		CALL STATCHANGE_EFFECT(듀얼리스트,"상대필드",場所,"無効")
		CALL STATCHANGE_EFFECT(듀얼리스트,"상대필드",場所,"戦無")
	ENDIF
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲마함 = 1
