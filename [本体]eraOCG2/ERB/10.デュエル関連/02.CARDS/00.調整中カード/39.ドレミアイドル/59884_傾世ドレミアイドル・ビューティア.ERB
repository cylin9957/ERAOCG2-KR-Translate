@CARDEFFECT_59884(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択場所
#DIM DYNAMIC レベル
#DIM DYNAMIC 조교대상
#DIM DYNAMIC 実行役대상

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 특수소환時 || 종류 == EX소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59884,1)
	IF RESULT == 0
		CALL FULLRESET
		효과条件표시 = 表側표시
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"상대필드",ゾーン,場所)
		選択場所 = RESULT
		조교대상 = NO_TO_CHARANUM(2092)
		CALL Doremi_fans(対面者,選択場所,조교대상,"登場")
	ENDIF
	
ELSEIF 종류 == 발동誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59884,2)
	IF RESULT == 0
		カウンタ = 0
		CALL FULLRESET
		효과範囲EX덱 = 1
		효과条件カテゴリ = 도레미코드
		효과条件종류 = EX以外全般
		CALL 효과条件レベル_ADV(3, "以下")
		효과条件行先플래그 = 덱行き
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"EX덱",ゾーン,場所)
		SIF RESULT != 9999
			カウンタ++
		CALL FULLRESET
		효과範囲EX덱 = 1
		효과条件カテゴリ = 도레미코드
		효과条件종류 = EX以外全般
		CALL 효과条件レベル_ADV(4, "以上")
		효과条件行先플래그 = 덱行き
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"EX덱",ゾーン,場所)
		SIF RESULT != 9999
			カウンタ++	
		IF カウンタ == 2
			CALL FULLRESET
			CALL CARD_REMOVAL_BASIC(듀얼리스트,"발동無効",場所,"無効",59884)
		ENDIF

		TRYCALLFORM CARD_{効後_효과ID:(処理中番号 - 1)},"종류"
		IF RESULT <= 10
			選択場所 = 効後_設場所:(処理中番号 - 1) 
			CALL FULLRESET
			효과추가종류 = 21	;素材불가
			CALL CARD_SP_SUMMON(対面者,"묘지",選択場所)
			CALL FULLRESET
			選択場所 = カード소환先
			조교대상 = NO_TO_CHARANUM(2092)
			CALL Doremi_fans(対面者,選択場所,조교대상,"無効")
		ENDIF
	ENDIF

ELSEIF 종류 == ターン開始時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59884,3)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所,EX덱行き)
		
		CALL FULLRESET
		효과範囲묘지 = 1
		효과範囲제외 = 1
		효과条件カテゴリ = 도레미코드
		효과条件複数 = 5
		효과条件行先플래그 = 덱行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		SIF RESULT == 1
			CALL CARDDRAW_BASIC(듀얼리스트,2)
		
		조교대상 = NO_TO_CHARANUM(2092)
		CALL Doremi_business(対面者,조교대상)
	ENDIF

ELSEIF 종류 == 攻撃時誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59884,4)
	IF RESULT == 0
		CALL FULLRESET
		選択場所 = 戦闘場所(듀얼리스트,1)
		조교대상 = NO_TO_CHARANUM(2092)
		CALL Doremi_fans(対面者,選択場所,조교대상,"汎用")
		CALL EVADE_ATTACK(듀얼리스트)
	ENDIF

ELSEIF 종류 == ターン終了時強制
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,59884)
	IF RESULT == 0
		FOR カウンタ,0,5
			SIF GETVAR(@"%対面者%_필드:カウンタ") == -1
			CONTINUE
			選択場所 = カウンタ
		NEXT
		CALL Doremi_FansAttack(듀얼리스트,場所,選択場所)
	ENDIF
ENDIF