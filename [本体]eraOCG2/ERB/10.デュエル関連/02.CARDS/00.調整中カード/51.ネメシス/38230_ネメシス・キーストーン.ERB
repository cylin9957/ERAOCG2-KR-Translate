@CARDNAME_38230,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 네메시스 키스톤
	CASE "略称"
		RESULTS = Nm키스톤
	CASE "カテゴリ"
		RESULTS:0 = 네메시스
ENDSELECT

@CARD_38230(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 효과モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 원소정령족
	CASE "レベル"
		RETURN 1
	CASE "攻撃力"
		RETURN 700
	CASE "守備力"
		RETURN 0
	CASE "성별"
		RETURN 無性
	CASE "추가日"
		RETURN 250814
ENDSELECT
RETURN 0

