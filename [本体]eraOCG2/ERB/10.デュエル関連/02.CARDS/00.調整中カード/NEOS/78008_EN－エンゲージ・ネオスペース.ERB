@CARDNAME_78008,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = EN－인게이지 네오스페이스
	CASE "略称"
		RESULTS = EN인게이지
	CASE "カテゴリ"
		RESULTS:0 = 네오스
		RESULTS:1 = E•HERO
ENDSELECT


@CARD_78008(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 通常마법
	CASE "レア度"
		RETURN SPR
ENDSELECT

RETURN 0

@CARD_EXPLANATION_78008(종류)
#DIM DYNAMIC 종류

PRINTL このカード名のカードは１ターンに１枚しか발동できず、
PRINTL このカードを발동するターン、자신は융합モンスターしかEX덱から특수소환できない。
CALL CARD_ABLE(1,마함起動 ,손패起動)
PRINTL ①：손패及び덱からそれぞれ１体ずつ、「N」モンスター１体と「E•HERO」モンスター１体を묘지へ送る。
PRINTL 덱から「N」モンスターまたはレベル５以上の「E•HERO」モンスター１体を특수소환し、
PRINTL 자신の덱・묘지から「융합」１枚を選んで손패に加える。
PRINTL この효과で특수소환したモンスターが「엘리멘틀 히어로 네오스」の場合、その攻撃力は１０００アップする。



@CARDCAN_78008(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ


CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,78008)
SIF RESULT == 1
	RETURN 0

IF 종류 == 마함起動 || 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,78008)
		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件カテゴリ = E•HERO
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = 네오 스페이시언
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
	ENDIF
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件カテゴリ = 네오 스페이시언
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	IF RESULT == 1
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件カテゴリ = E•HERO
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
	ENDIF
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件スーパーカテゴリ = 네오 스페이시언
	CALL 효과条件レベル_ADV(5, "以上")
	효과条件カテゴリ = E•HERO
	효과条件소환可 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과範囲묘지 = 1
	효과条件名前 = 융합
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL FULLRESET
	효과条件종류 = 융합以外
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"EX履歴")
	SIF RESULT != 0
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_78008(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カード_ID

IF 종류 == 마함起動 || 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,78008,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		효과条件カテゴリ:0 = E•HERO
		효과条件カテゴリ:1 = 네오 스페이시언
		효과条件종류 = モン全般
		CALL CARD_ADD_GY_BASIC(듀얼리스트,"손패",ゾーン,場所)
		IF RESULT == 1
			選択カード_ID = RESULT:1
			TRYCALLFORM CARDNAME_{選択カード_ID}("カテゴリ")
			IF MATCH(RESULTS,"네오 스페이시언")
				CALL FULLRESET
				효과条件カテゴリ = E•HERO
				효과条件종류 = モン全般
				CALL CARD_ADD_GY_BASIC(듀얼리스트,"손패",ゾーン,場所)
			ELSE
				CALL FULLRESET
				효과条件カテゴリ = 네오 스페이시언
				효과条件종류 = モン全般
				CALL CARD_ADD_GY_BASIC(듀얼리스트,"손패",ゾーン,場所)
			ENDIF
			IF RESULT == 1
				CALL FULLRESET
				효과範囲덱 = 1
				효과条件スーパーカテゴリ = 네오 스페이시언
				CALL 효과条件レベル_ADV(5, "以上")
				효과条件カテゴリ = E•HERO
				CALL CARD_SP_SUMMON_ADV(듀얼리스트,ゾーン,場所)
				IF RESULT == 1
					SIF RESULT:1 == 38000
						CALL SET_FIELD_INFO(듀얼리스트,"필드",カード소환先,"永攻",1000)
					CALL FULLRESET
					효과範囲덱 = 1
					효과範囲묘지 = 1
					효과条件名前 = 융합
					CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
				ENDIF
			ENDIF
		ENDIF
		EX소환可能종류 = 융합
		CALL CARD_EX_RESTRICTION(듀얼리스트)
	ENDIF
ENDIF
