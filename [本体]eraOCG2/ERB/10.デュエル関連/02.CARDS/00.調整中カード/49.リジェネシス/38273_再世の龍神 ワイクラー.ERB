@CARDNAME_38273,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 리제네시스 드래곤 와이크라
	CASE "略称"
		RESULTS = 와이크라
	CASE "カテゴリ"
		RESULTS:0 = 리제네시스
ENDSELECT


@CARD_38273(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	;효과モン/通常モン/의식/융합/싱크로/엑시즈/링크/초차원
	CASE "종류"
		RETURN 효과モン
	;어둠속성/빛속성/땅속성/물속성/화염속성/바람속성
	CASE "속성"
		RETURN 바람속성
	CASE "종족"
		RETURN 드래곤족
	CASE "レベル"
		RETURN 8
	CASE "攻撃力"
		RETURN 2500
	CASE "守備力"
		RETURN 2500
	CASE "추가日"
		RETURN 250913
ENDSELECT
RETURN 0

@CARD_EXPLANATION_38273(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 ①②③의 효과는 각각 １턴에 １번밖에 사용할 수 없다。
CALLFORM CARD_ABLE(1,손패起動)
PRINTL ①：필드에 원래 공격력이나 원래 수비력이 ２５００인 몬스터가 존재하는 경우에 발동할 수 있다。
PRINTL 이 카드를 패에서 특수 소환한다。
CALLFORM CARD_ABLE(2,필드起動,フリー誘発)
PRINTL ②：자신 / 상대 턴에、이 카드를 릴리스하고、상대 필드의 마법 / 함정 카드 １장을 대상으로 발동할 수 있다。
PRINTL 그 카드를 제외한다。
CALLFORM CARD_ABLE(3,필드→묘지 ,손패→묘지 ,덱→묘지 ,破壊→묘지 ,戦闘→묘지)
PRINTL ③：이 카드가 상대 턴에 묘지로 보내진 경우、발동할 수 있다。
PRINTL 이 턴의 엔드 페이즈에、묘지에서 이 카드를 패에 넣는다。


@CARDCAN_38273(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,38273)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動
	SIF メインフェイズ != 1
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0
	SIF 발동済_1(듀얼리스트,38273)
		RETURN 0
	CALL CARD_SS_CHECK(듀얼리스트,38273,"손패",1)
	SIF RESULT == 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件元固攻撃力 = 2500
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		RETURN 1
	CALL FULLRESET
	효과範囲필드 = 1
	효과範囲상대필드 = 1
	효과条件元固守備力 = 2500
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT == 1
		RETURN 1
	RETURN 0

ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	SIF 발동済_2(듀얼리스트,38273)
		RETURN 0
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과範囲상대마함 = 1
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 필드→묘지 || 종류 == 손패→묘지 || 종류 == 덱→묘지 || 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
    SIF 발동済_3(듀얼리스트,38273)
		RETURN 0
	SIF ゾーン != "묘지"
		RETURN 0
	SIF ターンプレイヤー == 듀얼리스트
		RETURN 0
	RETURN 1	
	
ELSEIF 종류 == ターン終了時予約
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件名前 = 리제네시스 드래곤 와이크라
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ENDIF

@CARDEFFECT_38273(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 選択カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,38273,1)
	IF RESULT == 0
		CALL FULLRESET
		CALL CARD_SP_SUMMON(듀얼리스트, "손패", 場所)
	ENDIF
		
ELSEIF 종류 == 필드起動 || 종류 == フリー誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,38273,2)
	IF RESULT == 0
	    CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL FULLRESET
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"마함単体",場所,"제외")
	ENDIF

ELSEIF 종류 == 필드→묘지 || 종류 == 손패→묘지 || 종류 == 덱→묘지 || 종류 == 破壊→묘지 || 종류 == 戦闘→묘지
    CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,38273,3)
	IF RESULT == 0
		CALL CARD_RESERV(듀얼리스트,38273)
	ENDIF
	
ELSEIF 종류 == ターン終了時予約
	CALL FULLRESET
	효과範囲묘지 = 1
	효과条件名前 = 리제네시스 드래곤 와이크라
	CALL CARD_ADD_ADV(듀얼리스트,ゾーン,場所)
ENDIF






