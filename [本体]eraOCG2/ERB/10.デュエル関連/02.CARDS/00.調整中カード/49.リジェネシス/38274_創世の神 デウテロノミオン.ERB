@CARDNAME_38274,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 리제네시스 갓 데우테로노미온
	CASE "略称"
		RESULTS = 데우테로노미온
	CASE "カテゴリ"
		RESULTS:0 = 리제네시스
ENDSELECT


@CARD_38274(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	;효과モン/通常モン/의식/융합/싱크로/엑시즈/링크/초차원
	CASE "종류"
		RETURN 효과モン
	;어둠속성/빛속성/땅속성/물속성/화염속성/바람속성
	CASE "속성"
		RETURN 빛속성
	CASE "종족"
		RETURN 미확인족
	CASE "レベル"
		RETURN 10
	CASE "攻撃力"
		RETURN 2500
	CASE "守備力"
		RETURN 2500
	CASE "추가日"
		RETURN 250913
	CASE "소환불가"
		RETURN 1
	CASE "특수불가"
		RETURN 1
ENDSELECT
RETURN 0

@CARD_EXPLANATION_38274(종류)
#DIM DYNAMIC 종류

PRINTL このカードは通常소환できず、このカードの효과でしか특수소환できない。
PRINTL このカードの①の효과は１ターンに１度しか발동できない。
CALLFORM CARD_ABLE(1,손패起動)
PRINTL ①：자신フィールドの元々の攻撃力と元々の守備力が２５００の表側표시モンスター１体を제외し、발동できる。
PRINTL このカードを특수소환する。
CALLFORM CARD_ABLE(2,특수소환時)
PRINTL ②：このカードが특수소환した場合に발동できる。
PRINTL 덱から「리제네시스」마법・함정カード１枚を자신フィールドにセットする。
CALL CARD_ABLE(0,攻守変動)
PRINTL ③：このカードの攻撃力はバトルフェイズの間、２５００アップする。
CALLFORM CARD_ABLE(0,지속효과)
PRINTL ④：このカードの攻撃は貫通する。


@CARDCAN_38274(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,38274)
SIF RESULT == 1
	RETURN 0

IF 종류 == 손패起動
	SIF 발동済_1(듀얼리스트,38274)
		RETURN 0
	SIF ターンプレイヤー != 듀얼리스트
		RETURN 0	
	SIF メインフェイズ != 1
		RETURN 0
	SIF ゾーン != "손패"
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_제외NG置換") >= 1
		RETURN 0
	CALL FULLRESET
	효과範囲필드 = 1
	효과条件元固攻撃力 = 2500
	효과条件元固守備力 = 2500
	효과条件표시 = 表側표시
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 특수소환時
	SIF 발동済_2(듀얼리스트,38274)
		RETURN 0
	SIF 마함数(듀얼리스트) >= 5
		RETURN 0
	CALL FULLRESET
	효과範囲덱 = 1
	효과条件カテゴリ = 리제네시스
	효과条件종류 = 마함全般
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	CALL CARD_DRAW_CHECK(1)
	RETURN 1

ELSEIF 종류 == 攻守変動
	SIF ゾーン != "필드"
		RETURN 0
	SIF バトルフェイズ != 1
		RETURN 0
	RETURN 1

ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1

ENDIF


@CARDEFFECT_38274(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者 
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 攻撃力修正
#DIM DYNAMIC 選択カード

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%


IF 종류 == 손패起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,38274,1)
	IF RESULT == 0
	    CALL FULLRESET
		효과範囲필드 = 1
	    효과条件元固攻撃力 = 2500
	    효과条件元固守備力 = 2500
	    효과条件표시 = 表側표시
		효과条件行先플래그 = 제외行き
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
		IF RESULT == 1
			CALL FULLRESET
			CALL CARD_SP_SUMMON(듀얼리스트,"손패",場所)
		ENDIF
	ENDIF

ELSEIF 종류 == 특수소환時
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,38274,2)
	IF RESULT == 0
		CALL FULLRESET
		효과条件종류 = 마함全般
		효과条件カテゴリ = 리제네시스
		CALL CARD_SET_BASIC(듀얼리스트,"덱",ゾーン,場所,"裏側표시")
	ENDIF

ELSEIF 종류 == 攻守変動
	攻撃力修正 = 2500
	CALL STATCHANGE_BASIC(듀얼리스트,"自身",場所,攻撃力修正)	

ELSEIF 종류 == 지속효과
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"貫通")

ENDIF