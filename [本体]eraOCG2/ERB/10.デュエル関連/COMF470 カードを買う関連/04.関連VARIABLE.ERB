@CHECK_STRUC_GET
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 덱,10000
#DIM DYNAMIC パック,10000



FOR カウンタ,1000,100000
	VARSET RESULT
	VARSET RESULTS
	TRYCALLFORM CARDNAME_{カウンタ}("名前")
	SIF RESULTS == ""
		CONTINUE
	TRYCALLFORM CARD_{カウンタ}("トークン")
	SIF RESULT == 1
		CONTINUE
	SIF ITEM:カウンタ <= 0
		CONTINUE

	TRYCALLFORM CARDNAME_{カウンタ}("종족")
	SELECTCASE RESULT
	CASE 식물족
		パック:13 += 1
	CASE 천사족
		パック:14 += 1
	CASE 언데드족
		パック:15 += 1
	CASE 기계족
		パック:16 += 1
	CASE 바다생물족
		パック:18 += 1
	CASE 사이킥족
		パック:20 += 1
	CASE 비늘야수족
		パック:22 += 1
	ENDSELECT

	TRYCALLFORM CARDNAME_{カウンタ}("カテゴリ")
	SELECTCASE RESULTS
	CASE "충혹마"
		덱:100 += 1
	CASE "드래곤메이드"
		덱:101 += 1
	CASE "Live☆Twin","Evil☆Twin"
		덱:102 += 1
	CASE "센츄리온"
		덱:103 += 1
	CASE "엑소시스터"
		덱:104 += 1
	CASE "바르모니카"
		덱:105 += 1
	CASE "맬리스"
		덱:106 += 1

	CASE "도레미코드","환주","바르모니카"
		パック:35 += 1
	CASE "라바르","염왕","참기"
		パック:36 += 1
	CASE "성기사","트럼프의 삼총사","센츄리온"
		パック:37 += 1
	CASE "리추어","빙결계","네크로즈"
		パック:38 += 1
	CASE "뱅키시 소울","세리온즈","밸리언츠"
		パック:39 += 1
	CASE "루닉","클리포트","얼터가이스트"
		パック:40 += 1
	CASE "무사신","여섯 무사","첩자"
		パック:42 += 1
	ENDSELECT
NEXT

FOR カウンタ,1,CHARANUM
	SIF CFLAG:カウンタ:면식 != 0
		CONTINUE
	IF TALENT:カウンタ:외부자 != 0
		パック:70 += 1
		パック:71 += 1
	ENDIF
NEXT


