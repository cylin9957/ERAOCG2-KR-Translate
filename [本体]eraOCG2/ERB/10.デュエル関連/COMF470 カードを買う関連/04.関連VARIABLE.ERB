@POOL_COUNT(リセット플래그)
#FUNCTION
#DIM カウンタ
#DIM DYNAMIC リセット플래그

IF リセット플래그
	カウンタ = 0
	RETURNF 0
ELSE
	カウンタ += 1
	RETURNF カウンタ
ENDIF


