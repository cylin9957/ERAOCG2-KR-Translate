@IRAI_EVENT_309(参照先)
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 대상캐릭터
대상캐릭터 = NO_TO_CHARANUM(1645)

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE  "名前"
		RESULTS = (CP15)닫힌 세계의 명 제왕
	CASE "이벤트種"
		RETURN 이벤트_通常
	CASE "依頼先"
		RETURN 即時開始
	CASE "詳細"
		PRINTL 전략
		PRINTL 
		PRINTL 당신은 죽은 것 같다.
		PRINTL
		PRINTL 
	CASE "条件"
		SIF 依頼_完了횟수:306 == 0
			RETURN 0
		;さっちゃんが애처
		SIF !TALENT:대상캐릭터:애처
			RETURN 0
		RETURN 1
	CASE "回想可"
		RETURN 1
ENDSELECT

RETURN 0

