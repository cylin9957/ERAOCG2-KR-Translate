@CARDCAN_259855(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,259855)
SIF RESULT == 1
	RETURN 0

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン開始時強制
	SIF ターンプレイヤー == 듀얼리스트
		RETURN 0
	RETURN 1
ELSEIF 종류 == 필드起動
	SIF 発動済_1(듀얼리스트,259855)
		RETURN 0
	SIF ターンプレイヤー == 듀얼리스트
		RETURN 0
	SIF バトルフェイズ != 1
		RETURN 0
	SIF GETVAR(@"%듀얼리스트%_MZ_表:場所") != 1 
		RETURN 0
	RETURN 1
ENDIF

@CARDEFFECT_259855(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カードID

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 지속효과
	CALL FULLRESET
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"破耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"戦耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"対耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"表固")
ELSEIF 종류 == ターン開始時強制
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,259855)
	IF RESULT == 0
		CALL SET_PLAYER_INFO(듀얼리스트,"ドロースキップ",1)
		CALL SET_PLAYER_INFO(対面者,"ドロースキップ",1)
		CALL FULLRESET
		효과範囲상대묘지 = 1
		효과条件名前 = 현자의 보액
		효과条件CNT = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		IF RESULT >= 10
			PRINTFORML 필드에 진좌하는 퀸즈 베드의 천개가 열리고있다……！
			CALL FULLRESET
			CALL CARD_GENERATE_DECK_EX(対面者,59854)
			CALL SET_FIELD_INFO(듀얼리스트,"필드",場所,"表",1,上書きモード)
		ENDIF
	ENDIF
ELSEIF 종류 == 필드起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,259855,1)
	IF RESULT == 0
		CALL CARD_REMOVAL_BASIC(듀얼리스트,"필드全体",場所,"永攻DOWN",999999)
	ENDIF
ENDIF

