		RETURN 0
	SIF メインフェイズ != 1
		RETURN 0
	IF ターンプレイヤー == 듀얼리스트
		CALL FULLRESET
		효과範囲상대필드 = 1
		효과範囲상대마함 = 1
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
	ELSE
		CALL FULLRESET
		효과範囲필드 = 1
		효과条件名前 = 매지컬 실크 퀸즈 베드
		효과条件표시 = 攻撃표시
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
