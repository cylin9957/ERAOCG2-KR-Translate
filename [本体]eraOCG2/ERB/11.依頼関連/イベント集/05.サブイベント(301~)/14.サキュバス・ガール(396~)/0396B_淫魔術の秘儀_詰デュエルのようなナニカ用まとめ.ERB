@CARDCAN_259851(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,259851)
SIF RESULT == 1
	RETURN 0

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == ターン開始時強制
	RETURN 1
ELSEIF 종류 == 지속효과
	SIF ゾーン != "필드"
		RETURN 0
	RETURN 1
ELSEIF 종류 == 효과後強制誘発
	SIF ゾーン != "필드"
		RETURN 0
	CALL FULLRESET
	효과条件カテゴリ = 서큐버스 걸
	효과条件종류 = 융합
	CALL CARD_CHECK_FIN_DATA(듀얼리스트,"특수소환",1)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == ターン終了時予約
	RETURN 1
ENDIF

@CARDEFFECT_259851(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カードID

IF 종류 == ターン開始時強制
	CALL SET_PLAYER_INFO("자신","ドロースキップ",1)
	CALL SET_PLAYER_INFO("자신","バトルスキップ", 1)
	CALL SET_PLAYER_INFO("상대","ドロースキップ",1)
	CALL SET_PLAYER_INFO("상대","バトルスキップ", 1)
ELSEIF 종류 == 지속효과
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"破耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"対耐")
	CALL STATCHANGE_EFFECT(듀얼리스트,"自身",場所,"戦耐")
ELSEIF 종류 == 효과後強制誘発
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,259851)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		CALL CARD_RESERV(듀얼리스트,259851)
	ENDIF
ELSEIF 종류 == ターン終了時予約
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,259851)
	IF RESULT == 0
		CALL SET_PLAYER_INFO(듀얼리스트,"듀얼패배플래그",4)
	ENDIF
ENDIF

