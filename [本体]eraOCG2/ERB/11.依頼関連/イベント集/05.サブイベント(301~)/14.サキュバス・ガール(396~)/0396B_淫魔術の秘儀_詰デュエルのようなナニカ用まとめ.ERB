@CARDNAME_259850,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 섹스하지 않으면 못나가는 방
	CASE "略称"
		RESULTS = 수수께끼의 방
	CASE "カテゴリ"
	    RESULTS:0 = 매지션 걸
		RESULTS:1 = 서큐버스 걸
		RESULTS:2 = E.R.A
ENDSELECT


@CARD_259850(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 지속마법
	CASE "生産불가"
		RETURN 1
	CASE "生成カード"
		RETURN 259851
ENDSELECT

RETURN 0

@CARD_EXPLANATION_259850(종류)
#DIM DYNAMIC 종류

CALL TEXT_DECORATION("ROGUE")
; PRINTL
; ALIGNMENT CENTER
; PRINTL 光と闇が交わりし時。
; PRINTL 闇は契りと共に全てを喰らう悪魔の形を成し、
; PRINTL 光の供物でもって全き大いなる闇を司さどらん。
; ALIGNMENT LEFT
; PRINTL
PRINTL 자신 필드는 "음마도의 계약장"이 된다。
PRINTL ①：자신 메인 페이즈에 발동할 수 있다。
PRINTL 융합 몬스터 카드에 의해 정해진 １장째 융합 소재 몬스터만을 자신의 패 / 필드에서 묘지로 보내고、
PRINTL 자신 필드에 동명 카드가 존재하지 않는 "서큐버스 걸" 융합 몬스터 １체를 ＥＸ덱에서 특수 소환한다。
PRINTL 그 후, 묘지에 "현자의 보액" 토큰을 １장 생성한다。
PRINTL 이 효과에서는 같은 카드명 몬스터는 １턴에 １번밖에 특수 소환할 수 없으며、
PRINTL 이 효과를 발동하는 턴, 자신은 "서큐버스 걸" 몬스터밖에 ＥＸ덱에서 특수 소환할 수 없다。
CALL TEXT_DECORATION("ROGUE")

@CARDCAN_259850(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,259850)
SIF RESULT == 1
	RETURN 0

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 마함起動
	CALL FULLRESET
	효과範囲EX덱 = 1
	효과条件名前 = 블랙 매지션 걸의 이름이 새겨진 공백의 카드
	CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
	SIF RESULT != 1
		RETURN 0
	RETURN 1
ELSEIF 종류 == 지속효과
	RETURN 1
ENDIF

@CARDEFFECT_259850(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 選択カードID

IF 종류 == 마함起動
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,259850)
	IF RESULT == 0
		CALL FULLRESET
		효과条件名前 = 블랙 매지션 걸의 이름이 새겨진 공백의 카드
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"EX덱",ゾーン,場所)
		選択カード = RESULT
		IF 選択カード != 9999
			CALL FULLRESET
			효과範囲필드 = 1
			효과条件名前 = 블랙 매지션 걸
			CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
			IF RESULT == 1
				CALL SUCCUGIRL_FUSION_BEDROOM("자신",59850)
				CALL CARD_GENERATE(듀얼리스트,59850)
				SETVAR @"%듀얼리스트%_EX덱:選択カード",-1
				CALL CARD_GENERATE_GY(듀얼리스트,宝液ID)
			ENDIF
		ENDIF
		CALL CARD_EX_RESTRICTION(듀얼리스트,"서큐버스 걸")
	ENDIF
ELSEIF 종류 == 지속효과
	CALL SET_FIELD_MAGIC_INFO(듀얼리스트, "음마도의 계약장")
ENDIF

