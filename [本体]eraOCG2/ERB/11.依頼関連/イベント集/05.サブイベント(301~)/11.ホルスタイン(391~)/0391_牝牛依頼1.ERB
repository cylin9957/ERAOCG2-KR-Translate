@IRAI_EVENT_391(参照先)
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 대상캐릭터

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE  "名前"
		RESULTS = (CP-HLS1)"호루스"타인 뷰티
	CASE "이벤트種"
		RETURN 이벤트_通常
	CASE "依頼先"
		RETURN 即時開始
	CASE "詳細"
		PRINTL 묘한 광고가 도착했다…
		PRINTL 
		PRINTL 
		PRINTL 보수: 밀크 팜, 암소 등록 【미유】
	CASE "条件"
		SIF 依頼_完了済 != 0
			RETURN 0
		;E.R.Aカード解放済み
		SIF パック_解放:99 == 0
			RETURN 0
		;「ＶＳ！ＥＲＡ구울즈！1-2」のクリアか、イシズとの면식が必要
		SIF 依頼_完了횟수:110 != 0 || CFLAG:NO_TO_CHARANUM(2501):면식 != 0
			RETURN 1
		RETURN 0
	CASE "回想可"
		RETURN 1
ENDSELECT

RETURN 0

