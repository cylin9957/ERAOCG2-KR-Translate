@IRAI_EVENT_435(参照先)
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 대상캐릭터

VARSET RESULT
VARSET RESULTS



SELECTCASE 参照先
	CASE  "名前"
		RESULTS = (CP-DMR4)음교의 낙인
	CASE "이벤트種"
		RETURN 이벤트_施設
	CASE "依頼先"
		RETURN 場所_教会
	CASE "詳細"
		IF 依頼_完了횟수:435 == 0
			PRINTL 신자가 늘어난 어느 날, 쿠엠에게서 새로운 제안을 받았다
		ELSE
			PRINTL 임신하고 있는 여자에게 카드를 낳게 할까…?
		ENDIF
		PRINTL 
		PRINTL 
	CASE "条件"
		SIF TALENT:MASTER:성별 == 1
			RETURN 0
		CALL COUNT_EVENT433
		SIF 12 > RESULT
			RETURN 0
		RETURN 1
	CASE "利用制限"
		SIF 依頼플래그:435 != 0
			RETURN 3
		RETURN 0
ENDSELECT

RETURN 0

