@IRAI_EVENT_531(参照先)
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 대상캐릭터
#DIM DYNAMIC 条件체크
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE  "名前"
		RESULTS = (CP-ZI2)무구한 자의 전장
	CASE "이벤트種"
		RETURN 이벤트_通常
	CASE "依頼先"
		RETURN 即時開始
	CASE "詳細"
		PRINTL 【제니스의 무녀】
		PRINTL 제니스에 봉사하며, 그 목적을 위해 행동하는 소녀.
		PRINTL 이차원과 공간을 연결하는 능력을 가지고 있으며, 그것으로 크리처를 불러내 싸운다.
		PRINTL 초수 세계에서 몇 안 되는 인류 중 한 명.
	CASE "条件"
		;전회 클리어
		SIF 依頼_完了횟수:530 == 0
			RETURN 0
		;군자금 시세는 얼마지……?
		;우선 듀얼 비용 5000엔이네!
		;음료수 사지 않으면 진행 불가 +600엔
		SIF MONEY < 5600
			RETURN 0
		;영업시간은 12시~19시를 예상하고 있습니다
		;그렇지 않더라도 8시간 이상의 노동은 노동 기준에 걸려요!
		SIF TIME < KANTAN_TIME(12,00) || TIME >= KANTAN_TIME(19,00)
			RETURN 0
		RETURN 1
ENDSELECT

RETURN 0

