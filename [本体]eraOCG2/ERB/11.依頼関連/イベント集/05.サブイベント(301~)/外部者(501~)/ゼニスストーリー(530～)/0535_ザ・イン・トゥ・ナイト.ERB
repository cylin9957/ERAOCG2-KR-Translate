	CALL STATCHANGE_BASIC(듀얼리스트,"상대필드",場所,-1000)

ELSEIF 종류 == 永紋章起動
	CALL FULLRESET
	효과条件カテゴリ = 제니스
	CALL STATCHANGE_EFFECT(듀얼리스트,"필드",場所,"戦阻")

ELSEIF 종류 == 時限紋章起動
	CALL FULLRESET
	효과範囲손패 = 1
	효과条件종류:0 = 通常마법
	효과条件종류:1 = 通常함정
	효과条件任意 = 1
	CALL CARD_ADD_GY_ADV(対面者,ゾーン,場所)
	IF RESULT != 1
		CALL CARDDRAW_BASIC(듀얼리스트,2)
		CALL FULLRESET
		효과範囲덱 = 1
		효과条件종류:0 = 通常마법
		효과条件종류:1 = 通常함정
		CALL CARD_ADD_GY_ADV(듀얼리스트,ゾーン,場所)
	ENDIF
ENDIF
