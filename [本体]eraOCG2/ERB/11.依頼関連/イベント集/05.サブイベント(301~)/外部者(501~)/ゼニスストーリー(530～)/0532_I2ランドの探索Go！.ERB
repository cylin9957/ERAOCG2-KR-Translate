@IRAI_EVENT_532(参照先)
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 대상캐릭터
#DIM DYNAMIC 条件チェック
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE  "名前"
		RESULTS = (CP-ZI3)I2랜드로 탐색 Go！
	CASE "이벤트種"
		RETURN 이벤트_通常
	CASE "依頼先"
		RETURN 即時開始
	CASE "詳細"
		PRINTL 【무구한 수호자】
		PRINTL 듀얼마시티에서, 사람들과 크리처의 조화를 유지하는 역할을 맡은 자.
		PRINTL 평소에는 초수세계에서 복구를 하고 있지만, 때때로 듀얼마시티에 놀러 오기도 한다.
		PRINTL 기본적으로 수호자로서의 일은 특별히 없으므로, 아르바이트를 하며 생활비 등을 벌고 있다.
	CASE "条件"
		;前回クリア
		SIF 依頼_完了횟수:531 == 0
			RETURN 0
		RETURN 1
		CASE "추가条件"
		;入場料１人分（フリーパス）
		IF TALENT:MASTER:연령 == -1
			IF MONEY < 5000
				RESULTS = 필요금액：５０００엔
				RETURN 1
			ENDIF
		ELSEIF TALENT:MASTER:연령 >= 0
			IF MONEY < 8000
				RESULTS = 필요금액：８０００엔
				RETURN 1
			ENDIF
		ENDIF
		;お昼描写は保留中なので昼より後ね
		;遅いと遊ぶ時間無くなっちゃう！
		IF TIME < KANTAN_TIME(11,00)
			RESULTS = １１시가 될때까지 받을 수 없습니다
			RETURN 1
		ELSEIF TIME >= KANTAN_TIME(14,00)
			RESULTS = １４시 보다 빠르지 않으면 받을 수 없습니다
			RETURN 1
		ENDIF
		RETURN 0
ENDSELECT

RETURN 0

