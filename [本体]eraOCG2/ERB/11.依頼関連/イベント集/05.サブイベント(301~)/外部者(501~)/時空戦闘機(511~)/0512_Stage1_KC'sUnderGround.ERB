@IRAI_EVENT_512(参照先)
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 대상캐릭터

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE  "名前"
		RESULTS = (VV1)Stage1 KC's UnderGround
	CASE "이벤트種"
		RETURN 이벤트_通常
	CASE "依頼先"
		RETURN 即時開始
	CASE "詳細"
		PRINTL 바이퍼에게서 할 이야기가 있는 것 같다.
		PRINTL 
		PRINTL
	CASE "条件"
		SIF 依頼_完了횟수:511 == 0
			RETURN 0
		RETURN 1
ENDSELECT

RETURN 0

