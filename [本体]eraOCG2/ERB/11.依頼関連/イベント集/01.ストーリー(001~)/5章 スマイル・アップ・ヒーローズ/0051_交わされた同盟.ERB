@CARDNAME_200050,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 증식의 G
	CASE "略称"
		RESULTS = 증식의 G
	CASE "カテゴリ"
		RESULTS:0 = 없음
ENDSELECT


@CARD_200050(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
	
		RETURN 효과モン
	CASE "속성"
		RETURN 땅속성
	CASE "종족"
		RETURN 곤충족
	CASE "レベル"
		RETURN 2
	CASE "攻撃力"
		RETURN 500
	CASE "守備力"
		RETURN 200
ENDSELECT

RETURN 0

@CARD_EXPLANATION_200050(종류)
#DIM DYNAMIC 종류

PRINTL 이 카드명의 효과는 １턴에 １번밖에 사용할 수 없다。
CALLFORM CARD_ABLE(1,손패起動,フリー誘発)
PRINTL ①：자신 / 상대턴에, 이 카드를 손패에서 버리고 발동할 수 있다。이 턴 중, 이하의 효과를 적용한다。
PRINTL ●상대가 몬스터를 특수소환할 때마다, 자신은 1장 드로우한다。
PRINTL

@CARDCAN_200050(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 효과制限달성
#DIM DYNAMIC カウンタ

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,200050)
SIF RESULT == 1
	RETURN 0
IF 종류 == 손패起動 || 종류 == フリー誘発
	SIF ゾーン != "손패"
		RETURN 0
	SIF 발동済_1(듀얼리스트,200050)
		RETURN 0
	SIF ターンプレイヤー == 듀얼리스트
		RETURN 0
	RETURN 1
ENDIF



@CARDEFFECT_200050(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC 対面者
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC 選択カード
#DIM DYNAMIC 제외カードID
#DIM DYNAMIC カウンタ

CALL 対面者판정(듀얼리스트)
対面者 = %RESULTS%

IF 종류 == 손패起動 || 종류 == フリー誘発
	;カード발동確認
	CALL CARD_CHOICE(듀얼리스트,종류,ゾーン,場所,200050,1)
	IF RESULT == 0
		CALL CARD_DISPOSE(듀얼리스트,ゾーン,場所)
		SETCOLOR C_YELLOW
		PRINTFORML >>——MESSAGE:——————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————>>>
		PRINTL
		PRINTFORML "이게, 지금의 유희왕 최강의 카드다ー！！"
		PRINTL
		CUSTOMDRAWLINE ―
		PRINTW
		RESETCOLOR
		CALL URARA_CHANCE(듀얼리스트,200050,"덱")
		IF RESULT == 1
			SETCOLOR C_YELLOW
			PRINTFORML >>——MESSAGE:——————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————————>>>
			PRINTL
			PRINTFORML "어라?"
			PRINTL
			CUSTOMDRAWLINE ―
			PRINTW
			RESETCOLOR

			RETURN 0
		ENDIF
		CALL SET_PLAYER_INFO(듀얼리스트,"G효과", 1)
	ENDIF
ENDIF
