@CARDNAME_268004,参照先
;ココで지정カードの名前、略称を返す予定
#DIMS DYNAMIC 参照先

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 거대전함 빅 코어Mk-E.R.A
	CASE "略称"
RESULTS = BIG CORE Mk-ERA
	CASE "カテゴリ"
		RESULTS:0 = 박테리안
		RESULTS:1 = 거대전함
RESULTS:2 = E.R.A
ENDSELECT


@CARD_268004(参照先)

#DIMS DYNAMIC 参照先
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "종류"
		RETURN 초차원
	CASE "속성"
		RETURN 어둠속성
	CASE "종족"
		RETURN 기계족
	CASE "レベル"
		RETURN 8
	CASE "攻撃力"
		RETURN 3000
	CASE "守備力"
		RETURN 1900
	CASE "특수불가"
		RETURN 1
	CASE "성별"
		RETURN 牝性
ENDSELECT
RETURN 0

