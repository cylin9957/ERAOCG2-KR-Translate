@IRAI_EVENT_517(参照先)
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 이벤트主

이벤트主 = NO_TO_CHARANUM(8001)

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE  "名前"
		RESULTS = (VV6) Stage6 BigCore_Mk-E.R.A
	CASE "이벤트種"
		RETURN 이벤트_通常
	CASE "依頼先"
		RETURN 即時開始
	CASE "詳細"
		PRINTL 바이퍼에게서 긴급 콜이 왔다.
		PRINTL 또 유괴당한 걸까?
		PRINTL
		SETCOLOR C_YELLOW
		PRINTL （주의！５연전＋덱변경불가！）
		RESETCOLOR
		PRINTL  
	CASE "条件"
		;前回이ベントクリア
		SIF 依頼_完了횟수:516 == 0
			RETURN 0
		;改造レベル最大で불가
		SIF CFLAG:이벤트主:500 == 9
			RETURN 0
		RETURN 1
	CASE "回想可"
		RETURN 1
ENDSELECT

RETURN 0

