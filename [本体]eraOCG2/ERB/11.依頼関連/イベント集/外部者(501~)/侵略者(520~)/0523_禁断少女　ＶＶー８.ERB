@IRAI_EVENT_523(参照先)
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 대상캐릭터
#DIM DYNAMIC 条件チェック
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE  "名前"
		RESULTS = (CP-IV4)금단소녀　ＶＶー８
	CASE "이벤트種"
		RETURN 이벤트_通常
	CASE "依頼先"
		RETURN 即時開始
	CASE "詳細"
		PRINTL "【지난번 줄거리】"
		PRINTL 
		PRINTFORML 상점가의 복권이 대박이 터져, 온천 여행을 가게 된 %CALLNAME:MASTER%와 S급 침략자 일행.
		PRINTL 맛있는 요리와 기분 좋은 온천에서 편안한 시간을 보내는 일행이었지만,
		PRINTL 그것은 악의 조직 『오프사이드 트래커』가 교묘하게 설치한 함정이었다!
		PRINTFORML 암흑 온천 물대포에 의해 몸의 70퍼센트가 움직이지 않게 된 썬매드를 안고 도망치는 %CALLNAME:MASTER% 일행.
		PRINTL 그들은 무사히 도망칠 수 있을 것인가……
	CASE "条件"
		SIF 依頼_完了횟수:522 == 0
			RETURN 0
		RETURN 1
	CASE "回想可"
		RETURN 1
ENDSELECT

RETURN 0

