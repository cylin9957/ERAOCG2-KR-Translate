@IRAI_EVENT_525(参照先)
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 대상캐릭터
#DIM DYNAMIC 条件チェック
#DIM DYNAMIC ヴィヴィ
ヴィヴィ = NO_TO_CHARANUM(4065)
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE  "名前"
		RESULTS = (CP-IV6)규진마루 습격
	CASE "이벤트種"
		RETURN 이벤트_通常
	CASE "依頼先"
		RETURN 即時開始
	CASE "詳細"
		PRINTL 드디어 작전 결행의 날이 왔다.
		PRINTL 덱 조정이나 드로우 연습은 지금 미리 끝내두자.
		IF 依頼_進行보존:525 >= 2
			PRINTL
			PRINTFORML (의뢰를 완료하려면 %CALLNAME:ヴィヴィ%의 연모 또는 굴복이 필요합니다.)
			PRINTL
		ENDIF
	CASE "条件"
		SIF 依頼_完了횟수:524 == 0
			RETURN 0
		RETURN 1
	CASE "回想可"
		RETURN 1
ENDSELECT

RETURN 0

