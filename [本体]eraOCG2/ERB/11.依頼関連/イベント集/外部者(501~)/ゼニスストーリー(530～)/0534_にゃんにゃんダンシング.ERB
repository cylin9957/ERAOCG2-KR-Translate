@IRAI_EVENT_534(参照先)
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 대상캐릭터
#DIM DYNAMIC 条件チェック
VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE  "名前"
		RESULTS = (CP-ZI4R)냥냥댄싱
	CASE "이벤트種"
		RETURN 이벤트_通常
	CASE "依頼先"
		RETURN 即時開始
	CASE "詳細"
		PRINTL 【"나"의 정점】
		PRINTL 사자의 머리를 가진 제니스.
		PRINTL 크리처들이 가진 "내가 최고다"라는 생각이 하나의 "나"라는 의식으로 통일되어 있고,
		PRINTL 빛의 모습이나 어둠의 모습 등이 각각 다른 "나"로 존재하고 있다.
		PRINTL 결국 제니스에서 이탈하여, 제로의 세계가 아니라 전사로서 자부심을 가지고 싸우기를 원했다.
	CASE "条件"
		;前提クリアで불가
		SIF 依頼_完了횟수:533 != 0
			RETURN 0
		;関連이벤트플래그
		SIF 依頼_進行보존:533 != 1
			RETURN 0
		;善人だったらダメー
		SIF FLAG:악랄치 < 35
			RETURN 0
		;裏通りで買い物できないとダメー
		SIF 依頼_完了횟수:206 == 0
			RETURN 0
		;おもちゃ代
		SIF MONEY < 949800
			RETURN 0
		RETURN 1
	CASE "回想可"
		RETURN 1
ENDSELECT

RETURN 0


