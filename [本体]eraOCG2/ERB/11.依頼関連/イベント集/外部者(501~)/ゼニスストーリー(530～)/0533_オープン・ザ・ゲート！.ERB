@CARDCAN_2024533(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所

CALL CARD_NEGATE(듀얼리스트,종류,ゾーン,場所,2024533)
SIF RESULT == 1
	RETURN 0

IF 종류 == 永攻紋章起動
	RETURN 1

ELSEIF 종류 == 時限紋章起動
	IF エンドフェイズ == 1
		CALL FULLRESET
		효과範囲묘지 = 1
		효과条件カテゴリ:0 = 오라클
		효과条件カテゴリ:1 = 언노운
		효과条件カテゴリ:2 = 제니스
		CALL CARD_CHECK_ADV(듀얼리스트,종류,ゾーン,場所)
		SIF RESULT != 1
			RETURN 0
	ENDIF
	RETURN 1
ENDIF

@CARDEFFECT_2024533(듀얼리스트,종류,ゾーン,場所)
#DIMS DYNAMIC 듀얼리스트
#DIMS DYNAMIC ゾーン
#DIM DYNAMIC 종류
#DIM DYNAMIC 場所
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 選択カード

IF 종류 == 永攻紋章起動
	CALL FULLRESET
	CALL STATCHANGE_BASIC(듀얼리스트,"필드",場所,500,1000)

ELSEIF 종류 == 時限紋章起動
	IF スタンバイフェイズ == 1
		CALL LIFE_CHANGE(듀얼리스트,"増加","효과",1000)
	ELSEIF エンドフェイズ == 1
		CALL FULLRESET
		효과条件カテゴリ:0 = 오라클
		효과条件カテゴリ:1 = 언노운
		효과条件カテゴリ:2 = 제니스
		효과条件任意 = 1
		CALL CARD_CHOOSE_BASIC(듀얼리스트,"묘지",ゾーン,場所)
		IF RESULT != 9999
			CALL CARD_ADD_DECK_SPC(듀얼리스트,"묘지",RESULT)
		ENDIF
	ENDIF
ENDIF
