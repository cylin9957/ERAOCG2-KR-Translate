@SET_CHARA_RAND_SENSE_PRETTYWEAK(설정캐릭터_ID, MOB_NO)
#DIM DYNAMIC 설정캐릭터_ID
#DIM DYNAMIC MOB_NO

SELECTCASE RAND:3
