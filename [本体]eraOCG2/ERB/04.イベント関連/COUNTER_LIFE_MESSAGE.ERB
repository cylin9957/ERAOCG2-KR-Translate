@EVENT_COUNTE_LIFE_MESSAGE11
#DIM DYNAMIC 호감도修正
IF CFLAG:TARGET:변기화 == 1
	SELECTCASE RAND:3
		CASE 1
			PRINTFORML %CALLNAME:TARGET%는 숨을 헐떡이며、구속된 몸을 비틀었다。
		CASE 2
			PRINTFORML %CALLNAME:TARGET%의 아이마스크를 살짝 벗기자、흐리멍덩한 눈에서 눈물을 흘렸다。
		CASE 3
			PRINTFORML %CALLNAME:TARGET%의 바이브를 밀어 넣자 신음하며 몸부림쳤다。
		CASEELSE

	ENDSELECT
ELSE
	호감도修正 = 1
	SELECTCASE RAND:호감도修正 + 1
		CASE 1
			PRINTFORML %CALLNAME:TARGET%는 이제는 좀 나가줬으면 하는 것 같다。
		CASEELSE
	ENDSELECT
ENDIF
RETURN 1

