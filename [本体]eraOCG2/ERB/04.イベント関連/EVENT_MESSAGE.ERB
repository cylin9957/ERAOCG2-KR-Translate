@EVENT_MESSAGE



;-------------------------------------------------
;関数名:OPPAI_DESCRIPTION
;概　要:言葉でなく心で理解しろ
;引　数:ARG…캐릭터登録番号
;戻り値:夢
;備　考:文中関数
;使用例:%OPPAI_DESCRIPTION(TARGET)%
;-------------------------------------------------
@OPPAI_DESCRIPTION(ARG)
#FUNCTIONS
;爆乳
IF BUST_SIZE(ARG) >= 3
	RETURNF "같은 방에 있는것만으로도 존재감을 느끼는 가슴"
;爆乳
ELSEIF BUST_SIZE(ARG) == 2
	RETURNF "손에 넘칠정도로 큰 가슴"
;巨乳
ELSEIF BUST_SIZE(ARG) == 1
	RETURNF "손에서 흘러넘칠것같은 정도의 가슴"
;虚가슴
ELSEIF BUST_SIZE(ARG) == -3
	RETURNF "허무"
;絶壁
ELSEIF BUST_SIZE(ARG) == -2
	RETURNF "희미하게 부풀음이 느껴지는 정도의 가슴"
;貧乳
ELSEIF BUST_SIZE(ARG) == -1
	RETURNF "작은 가슴"
;並乳
ELSE
	RETURNF "가슴"
ENDIF


;-------------------------------------------------
;関数名:NIPPLE_DESCRIPTION
;引　数:ARG…캐릭터登録番号
;使用例:%NIPPLE_DESCRIPTION(TARGET)%
;例文:유두
;備　考:文中関数
;ARG:1を1にすると状態に応じた形容詞がつく
;使用例:%NIPPLE_DESCRIPTION(TARGET,1)%
;例文:ビンビンになっている유두
;-------------------------------------------------
@NIPPLE_DESCRIPTION(ARG,ARG:1)
#FUNCTIONS
VARSET LOCALS

IF ARG:1
	IF EX:ARG:Ｂ절정 * 10000 + PALAM:ARG:쾌Ｂ + PALAM:ARG:욕정 >= 30000
		IF RAND:4 == 0
			LOCALS += "꽝꽝해진"
		ELSEIF RAND:3 == 0
			LOCALS += "유륜부터 봉긋하게 부풀은"
		ELSEIF RAND:2 == 0
			LOCALS += "유륜째로 완전히 부풀어오른"
		ELSE
			LOCALS += "쾌감에 꺼림찍하게 부풀은"
		ENDIF
	ELSEIF EX:ARG:Ｂ절정 * 10000 + PALAM:ARG:쾌Ｂ + PALAM:ARG:욕정 >= 10000
		IF RAND:4 == 0
			LOCALS += "봉오리처럼 위를 향한"
		ELSEIF RAND:3 == 0
			LOCALS += "우뚝솟은"
		ELSEIF RAND:2 == 0
			LOCALS += "완전히 딱딱해진"
		ELSE
			LOCALS += "발기한"
		ENDIF
	ELSEIF EX:ARG:Ｂ절정 * 10000 + PALAM:ARG:쾌Ｂ + PALAM:ARG:욕정 >= 3000
		IF RAND:4 == 0
			LOCALS += "딱딱하게 굳은"
		ELSEIF RAND:3 == 0
			LOCALS += "딱딱함을 갖추기 시작한"
		ELSEIF RAND:2 == 0
			LOCALS += "발기하는중인"
		ELSE
			LOCALS += "충혈되고있는"
		ENDIF
	ELSE
		IF TALENT:체형 <= -1
			LOCALS += "작은"
		ELSE
			LOCALS += "아직 부드러운"
		ENDIF
	ENDIF
ENDIF

IF RAND:4 == 0
	LOCALS += "유두"
ELSEIF RAND:3 == 0
	LOCALS += "돌기"
ELSEIF RAND:2 == 0
	LOCALS += "끝단"
ELSE
	LOCALS += "유두"
ENDIF
RETURNF LOCALS

