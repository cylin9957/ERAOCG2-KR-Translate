@TROPHY_NO_21(参照先)
#DIMS DYNAMIC 参照先
#DIM DYNAMIC 캐릭터_NO
#DIM DYNAMIC 堕ち数

VARSET RESULT
VARSET RESULTS

SELECTCASE 参照先
	CASE "名前"
		RESULTS = 이 게임에 타락은 3개 있다
	CASE "詳細"
		PRINTL 
		PRINTL 연모, 음란, 굴복 함락시킨 수가 각각 1 이상
		PRINTL 
		PRINTL 보수: 3,000DP
		PRINTL 
	CASE "条件"
		VARSET 堕ち数
		FOR 캐릭터_NO,1,CHARANUM
			IF TALENT:캐릭터_NO:연모 >= 1
				堕ち数 = 1
			ENDIF
		NEXT
		SIF 堕ち数 == 0
			RETURN 0
		VARSET 堕ち数
		FOR 캐릭터_NO,1,CHARANUM
			IF TALENT:캐릭터_NO:음란 >= 1
				堕ち数 += 1
			ENDIF
		NEXT
		SIF 堕ち数 == 0
			RETURN 0
		VARSET 堕ち数
		FOR 캐릭터_NO,1,CHARANUM
			IF TALENT:캐릭터_NO:굴복됨 >= 1
				堕ち数 += 1
			ENDIF
		NEXT
		SIF 堕ち数 == 0
			RETURN 0
		RETURN 1
	CASE "報酬"
		PRINTFORMW DP + 3,000
		FLAG:듀얼포인트 += 3000
ENDSELECT
RETURN 0


