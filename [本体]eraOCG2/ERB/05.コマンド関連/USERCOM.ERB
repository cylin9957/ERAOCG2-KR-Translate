		追跡설정_ID = TARGET
	ELSE
		VARSET 追跡설정_ID
	ENDIF
	RETURN 1
ELSEIF RESULT == 820 && CFLAG:우후후 && EQUIP:MASTER:0
	EQUIP:MASTER:0 = 0
	CALL CLOTHES_SETTING_TRAIN(MASTER)
	RETURN 1
;ELSEIF RESULT == 888
;	FLAG:1 = 1
ELSEIF RESULT == 999 && CFLAG:우후후
	IF CFLAG:(TARGET:1):우후후 == 2 && BASE:(TARGET:1):만족 < ((ABL:(TARGET:1):욕망) * 100) && CFLAG:(TARGET:1):수면 != 1
