@MAP_SETING, ARGS, C_CODE, ARG:0, ARG:1, ARG:2, ARG:3, ARG:4

#DIM C_CODE
#DIMS MAPING, 50

;ARG:n = マップの横に표시するボタン

VARSET MAPING
SPLIT ARGS, "/", MAPING
LOCAL:1 = RESULT
;マップ표시
PRINT 
FOR LOCAL, 0, LOCAL:1
	;数値か確認
	TOHALF MAPING:LOCAL
	TOINT RESULTS
	;数値ならボタン化
	IF RESULT != 0
		CALL GETMAP(RESULT)
	ELSE
		PRINTFORM %MAPING:LOCAL%
	ENDIF
NEXT
PRINT 



;マップ側面のボタン표시
FOR LOCAL, 0, 5
	SIF ARG:LOCAL == 0
		CONTINUE
	
	;数値1~9なら大文字化
	IF ARG:LOCAL < 10
		TOSTR ARG:LOCAL
		TOFULL RESULTS
		LOCALS = %RESULTS%
	ELSE
		LOCALS = %@"{ARG:LOCAL}"%
	ENDIF
	PRINT  
	PRINTBUTTON @"%LOCALS%:%GETPLACENAME(ARG:LOCAL)% ", ARG:LOCAL
NEXT
PRINTL

