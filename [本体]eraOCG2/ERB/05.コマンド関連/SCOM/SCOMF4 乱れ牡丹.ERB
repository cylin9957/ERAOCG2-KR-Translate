;-------------------------------------------------
;흐트러진 모란
;식스系파생コマンド
;-------------------------------------------------
@SCOM4
;-------------------------------------------------
;COM別FLAGの설정
;-------------------------------------------------
TCVAR:MASTER:사정대상캐릭터_1 = TARGET
TCVAR:MASTER:사정부위플래그 = 1
TCVAR:MASTER:사정쾌락강도 = 2300

;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;消費체력、기력
DOWNBASE:체력 += 50
DOWNBASE:기력 += 100

CALL SOURCE_CALC_쾌Ｖ(TARGET, MASTER, 400)
CALL SOURCE_CALC_쾌Ｃ(TARGET, MASTER, 300)
CALL SOURCE_CALC_쾌Ｂ(TARGET, MASTER, 300)
CALL SOURCE_CALC_노출(TARGET, MASTER, 600)
CALL SOURCE_CALC_접촉(TARGET, MASTER, 180)

;고통
LOCAL = 200 * PAIN_CHECK_V(TARGET) / 1000
CALL SOURCE_CALC_고통(TARGET, MASTER, LOCAL)


;애널、파과血、촉수の汚れ
IF (STAIN:MASTER:0 & 8) || (STAIN:MASTER:0 & 32) || (STAIN:MASTER:0 & 64)
	CALL SOURCE_CALC_불결(TARGET, MASTER, 500)
;정액汚れ
ELSEIF STAIN:MASTER:0 & 4
	CALL SOURCE_CALC_불결(TARGET, MASTER, 250)
ENDIF
;처녀상실
CALL LOST_VIRGIN(TARGET, MASTER, 1)
;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;사정に관계なく行われる処理はここから
;질내に파과の血の汚れを付加
SIF TALENT:처녀
	STAIN:6 |= 64
;奴隷の질내⇔조교者のＰの汚れが이동
STAIN:6 |= STAIN:MASTER:2
STAIN:MASTER:2 |= STAIN:6

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;성교경험
EXP:Ｖ성교경험 += 1
CFLAG:TARGET:엉덩이성장 ++
;同性愛　레즈なら戻り値1게이なら戻り値2
IF HETEROSEX(TARGET,MASTER)
	EXP:(50 + HETEROSEX(TARGET,MASTER)) += 4
	EXP:MASTER:(50 + HETEROSEX(TARGET,MASTER)) += 4
ENDIF

;애정경험
SIF !ASSIPLAY
	EXP:애정경험 += 2 * TALENT:연모

RETURN 1

@CAN_SCOM4(ARG)
#DIM DYNAMIC 素質確認
#DIM DYNAMIC 판정値
#DIM DYNAMIC 汚れ値
#DIM DYNAMIC 実行値
;-------------------------------------------------
;実行판정処理
;-------------------------------------------------
;밧줄で縛られている場合は無視（밧줄が無い時のみ実行판정を行う）
IF !TEQUIP:밧줄
	SKIPDISP ARG
	PRINTL 흐트러진 모란

	;すべての命令に共通の要素を考慮
	;(순종が高いと命令に従いやすいなど)
	CALL COM_ORDER
	素質確認 = RESULT:1
	판정値 += RESULT
	;ABL:욕망
	IF ABL:욕망
		SIF 素質確認
			PRINT  + 
		판정値 += ABL:욕망 * 3
		PRINT 욕망
		PRINTV 'LV,ABL:욕망,'(,ABL:욕망 * 3,')
		素質確認 = 1
	ENDIF
	;ABL:Ｖ감각
	IF ABL:Ｖ감각
		SIF 素質確認
			PRINT  + 
		판정値 += ABL:Ｖ감각 * 2
		PRINT Ｖ감각
		PRINTV 'LV,ABL:Ｖ감각,'(,ABL:Ｖ감각 * 2,')
		素質確認 = 1
	ENDIF
	;쾌락각인
	IF MARK:쾌락각인
		SIF 素質確認
			PRINT  + 
		판정値 += MARK:쾌락각인 * 3
		PRINT 쾌락각인
		PRINTV 'LV,MARK:쾌락각인,'(,MARK:쾌락각인 * 3,')
		素質確認 = 1
	ENDIF
	;PALAM:윤활（の不足）
	IF PALAM:윤활 < PALAMLV:3
		PRINT  - 
		판정値 -= 5
		PRINT 윤활不足
		PRINTV '(,5,')
		素質確認 = 1
	ENDIF
	;PALAM:욕정
	IF GETPALAMLV(PALAM:욕정,5)
		SIF 素質確認
			PRINT  + 
		판정値 += GETPALAMLV(PALAM:욕정,5) * 3
		PRINT 욕정
		PRINTV 'LV,GETPALAMLV(PALAM:욕정,5),'(,GETPALAMLV(PALAM:욕정,5) * 3,')
		素質確認 = 1
	ENDIF
	;恥じらい
	IF TALENT:수치심 > 0
		PRINT  - 
		판정値 -= 2
		PRINT 恥じらい
		PRINTV '(,2,')
		素質確認 = 1
	ENDIF
	;쾌락の부정
	IF TALENT:쾌감반응 < 0 && !TALENT:연모
		PRINT  - 
		판정値 -= 5
		PRINT 쾌락の부정
		PRINTV '(,5,')
		素質確認 = 1
	ENDIF
	;男嫌い（연모、친애で無効）
	IF TALENT:성별기호 & 1 && !(TALENT:MASTER:성별 & 1) && (!TALENT:연모 || MASTER != MASTER)
		PRINT  - 
		판정値 -= 12
		PRINT 男嫌い
		PRINTV '(,12,')
		素質確認 = 1
	ENDIF
	;연모
	IF TALENT:연모
		SIF 素質確認
			PRINT  + 
		판정値 += 15
		PRINT 연모
		PRINTV '(,15,')
		素質確認 = 1
	ENDIF
	;친애
	IF TALENT:연모 > 1
		SIF 素質確認
			PRINT  + 
		판정値 += 5
		PRINT 친애
		PRINTV '(,5,')
		素質確認 = 1
	ENDIF
	;처녀
	LOCAL = MAX(0,40 - EXP:Ｖ경험)
	IF TALENT:처녀 && LOCAL
		PRINT  - 
		판정値 -= LOCAL
		PRINT 처녀
		PRINTV '(,LOCAL,')
		素質確認 = 1
	;V경험
	ELSEIF EXP:Ｖ경험 < EXPLV:2
		PRINT  - 
		판정値 -= 15
		PRINT Ｖ경험
		PRINT 不足
		PRINTV '(,15,')
		素質確認 = 1
	ENDIF

	;조교者がふたなり
	IF (TALENT:MASTER:성별 & 3) == 3
		SIF 素質確認
			PRINT  + 
		판정値 += 4
		PRINT ふたなり
		PRINTV '(,4,')
		素質確認 = 1
	ENDIF

	;미약
	IF TEQUIP:미약
		SIF 素質確認
			PRINT  + 
		판정値 += 6
		PRINT 미약
		PRINTV '(,6,')
		素質確認 = 1
	ENDIF

	
	;愛液の汚れ
	SIF STAIN:MASTER:2 & 1
		汚れ値 += 1
	;정액の汚れ
	SIF STAIN:MASTER:2 & 4
		汚れ値 += 3
	;애널の汚れ
	SIF STAIN:MASTER:2 & 8
		汚れ値 += 7
	SIF STAIN:MASTER:2 & 16
		汚れ値 += 15
	汚れ値 = 汚れ値 * (2 - TALENT:악취내성) / 2

	;더러움
	IF 汚れ値
 		PRINT  - 
		판정値 -= 汚れ値
		PRINT 더러움
		;악취둔감
		IF TALENT:악취내성 > 0
			PRINT 、악취둔감
		ELSEIF TALENT:악취내성 < 0
			PRINT 、악취민감
		ENDIF
		PRINTV '(,汚れ値,')
		素質確認 = 1
	ENDIF

	;合計を표시(47以上で実行)
	PRINT  = 
	PRINTV 판정値

	実行値 = 47
	SIF 판정値 < 実行値
		PRINT  < 
	SIF 판정値 == 実行値
		PRINT  = 
	SIF 판정値 > 実行値
		PRINT  > 
	PRINT 실행치
	PRINTV 実行値

	WAIT
	SKIPDISP 0
	;実行できない
	SIF 판정値 < 実行値
		RETURN 0
ENDIF
RETURN 1


;-------------------------------------------------
;実行판정
;-------------------------------------------------
@COM_ABLE904
;흐트러진 모란実行판정
SIF !TFLAG:100
	RETURN 0
;一括관리
SIF GLOBAL_COMABLE(904)
	RETURN RESULT
;ある程度발기してないとダメ
SIF BASE:MASTER:발기 < 1000
	RETURN 0
;수치플레이中じゃないとダメ
SIF TEQUIP:수치플레이 == 0
	RETURN 0
;바이브/딜도/로터삽입중はダメ
SIF TEQUIP:바이브 || TEQUIP:딜도 || TEQUIP:로터
	RETURN 0
;オトコだとダメ
SIF  !(TALENT:2 & 1)
	RETURN 0
;全裸でないとダメ（양말、특수は除く）
SIF TEQUIP:0
	RETURN 0
;조교者がオトコ、半人半妖、ふたなり、もしくは페니스バンドがないとダメ
SIF !(TALENT:MASTER:성별 & 2) && ITEM:PBAND == 0
	RETURN 0

RETURN 1