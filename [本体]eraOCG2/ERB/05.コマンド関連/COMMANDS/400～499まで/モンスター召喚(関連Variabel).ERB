;-------------------------------------------------
@MONSTERLIST_RESET

;VARSET 필터_所持,1
VARSET 필터_所持

VARSET 필터_속성
VARSET 필터_종족
VARSET 필터_구상
VARSET 필터_名前
VARSET 필터_듀얼리스트
VARSET 필터_전화번호
VARSET 필터_消去可


VARSET 필터_연모
VARSET 필터_お気に入り
VARSET 필터_덱所持
VARSET 필터_저장모유량
VARSET 필터_임신可
VARSET 필터_素質_ACT
VARSET 필터_화상
VARSET 필터_仲良し

VARSET 필터_素質, -100
VARSET 필터_각인, -100
VARSET 필터_特価品


@MONSTERLIST_SORT(ソート種)
#DIM DYNAMIC 캐릭터_ID
#DIM DYNAMIC 캐릭터番号
#DIM DYNAMIC リスト番号
#DIM DYNAMIC ソート種
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 気入_リスト,10000
#DIM DYNAMIC 気入_カウンタ
#DIM DYNAMIC 이력_リスト,10000
#DIM DYNAMIC 이력_カウンタ
#DIM DYNAMIC 通常_リスト,10000
#DIM DYNAMIC 通常_カウンタ

VARSET 소환_リスト
VARSET リスト最大値

FOR カウンタ,0,10
	IF お気に入り:カウンタ > 0
		CALL MONSTERLIST_FILTER(お気に入り:カウンタ)
		IF RESULT == 1
			SIF MATCH(気入_リスト,お気に入り:カウンタ)
				CONTINUE
			気入_リスト:気入_カウンタ = お気に入り:カウンタ
			気入_カウンタ ++
		ENDIF
	ENDIF
NEXT

FOR カウンタ,0,10
	IF 소환이력:カウンタ > 0
		SIF 소환이력:カウンタ > CHARANUM
			CONTINUE
		CALL MONSTERLIST_FILTER(소환이력:カウンタ)
		IF RESULT == 1
			SIF MATCH(이력_リスト,소환이력:カウンタ)
				CONTINUE
			SIF MATCH(気入_リスト,소환이력:カウンタ)
				CONTINUE
			이력_リスト:이력_カウンタ = 소환이력:カウンタ
			이력_カウンタ ++
		ENDIF
	ENDIF
NEXT


FOR 캐릭터番号,1,CHARANUM
	SIF MATCH(이력_リスト,캐릭터番号)
		CONTINUE
	SIF MATCH(気入_リスト,캐릭터番号)
		CONTINUE
	CALL MONSTERLIST_FILTER(캐릭터番号)
	IF RESULT == 1
		通常_リスト:通常_カウンタ = 캐릭터番号		
		通常_カウンタ ++
	ENDIF
NEXT


FOR カウンタ,0,通常_カウンタ
	通常_リスト:カウンタ += NO:(通常_リスト:カウンタ) * 10000000
NEXT
ARRAYSORT 通常_リスト,FORWARD,0,通常_カウンタ

FOR カウンタ,0,通常_カウンタ
	通常_リスト:カウンタ = 通常_リスト:カウンタ % 10000000 
NEXT

FOR カウンタ,0,気入_カウンタ
	소환_リスト:リスト最大値 = 気入_リスト:カウンタ
	リスト最大値 ++
NEXT

FOR カウンタ,0,이력_カウンタ
	소환_リスト:リスト最大値 = 이력_リスト:カウンタ
	リスト最大値 ++
NEXT

FOR カウンタ,0,通常_カウンタ
	소환_リスト:リスト最大値 = 通常_リスト:カウンタ
	リスト最大値 ++
NEXT



@MONSTERLIST_FILTER(캐릭터_ID)
#DIM DYNAMIC 캐릭터_ID
#DIM DYNAMIC カウンタ
#DIM DYNAMIC 条件달성
#DIM DYNAMIC 初期値
#DIM DYNAMIC 終結値
IF 필터_所持 == 1
	SIF CFLAG:캐릭터_ID:소유함 == 0
		RETURN 0
ELSEIF 필터_所持 == 2
	SIF CFLAG:캐릭터_ID:소유함 == 1
		RETURN 0
ENDIF

VARSET 条件달성
IF 필터_名前 != ""
	SIF STRFIND(NAME:캐릭터_ID, 필터_名前)  != -1
		条件달성 ++
	SIF STRFIND(CALLNAME:캐릭터_ID, 필터_名前)  != -1
		条件달성 ++
	SIF 条件달성 == 0
		RETURN 0
ENDIF


IF 필터_속성 > 0
	SIF TALENT:캐릭터_ID:속성 != 필터_속성
		RETURN 0
ENDIF

IF 필터_종족 > 0
	SIF TALENT:캐릭터_ID:종족 != 필터_종족
		RETURN 0
ENDIF

IF 필터_구상 > 0
	VARSET RESULT
	TRYCALLFORM M_KOJO_K{NO:캐릭터_ID}
	SIF RESULT != 1
		RETURN 0
ENDIF

IF 필터_저장모유량
	SIF CFLAG:캐릭터_ID:저장모유량 <= 0
		RETURN 0
ENDIF

IF 필터_임신可
	SIF TALENT:캐릭터_ID:임신가능 <= 0
		RETURN 0
ENDIF

VARSET 条件달성
IF 필터_消去可 == 1
	SIF TALENT:캐릭터_ID:모브 >= 1
		条件달성 = 1
	SIF TALENT:캐릭터_ID:복제 >= 1
		条件달성 = 1
	SIF 条件달성 == 0
		RETURN 0
ENDIF


IF 필터_듀얼리스트 > 0
	IF 필터_듀얼리스트 == 1
		SIF TALENT:캐릭터_ID:듀얼리스트 >= 1
			RETURN 0
	ELSEIF 필터_듀얼리스트 == 2
		SIF TALENT:캐릭터_ID:듀얼리스트 == 0
			RETURN 0
	ENDIF
ENDIF

IF 필터_전화번호 > 0
	SIF CFLAG:캐릭터_ID:전화번호 == 0
		RETURN 0
ENDIF


VARSET 条件달성
IF 필터_연모 > 0
	SIF TALENT:캐릭터_ID:연모 != 0
		条件달성 = 1
	SIF TALENT:캐릭터_ID:굴복됨 != 0
		条件달성 = 1
	SIF 条件달성 == 0
		RETURN 0
ENDIF
VARSET 条件달성
IF 필터_お気に入り
	FOR カウンタ,1,6
		SIF 캐릭터_ID == お気に入り:カウンタ
			条件달성 = 1
	NEXT
	SIF 条件달성 == 0
		RETURN 0
ENDIF
VARSET 条件달성
IF 필터_화상
	;カード화상が有るか無いか판정
	IF ENUMFILES("RESOURCES/CHARA", @"{NO:캐릭터_ID}_*") || ENUMFILES("RESOURCES/CHARA", @"%NAME:캐릭터_ID%_*")
		条件달성 = 1
	ENDIF
	IF 필터_화상 == 1
		SIF 条件달성 == 0
			RETURN 0
	ELSE
		SIF 条件달성 >= 1
			RETURN 0
	ENDIF
ENDIF

IF 필터_덱所持 > 0
	CALL ENEMYDECK_CHECK(캐릭터_ID)
	SIF RESULT == 0
		RETURN 0
ENDIF

VARSET 条件달성
IF 필터_仲良し
	SIF CFLAG:캐릭터_ID:호감도 >= 1000
		条件달성 = 1
	SIF TALENT:캐릭터_ID:굴복됨 != 0
		条件달성 = 1
	SIF 条件달성 == 0
		RETURN 0
ENDIF

VARSET 필터_素質_ACT
FOR カウンタ,0,200
	IF (필터_素質:カウンタ) != -100
		필터_素質_ACT = 1
		BREAK
	ENDIF
NEXT

IF 필터_素質_ACT == 1
	FOR カウンタ,0,200
		SIF (필터_素質:カウンタ) == -100
			CONTINUE
		IF カウンタ == 2
			IF 필터_素質:カウンタ == 4
				SIF TALENT:캐릭터_ID:カウンタ == 1
					RETURN 0
			ELSEIF 필터_素質:カウンタ == 5
				SIF TALENT:캐릭터_ID:カウンタ == 2
					RETURN 0
			ELSE
				SIF TALENT:캐릭터_ID:カウンタ != (필터_素質:カウンタ)
					RETURN 0
			ENDIF
		ELSE
			SIF TALENT:캐릭터_ID:カウンタ != (필터_素質:カウンタ)
				RETURN 0
		ENDIF
	NEXT
	ENDIF
VARSET 条件달성
IF 필터_特価品 == 1
	IF CFLAG:MASTER:현재위치 == 場所_カドショ
		初期値 = 0
	ELSEIF CFLAG:MASTER:현재위치 == 場所_モール
		初期値 = 10
	ELSEIF CFLAG:MASTER:현재위치 == 場所_KCカドショ
		初期値 = 20
	ELSEIF CFLAG:MASTER:현재위치 == 場所_MZカドショ
		初期値 = 30
	ELSEIF CFLAG:MASTER:현재위치 == 場所_裏通り
		初期値 = 40
	ELSEIF CFLAG:MASTER:현재위치 == 場所_STカドショ
		初期値 = 50
ENDIF


	FOR カウンタ,0 + 初期値,10 + 初期値

		SIF CFLAG:MASTER:현재위치 == 場所_STカドショ && カウンタ >= 55
			BREAK
		IF 日々_特価品:カウンタ == 캐릭터_ID
			条件달성 = 1
			BREAK
		ENDIF
	NEXT
	SIF 条件달성 == 0
		RETURN 0
ENDIF

FOR カウンタ,0,4
	SIF 필터_각인:カウンタ == -100
		CONTINUE
	SIF MARK:캐릭터_ID:カウンタ == 0
		RETURN 0
NEXT

RETURN 1



@SUMMON_FILTER
#DIM DYNAMIC カウンタ
#DIM 필터_ページ送り
#DIM CONST リスト표시 = 40
#DIM DYNAMIC リストスタート


;個人メモ、ココで필터ーを通していく、詳細でさらにいろいろと見れる
$INPUT_LOOP

CUSTOMDRAWLINE —
PRINTL 필터 설정
CUSTOMDRAWLINE —
SIF 필터_ページ送り >= 3
	필터_ページ送り = 3

FOR カウンタ,リストスタート,(リストスタート + リスト표시)
	IF カウンタ == 0
		PRINTFORML --성별계열/함락소질--
	ELSEIF カウンタ == 10
		PRINTL
		PRINTFORML --성격계열--
	ELSEIF カウンタ == 20
		PRINTL
		PRINTFORML --성으로의 관심--
	ELSEIF カウンタ == 30
		PRINTL
		PRINTFORML --소녀마음--

	ELSEIF カウンタ == 40
		PRINTFORML --체질--
	ELSEIF カウンタ == 50
		PRINTL
		PRINTFORML --기술--
	ELSEIF カウンタ == 60
		PRINTL
		PRINTFORML --결벽도--
	ELSEIF カウンタ == 70
		PRINTL
		PRINTFORML --솔직도--

	ELSEIF カウンタ == 80
		PRINTFORML --성애성--
	ELSEIF カウンタ == 90
		PRINTL
		PRINTFORML --매료--
	ELSEIF カウンタ == 100
		PRINTL
		PRINTFORML --신체특징--
	ELSEIF カウンタ == 130
		PRINTL
		PRINTFORML --특수특징--
	ENDIF


	SIF TALENTNAME:(カウンタ) == ""
		CONTINUE

	SIF (필터_素質:(カウンタ)) == -100
		SETCOLOR 100,100,100

	PRINTFORM [{(カウンタ),3}]
	PRINTFORM - %TALENTNAME:(カウンタ),20,LEFT%

	RESETCOLOR
NEXT
PRINTL
CUSTOMDRAWLINE —
FOR カウンタ,0,4
	SIF (필터_각인:(カウンタ)) == -100
		SETCOLOR 100,100,100
	PRINTFORM [{(カウンタ + 400),3}]
	PRINTFORM - %MARKNAME:カウンタ,20,LEFT%
	RESETCOLOR
NEXT
PRINTL
CUSTOMDRAWLINE —

SIF リスト표시 == 0
	SETCOLOR 0X444444
PRINTBUTTON "[9998]돌아간다", 9998
RESETCOLOR
PRINTFORM %LOCALS,40,RIGHT%  
SIF 필터_ページ送り == 3
	SETCOLOR 0X444444
PRINTBUTTON "[9999]다음으로", 9999
RESETCOLOR
PRINTL
CUSTOMDRAWLINE —
PRINTBUTTON "[확인종료]", 9996
BINPUT


IF RESULT == 9996
	RETURN 0
ELSEIF RESULT == 9998
	リストスタート -= リスト표시
	SIF リストスタート <= 0
		リストスタート = 0

ELSEIF RESULT == 9999 
	IF リストスタート >= 160
		PRINT
	ELSE
		リストスタート += リスト표시
	ENDIF

ELSEIF RESULT < 300
	IF 필터_素質:RESULT == -100
		CALL SUMMON_FILTER_DETAIL(RESULT)
	ELSE
		필터_素質:RESULT = -100
	ENDIF
ELSEIF RESULT > 300 && RESULT < 410
	RESULT -= 400
	IF 필터_각인:RESULT == -100
		필터_각인:RESULT = 1
	ELSE
		필터_각인:RESULT = -100
	ENDIF
ENDIF

GOTO INPUT_LOOP






@SUMMON_FILTER_DETAIL(대상素質)
#DIM DYNAMIC 대상素質


;オンオフ型の素質
IF TALENTNAME:대상素質 == GET_TALENTNAME(대상素質,1) 
	필터_素質:대상素質 = 1

ELSEIF TALENTNAME:대상素質 != GET_TALENTNAME(대상素質,1)
	필터_素質:대상素質 = 0
	$INPUT_LOOP
	PRINTFORML ----상세설정----
	PRINTFORM 『%TALENTNAME:대상素質%
	PRINTFORM …【%GET_TALENTNAME(대상素質,(필터_素質:대상素質))%】』
	PRINTBUTTON "[소질설정]", 0
	PRINTL
	PRINTFORML %LOCALS,10,LEFT%
	PRINTBUTTON "[종료]", 1
	PRINTFORM 
	BINPUT
	IF RESULT == 0
		IF 대상素質 == 2
			IF 필터_素質:대상素質 == 3
				필터_素質:대상素質 = 1
			ELSE
				필터_素質:대상素質 ++
			ENDIF
		ELSEIF 대상素質 == 3
			IF 필터_素質:대상素質 == 2
				필터_素質:대상素質 = 1
			ELSE
				필터_素質:대상素質 ++
			ENDIF
		ELSEIF 대상素質 == 72
			IF 필터_素質:대상素質 == 2
				필터_素質:대상素質 = 1
			ELSE
				필터_素質:대상素質 ++
			ENDIF
		;성별嗜好
		ELSEIF 대상素質 == 81
			IF 필터_素質:대상素質 == 3
				필터_素質:대상素質 = -1
			ELSEIF 필터_素質:대상素質 == 0
				필터_素質:대상素質 = 1
			ELSE
				필터_素質:대상素質 ++
			ENDIF
		;体系
		ELSEIF 대상素質 == 100
			IF 필터_素質:대상素質 == 3
				필터_素質:대상素質 = -4
			ELSE
				필터_素質:대상素質 ++
			ENDIF
		;몸두께
		ELSEIF 대상素質 == 120
			IF 필터_素質:대상素質 == 4
				필터_素質:대상素質 = 0
			ELSE
				필터_素質:대상素質 ++
			ENDIF
		;가슴사이즈
		ELSEIF 대상素質 == 121
			IF 필터_素質:대상素質 == 5
				필터_素質:대상素質 = -3
			ELSE
				필터_素質:대상素質 ++
			ENDIF
		;허벅지사이즈
		ELSEIF 대상素質 == 122
			IF 필터_素質:대상素質 == 3
				필터_素質:대상素質 = -1
			ELSE
				필터_素質:대상素質 ++
			ENDIF
		;가슴사이즈
		ELSEIF 대상素質 == 128
			IF 필터_素質:대상素質 == 3
				필터_素質:대상素質 = -1
			ELSE
				필터_素質:대상素質 ++
			ENDIF
		;연령
		ELSEIF 대상素質 == 117
			IF 필터_素質:대상素質 == 2
				필터_素質:대상素質 = -2
			ELSE
				필터_素質:대상素質 ++
			ENDIF
		;量的素質
		ELSE
			;성별
			IF 대상素質 == 2
				IF 필터_素質:대상素質 == 3
					필터_素質:대상素質 = 1
				ELSE
					필터_素質:대상素質 ++
				ENDIF
			;端まで行くと-1を掛ける
			ELSEIF STRLENS(GET_TALENTNAME(대상素質,필터_素質:대상素質 + 1)) || 필터_素質:대상素質 == -1
				IF 필터_素質:대상素質 == -1
					필터_素質:대상素質 = 1
				ELSE
					필터_素質:대상素質 ++
				ENDIF
			ELSE
				필터_素質:대상素質 = -1 * 필터_素質:대상素質
			ENDIF
		ENDIF
		GOTO INPUT_LOOP


	ELSEIF RESULT == 1
		RETURN 0
	ENDIF
ENDIF


