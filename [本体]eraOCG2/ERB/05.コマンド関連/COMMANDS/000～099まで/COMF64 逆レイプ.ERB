;-------------------------------------------------
;역 레이프
;식스계커맨드、レベル1
;-------------------------------------------------
@COM64
;-------------------------------------------------
;COM別FLAGの설정
;-------------------------------------------------
TCVAR:TARGET:사정대상캐릭터_1 = MASTER
TCVAR:TARGET:사정부위플래그 = 1
TCVAR:TARGET:사정쾌락강도 = 1500

;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;消費체력、기력
DOWNBASE:체력 += 40
DOWNBASE:기력 += 220

CALL SOURCE_CALC_쾌Ｖ(MASTER, TARGET, 400)
CALL SOURCE_CALC_노출(TARGET, MASTER, 100)
CALL SOURCE_CALC_접촉(TARGET, MASTER, 180)

;고통
LOCAL = 200 * PAIN_CHECK_V(MASTER) / 1000
CALL SOURCE_CALC_고통(MASTER, TARGET, LOCAL)


;애널、파과血、촉수の汚れ
IF (STAIN:MASTER:3 & 8) || (STAIN:MASTER:3 & 32) || (STAIN:MASTER:3 & 64) || (STAIN:MASTER:6 & 8) || (STAIN:MASTER:6 & 32) || (STAIN:MASTER:6 & 64)
	CALL SOURCE_CALC_불결(TARGET, MASTER, 300)
ENDIF
;정조観念
IF !TALENT:연모
	SIF TALENT:동정
		CALL SOURCE_CALC_불결(TARGET, MASTER, 500)
	IF TALENT:정조 > 0
		CALL SOURCE_CALC_불결(TARGET, MASTER, 300)
	ELSEIF TALENT:정조 == 0
		CALL SOURCE_CALC_불결(TARGET, MASTER, 100)
	ENDIF
ENDIF

;정조観念
IF !TALENT:연모
	IF TALENT:정조 == 1
		CALL SOURCE_CALC_불결(TARGET, MASTER, 500)
	ELSEIF TALENT:정조 == 0
		CALL SOURCE_CALC_불결(TARGET, MASTER, 250)
	ENDIF
ENDIF

;처녀상실
CALL LOST_VIRGIN(MASTER, TARGET, 1)
;-------------------------------------------------
;특수ソース処理
;-------------------------------------------------
;조교者が처녀（一回のみ）
IF TALENT:MASTER:처녀 && !GETBIT(CFLAG:3,10)
	SIF !TALENT:연모 && !TALENT:음란
	CALL SOURCE_CALC_불결(TARGET, MASTER, 1000)
	PRINTFORML ＜ %CALLNAME:MASTER%은 처녀를 잃었습니다＞
	;이상경험추가
	IF TALENT:MASTER:처녀 == 1
		EXP:이상경험 += 1
		SETBIT CFLAG:3,10
	ENDIF
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
IF TALENT:2 & 2
	;奴隷のＰ⇔조교者の질내の汚れが이동
	STAIN:2 |= STAIN:MASTER:3
	STAIN:2 |= STAIN:MASTER:6
	STAIN:MASTER:3 |= STAIN:2
	STAIN:MASTER:6 |= STAIN:2

ENDIF

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
EXP:MASTER:Ｖ경험 ++
EXP:MASTER:Ｖ성교경험 ++
CFLAG:MASTER:엉덩이성장 ++
;同性愛　레즈なら戻り値1게이なら戻り値2
IF HETEROSEX(TARGET,MASTER)
	EXP:(50 + HETEROSEX(TARGET,MASTER)) += 4
	EXP:MASTER:(50 + HETEROSEX(TARGET,MASTER)) += 4
ENDIF


RETURN 1

@CAN_COMF64(ARG)
#DIM DYNAMIC 素質確認
#DIM DYNAMIC 판정値
#DIM DYNAMIC 汚れ値
#DIM DYNAMIC 実行値
SKIPDISP ARG
PRINTL 역레이프
;-------------------------------------------------
;実行판정処理
;-------------------------------------------------


;すべての命令に共通の要素を考慮
;(순종が高いと命令に従いやすいなど)
CALL COM_ORDER
素質確認 = RESULT:1
판정値 += RESULT

;ABL:욕망
IF ABL:욕망
	SIF 素質確認
		PRINT  + 
	판정値 += ABL:욕망 * 2
	PRINT 욕망
	PRINTV 'LV,ABL:욕망,'(,ABL:욕망 * 2,')
	素質確認 = 1
ENDIF

IF MARK:쾌락각인
	SIF 素質確認
		PRINT  + 
	판정値 += MARK:쾌락각인 * 2
	PRINT 쾌락각인
	PRINTV 'LV,MARK:쾌락각인,'(,MARK:쾌락각인 * 2,')
	素質確認 = 1
ENDIF

;쾌감에 솔직
IF TALENT:쾌감반응 > 0
	SIF 素質確認
		PRINT  + 
	판정値 += 2
	PRINT 쾌감에 솔직
	PRINTV '(,2,')
	素質確認 = 1
ENDIF
;쾌감의 부정（음란、쾌락광で無効）
IF TALENT:쾌감반응 < 0 && !TALENT:음란
	PRINT  - 
	판정値 -= 2
	PRINT 쾌감의 부정
	PRINTV '(,2,')
	素質確認 = 1
ENDIF
;음란
IF TALENT:음란
	SIF 素質確認
		PRINT  + 
	판정値 += 5
	PRINT 음란
	PRINTV '(,5,')
	素質確認 = 1
ENDIF
;쾌락광
IF TALENT:음란 > 1
	SIF 素質確認
		PRINT  + 
	판정値 += 5
	PRINT 쾌락광
	PRINTV '(,5,')
	素質確認 = 1
ENDIF

;조교者が처녀
IF TALENT:MASTER:처녀
	PRINT  - 
	판정値 -= 15
	PRINTFORM %NAME:MASTER%가 처녀
	PRINTV '(,15,')
	素質確認 = 1
ENDIF

;조교者が정조観念
IF TALENT:MASTER:정조 > 0
	PRINT  - 
	판정値 -= 10
	PRINTFORM %NAME:MASTER%가 정조관념
	PRINTV '(,10,')
	素質確認 = 1
ENDIF

;조교者が정조無頓着
IF TALENT:MASTER:정조 < 0
	PRINT  + 
	판정値 += 5
	PRINTFORM %NAME:MASTER%가 정조무관심
	PRINTV '(,5,')
	素質確認 = 1
ENDIF

;조교者のABL:기교
IF ABL:MASTER:기교
	SIF 素質確認
		PRINT  + 
	판정値 += ABL:MASTER:기교 * 4
	PRINTFORM %NAME:MASTER%의 기교
	PRINTV 'LV,ABL:MASTER:기교,'(,ABL:MASTER:기교 * 4,')
	素質確認 = 1
ENDIF

;合計を표시(15以上で実行)
PRINT  = 
PRINTV 판정値

実行値 = 15
SIF 판정値 < 実行値
	PRINT  < 
SIF 판정値 == 実行値
	PRINT  = 
SIF 판정値 > 実行値
	PRINT  > 
PRINT 실행치
PRINTV 実行値

WAIT
SKIPDISP 0
;実行できない
SIF 판정値 < 実行値
	RETURN 0
RETURN 1


;-------------------------------------------------
;実行판정
;-------------------------------------------------
@COM_ABLE64
;역 레이프実行판정
SIF !TFLAG:100
	RETURN 0
SIF GLOBAL_COMABLE(64)
	RETURN RESULT
;オトコ、半人半妖、후타나리でないとダメ
SIF HAS_PENIS(TARGET) == 0
	RETURN 0
;ある程度발기してないとダメ
SIF BASE:발기 < 1000
	RETURN 0
;조교者がオトコだとダメ
SIF !(TALENT:MASTER:성별 & 1)
	RETURN 0
;体格差がある場合は主人が금단의지식持ちじゃないとダメ
SIF ABS(TALENT:체형 - TALENT:MASTER:체형) > 2 && !TALENT:MASTER:금단의지식
	RETURN 0


;하반신착의
SIF TEQUIP:0 & 4
	RETURN 0
;목욕탕플레이 중は플레이매트が無いとダメ
IF TEQUIP:목욕탕플레이
	SIF ITEM:20 == 0 && NOITEM == 0
		RETURN 0
ENDIF
;새댁플레이 중はダメ
SIF TEQUIP:새댁플레이
	RETURN 0
RETURN 1