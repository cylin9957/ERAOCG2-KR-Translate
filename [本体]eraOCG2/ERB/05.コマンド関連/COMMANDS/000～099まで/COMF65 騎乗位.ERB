;-------------------------------------------------
;기승위
;식스계커맨드、レベル2
;-------------------------------------------------
@COM65
;前回の조교が3Pのとき3Pへ
IF PREVCOM == 71
	CALL COM_ABLE71
	SIF RESULT
		JUMP COM71
ENDIF
;-------------------------------------------------
;COM別FLAGの설정
;-------------------------------------------------
TCVAR:MASTER:사정대상캐릭터_1 = TARGET
TCVAR:MASTER:사정부위플래그 = 1
TCVAR:MASTER:사정쾌락강도 = 2000


;봉사쾌락플래그
TFLAG:봉사쾌락플래그 = 1
;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;消費체력、기력
DOWNBASE:체력 += 60
DOWNBASE:기력 += 150
CALL SOURCE_CALC_쾌Ｖ(TARGET, MASTER, 500)
CALL SOURCE_CALC_노출(TARGET, MASTER, 120)
CALL SOURCE_CALC_접촉(TARGET, MASTER, 150)
CALL SOURCE_CALC_봉사(TARGET, MASTER, 250)

;고통
LOCAL = 200 * PAIN_CHECK_V(TARGET) / 1000
CALL SOURCE_CALC_고통(TARGET, MASTER, LOCAL)


;애널、파과血、촉수の汚れ
IF (STAIN:MASTER:0 & 8) || (STAIN:MASTER:0 & 32) || (STAIN:MASTER:0 & 64)
	CALL SOURCE_CALC_불결(TARGET, MASTER, 500)
;정액汚れ
ELSEIF STAIN:MASTER:0 & 4
	CALL SOURCE_CALC_불결(TARGET, MASTER, 250)
ENDIF
;정조観念
IF !TALENT:연모
	IF TALENT:정조 == 1
		CALL SOURCE_CALC_불결(TARGET, MASTER, 500)
	ELSEIF TALENT:정조 == 0
		CALL SOURCE_CALC_불결(TARGET, MASTER, 250)
	ENDIF
ENDIF
;처녀상실
CALL LOST_VIRGIN(TARGET, MASTER, 1)

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;사정に관계なく行われる処理はここから
;질내に파과の血の汚れを付加
SIF TALENT:처녀
	STAIN:6 |= 64
;奴隷の질내⇔조교者のＰの汚れが이동
STAIN:MASTER:2 |= STAIN:3
STAIN:MASTER:2 |= STAIN:6
STAIN:3 |= STAIN:MASTER:2
STAIN:6 |= STAIN:MASTER:2

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;성교경험
EXP:Ｖ성교경험 ++
CFLAG:TARGET:엉덩이성장 ++
SIF EXP:TARGET:포르치오경험 >= 50
	EXP:TARGET:포르치오경험 ++
;이상경험
IF TALENT:처녀 && ASSIPLAY && (TALENT:MASTER:성별 & 1)
	EXP:이상경험 += 3
ELSEIF TALENT:처녀
	EXP:이상경험 += 2
ENDIF
SIF TALENT:처녀 && !GETBIT(CFLAG:3,1)
	SETBIT CFLAG:3,1

;同性愛　레즈なら戻り値1게이なら戻り値2
IF HETEROSEX(TARGET,MASTER)
	EXP:(50 + HETEROSEX(TARGET,MASTER)) += 7
	EXP:MASTER:(50 + HETEROSEX(TARGET,MASTER)) += 7
ENDIF

EXP:애정경험 += TALENT:연모 * 2
;처녀
SIF TALENT:처녀 && TALENT:연모
	EXP:애정경험 += 5
RETURN 1

@CAN_COM65(ARG)
#DIM DYNAMIC 素質確認
#DIM DYNAMIC 판정値
#DIM DYNAMIC 汚れ値
#DIM DYNAMIC 実行値
SKIPDISP ARG
PRINTL 기승위
;-------------------------------------------------
;実行판정処理
;-------------------------------------------------


;すべての命令に共通の要素を考慮
;(순종が高いと命令に従いやすいなど)
CALL COM_ORDER
素質確認 = RESULT:1
판정値 += RESULT
;ABL:욕망
IF ABL:욕망
	SIF 素質確認
		PRINT  + 
	판정値 += ABL:욕망 * 3
	PRINT 욕망
	PRINTV 'LV,ABL:욕망,'(,ABL:욕망 * 3,')
	素質確認 = 1
ENDIF
;ABL:Ｖ감각
IF ABL:Ｖ감각
	SIF 素質確認
		PRINT  + 
	판정値 += ABL:Ｖ감각 * 2
	PRINT Ｖ감각
	PRINTV 'LV,ABL:Ｖ감각,'(,ABL:Ｖ감각 * 2,')
	素質確認 = 1
ENDIF
;ABL:봉사정신
IF ABL:봉사정신
	SIF 素質確認
		PRINT  + 
	판정値 += ABL:봉사정신 * 4
	PRINT 봉사정신
	PRINTV 'LV,ABL:봉사정신,'(,ABL:봉사정신 * 4,')
	素質確認 = 1
ENDIF
IF MARK:쾌락각인
	SIF 素質確認
		PRINT  + 
	판정値 += MARK:쾌락각인 * 3
	PRINT 쾌락각인
	PRINTV 'LV,MARK:쾌락각인,'(,MARK:쾌락각인 * 3,')
	素質確認 = 1
ENDIF

;PALAM:윤활（の不足）
IF PALAM:윤활 < PALAMLV:3
	PRINT  - 
	판정値 -= 5
	PRINT 윤활부족
	PRINTV '(,5,')
	素質確認 = 1
ENDIF
;PALAM:욕정
IF GETPALAMLV(PALAM:욕정,5)
	SIF 素質確認
		PRINT  + 
	판정値 += GETPALAMLV(PALAM:욕정,5) * 3
	PRINT 욕정
	PRINTV 'LV,GETPALAMLV(PALAM:욕정,5),'(,GETPALAMLV(PALAM:욕정,5) * 3,')
	素質確認 = 1
ENDIF
;부끄럼쟁이
IF TALENT:수치심 > 0
	PRINT  - 
	판정値 -= 2
	PRINT 부끄러움
	PRINTV '(,2,')
	素質確認 = 1
ENDIF
;쾌감의 부정（음란、쾌락광で無効）
IF TALENT:쾌감반응 < 0 && !TALENT:연모
	PRINT  - 
	판정値 -= 5
	PRINT 쾌감의 부정
	PRINTV '(,5,')
	素質確認 = 1
ENDIF
;男嫌い（연모、친애で無効）
IF TALENT:성별기호 & 1 && !(TALENT:MASTER:성별 & 1) && (!TALENT:연모 || MASTER != MASTER)
	PRINT  - 
	판정値 -= 12
	PRINT 남성혐오
	PRINTV '(,12,')
	素質確認 = 1
ENDIF
;연모
IF TALENT:연모
	SIF 素質確認
		PRINT  + 
	판정値 += 15
	PRINT 연모
	PRINTV '(,15,')
	素質確認 = 1
ENDIF
;친애
IF TALENT:연모 > 1
	SIF 素質確認
		PRINT  + 
	판정値 += 5
	PRINT 친애
	PRINTV '(,5,')
	素質確認 = 1
ENDIF

;マイペース
IF TALENT:페이스배분 == -1
	SIF 素質確認
		PRINT  + 
	판정値 += 5
	PRINT 마이페이스
	PRINTV '(,5,')
	素質確認 = 1
ENDIF


;처녀
LOCAL = MAX(10,40 - EXP:Ｖ경험)
IF TALENT:처녀
	PRINT  - 
	판정値 -= LOCAL
	PRINT 처녀
	PRINTV '(,LOCAL,')
	素質確認 = 1
ELSEIF EXP:Ｖ경험 < EXPLV:2
	PRINT  - 
	판정値 -= 15
	PRINT Ｖ경험부족
	PRINTV '(,15,')
	素質確認 = 1
ENDIF

;조교者が후타나리
IF (TALENT:MASTER:성별 & 3) == 3
	SIF 素質確認
		PRINT  + 
	판정値 += 4
	PRINT 후타나리
	PRINTV '(,4,')
	素質確認 = 1
ENDIF

;미약
IF TEQUIP:미약
	SIF 素質確認
		PRINT  + 
	판정値 += 6
	PRINT 미약
	PRINTV '(,6,')
	素質確認 = 1
ENDIF


;愛液の汚れ
SIF STAIN:MASTER:2 & 1
	汚れ値 += 1
;정액の汚れ
SIF STAIN:MASTER:2 & 4
	汚れ値 += 3
;애널の汚れ
SIF STAIN:MASTER:2 & 8
	汚れ値 += 7
SIF STAIN:MASTER:2 & 16
	汚れ値 += 15

汚れ値 = 汚れ値 * (2 - TALENT:악취내성) / 2

;더러움있음
IF 汚れ値
 	PRINT  - 
	판정値 -= 汚れ値
	PRINT 더러움 있음
	;악취둔감
	IF TALENT:악취내성 > 0
		PRINT 、악취둔감
	ELSEIF TALENT:악취내성 < 0
		PRINT 、악취민감
	ENDIF
	PRINTV '(,汚れ値,')
	素質確認 = 1
ENDIF


;合計を표시(35以上で実行)
PRINT  = 
PRINTV 판정値

実行値 = 35
SIF 판정値 < 実行値
	PRINT  < 
SIF 판정値 == 実行値
	PRINT  = 
SIF 판정値 > 実行値
	PRINT  > 
PRINT 실행치
PRINTV 実行値

WAIT
SKIPDISP 0
;実行できない
SIF 판정値 < 実行値
	RETURN 0
RETURN 1

;-------------------------------------------------
;実行판정
;-------------------------------------------------
@COM_ABLE65
;기승위実行판정
SIF !TFLAG:100
	RETURN 0
SIF GLOBAL_COMABLE(65)
	RETURN RESULT
;조교者がオトコ、半人半妖、후타나리の場合はある程度발기してないとダメ
SIF (TALENT:MASTER:성별 & 2) && BASE:MASTER:발기 < 1000
	RETURN 0
;바이브/딜도/로터삽입중はダメ
SIF TEQUIP:바이브 || TEQUIP:딜도 || TEQUIP:로터
	RETURN 0
;オトコだとダメ
SIF !(TALENT:2 & 1)
	RETURN 0
;体格差がある場合は主人が금단의지식持ちじゃないとダメ
SIF ABS(TALENT:체형 - TALENT:MASTER:체형) > 2 && !TALENT:MASTER:금단의지식
	RETURN 0
;조교者がオトコか半人半妖か후타나리じゃないとダメ
SIF !(TALENT:MASTER:성별 & 2)
	RETURN 0

;하반신착의
SIF TEQUIP:0 & 4
	RETURN 0
;밧줄사용중/手足が使えないと불가
SIF TEQUIP:밧줄 || TEQUIP:사지없음
	RETURN 0
;목욕탕플레이 중は플레이매트が無いとダメ
IF TEQUIP:목욕탕플레이
	SIF ITEM:20 == 0 && NOITEM == 0
		RETURN 0
ENDIF

;처녀かつ윤활不足はだめ
SIF PALAM:윤활 < PALAMLV:2 && TALENT:처녀
		RETURN 0
;침대ルームでないとダメ
SIF !BEDROOM(CFLAG:MASTER:현재위치)
	RETURN 0
RETURN 1
