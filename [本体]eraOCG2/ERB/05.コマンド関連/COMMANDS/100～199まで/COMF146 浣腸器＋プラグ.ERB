;-------------------------------------------------
;관장器＋プラグ
;ハードな조교コマンド、レベル4
;-------------------------------------------------
@COM146
;着脱の処理
TEQUIP:관장 = !TEQUIP:관장
IF !TEQUIP:관장
	IF !GETBIT(CFLAG:3,7)
		EXP:이상경험 += 1
		SETBIT CFLAG:3,7
	ENDIF
ENDIF
;-------------------------------------------------
;ソース추가処理
;-------------------------------------------------
;消費체력、기력
DOWNBASE:체력 += 60
DOWNBASE:기력 += 150

CALL SOURCE_CALC_쾌Ａ(TARGET, MASTER, 300)
CALL SOURCE_CALC_노출(TARGET, MASTER, 300)
CALL SOURCE_CALC_고통(TARGET, MASTER, 500)
CALL SOURCE_CALC_불결(TARGET, MASTER, 500)
;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
STAIN:7 |= STAIN:4
STAIN:4 |= STAIN:7
;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;Ａ경험
EXP:Ａ경험 += 4
;同性愛　레즈なら戻り値1게이なら戻り値2
IF HETEROSEX(TARGET,MASTER)
	EXP:(50 + HETEROSEX(TARGET,MASTER)) += 1
	EXP:MASTER:(50 + HETEROSEX(TARGET,MASTER)) += 1
ENDIF

RETURN 1
;-------------------------------------------------
;装着계속の処理
;-------------------------------------------------
@EQUIP_COM146(ARG)
PRINTL ＜관장＋애널플러그 삽입중＞

DOWNBASE:ARG:체력 += 60
DOWNBASE:ARG:기력 += 150

CALL SOURCE_CALC_쾌Ａ(TARGET, -1, 150)
CALL SOURCE_CALC_노출(TARGET, MASTER, 150)
CALL SOURCE_CALC_고통(TARGET, MASTER, 250)
CALL SOURCE_CALC_불결(TARGET, MASTER, 250)
;-------------------------------------------------
;경험の処理
;-------------------------------------------------
;Ａ경험
EXP:ARG:Ａ경험 += 1


;-------------------------------------------------
;実行판정
;-------------------------------------------------
@COM_ABLE146
;관장実行판정
SIF !TFLAG:100
	RETURN 0
;一括관리
SIF GLOBAL_COMABLE(146)
	RETURN RESULT
;하반신착의
SIF TEQUIP:0 > 1
	RETURN 0
;解除はいつでも可能
SIF TEQUIP:21
	RETURN 1
;아이템を持っているか、아이템なし설정でないとダメ
SIF ITEM:9 == 0 && NOITEM == 0
	RETURN 0
;애널바이브사용중は불가
SIF TEQUIP:14
	RETURN 0
;애널비즈사용중は불가
SIF TEQUIP:15
	RETURN 0
;バルーン사용중は불가
SIF TEQUIP:22
	RETURN 0
;전극사용중は불가
SIF TEQUIP:23
	RETURN 0
;Ａ경험が100未満じゃできない
SIF EXP:Ａ경험 < 100
	RETURN 0
;노출증、욕망、순종の합계가 10ないとできない
SIF ABL:순종 + ABL:욕망 + ABL:노출증 < 10
	RETURN 0
;침대ルームでないとダメ
SIF !BEDROOM(CFLAG:MASTER:현재위치)
	RETURN 0
RETURN 1
