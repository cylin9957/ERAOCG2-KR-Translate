	CALL EQUIP_COM55(ARG)

;클리캡装着中
SIF TEQUIP:ARG:클리캡
	CALL EQUIP_COM42(ARG)

;오나홀装着中
SIF TEQUIP:ARG:오나홀
	CALL EQUIP_COM43(ARG)

;바이브装着中
SIF TEQUIP:ARG:바이브
	CALL EQUIP_COM44(ARG)

;애널바이브装着中
SIF TEQUIP:ARG:애널바이브
	CALL EQUIP_COM45(ARG)

;애널비즈사용중
SIF TEQUIP:ARG:애널비즈
	CALL EQUIP_COM46(ARG)

;니플캡装着中
SIF TEQUIP:ARG:니플캡
	CALL EQUIP_COM47(ARG)

;착유기装着中
SIF TEQUIP:ARG:착유기
	CALL EQUIP_COM48(ARG)

;딜도装着中
SIF TEQUIP:ARG:딜도
	CALL EQUIP_COM53(ARG)

;착유기装着中
SIF TEQUIP:ARG:수치플레이
	CALL EQUIP_COM52(ARG)


;유두빨기中
SIF TEQUIP:ARG:유두빨기
	CALL EQUIP_COM11(ARG)

;아이마스크装着中
SIF TEQUIP:ARG:아이마스크
	CALL EQUIP_COM104(ARG)

;밧줄で긴박中
SIF TEQUIP:ARG:밧줄
	CALL EQUIP_COM105(ARG)

;사지없음で긴박中
SIF TEQUIP:ARG:사지없음
	CALL EQUIP_COM150(ARG)

;볼개그装着中
SIF TEQUIP:ARG:볼개그
	CALL EQUIP_COM106(ARG)

;관장＋애널プラグ装着中
SIF TEQUIP:ARG:관장
	CALL EQUIP_COM146(ARG)

;확장벌룬装着中
SIF TEQUIP:ARG:확장벌룬
	CALL EQUIP_COM147(ARG)

;애널전극装着中
SIF TEQUIP:ARG:애널전극
	CALL EQUIP_COM148(ARG)

;비디오촬영中
SIF ARG > 0 && TEQUIP:MASTER:28
	CALL EQUIP_COM183(ARG)
;야외플레이中
SIF TEQUIP:ARG:야외플레이
	CALL EQUIP_COM184(ARG)

;수치플레이中
;SIF TEQUIP:ARG:30
;	CALL EQUIP_COM186(ARG)

;목욕탕플레이中
SIF TEQUIP:ARG:31
	CALL EQUIP_COM187(ARG)

;샤워사용중
SIF TEQUIP:ARG:32
	CALL EQUIP_COM188(ARG)

;새댁플레이中
SIF TEQUIP:ARG:33
	CALL EQUIP_COM189(ARG)
;삽입중
IF ARG != MASTER
	SIF TEQUIP:ARG:Ｖ섹스 >= 0 && !CUP:ARG:쾌Ｖ
		CALL EQUIP_INCERT_V(ARG)
	SIF TEQUIP:ARG:Ａ섹스 >= 0 && !CUP:ARG:쾌Ａ
		CALL EQUIP_INCERT_A(ARG)
ENDIF
RETURN 1

;삽입계속の処理
@EQUIP_INCERT_V(ARG)
;-------------------------------------------------
;行為者
;-------------------------------------------------
;PRINTFORML ターゲットインサート：{TEQUIP:TARGET:Ｖ섹스}
IF CFLAG:ARG:현재위치 != CFLAG:MASTER:현재위치
	TEQUIP:ARG:Ｖ섹스 = -1
	RETURN 0
ENDIF

TCVAR:(TEQUIP:ARG:50):사정쾌락강도 = 700
TCVAR:(TEQUIP:ARG:50):사정대상캐릭터_1 = ARG
TCVAR:(TEQUIP:ARG:50):사정부위플래그 = 1

PRINTFORML ＜ %CALLNAME:(TEQUIP:ARG:Ｖ섹스)%Ｖ인서트＞
DOWNBASE:ARG:체력 += 20
DOWNBASE:ARG:기력 += 50

CALL SOURCE_CALC_쾌Ｖ(ARG, TEQUIP:ARG:50, 150)
CALL SOURCE_CALC_노출(ARG, TEQUIP:ARG:50, 30)
CALL SOURCE_CALC_접촉(ARG, TEQUIP:ARG:50, 75)
IF BASE:ARG:체력 <= 300
	CALL SOURCE_CALC_고통(ARG, TEQUIP:ARG:50, 200)
	CALL SOURCE_CALC_구속(ARG, TEQUIP:ARG:50, 100)
ENDIF
;정조観念
IF !TALENT:연모
	IF TALENT:정조 == 1
		CALL SOURCE_CALC_불결(ARG, TEQUIP:ARG:50, 500)
	ELSEIF TALENT:정조 == 0
		CALL SOURCE_CALC_불결(ARG, TEQUIP:ARG:50, 250)
	ENDIF
ENDIF
;고통
LOCAL = 200 * PAIN_CHECK_V(TARGET) / 1000
CALL SOURCE_CALC_고통(ARG, TEQUIP:ARG:50, LOCAL)

;-------------------------------------------------
;경험の処理
;-------------------------------------------------
EXP:ARG:Ｖ성교경험 ++
CFLAG:ARG:엉덩이성장 ++
RETURN 1

@EQUIP_INCERT_A(ARG)

IF CFLAG:ARG:현재위치 != CFLAG:MASTER:현재위치
	TEQUIP:ARG:Ａ섹스 = -1
	RETURN 0
ENDIF


TCVAR:(TEQUIP:ARG:51):사정쾌락강도 = 700
TCVAR:(TEQUIP:ARG:51):사정대상캐릭터_1 = ARG
TCVAR:(TEQUIP:ARG:51):사정부위플래그 = 2

PRINTFORML ＜ %CALLNAME:(TEQUIP:ARG:51)%Ａ인서트＞
DOWNBASE:ARG:체력 += 30
DOWNBASE:ARG:기력 += 60

CALL SOURCE_CALC_쾌Ａ(ARG, TEQUIP:ARG:51, 200)
CALL SOURCE_CALC_노출(ARG, TEQUIP:ARG:51, 50)
CALL SOURCE_CALC_접촉(ARG, TEQUIP:ARG:51, 75)
CALL SOURCE_CALC_불결(ARG, TEQUIP:ARG:51, 100)
IF BASE:ARG:체력 <= 300
	CALL SOURCE_CALC_고통(ARG, TEQUIP:ARG:50, 300)
	CALL SOURCE_CALC_구속(ARG, TEQUIP:ARG:50, 100)
ENDIF


;고통
LOCAL = 400 * PAIN_CHECK_A(ARG) / 1000
CALL SOURCE_CALC_고통(ARG, TEQUIP:ARG:51, LOCAL)
;-------------------------------------------------
;경험の処理
;-------------------------------------------------
EXP:ARG:Ａ성교경험 ++
CFLAG:ARG:엉덩이성장 ++
RETURN 1

