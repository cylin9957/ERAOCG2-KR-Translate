@SOURCE_ABLUP(ARG)
IF ABL:ARG:신체능력 < 6
	IF EXP:ARG:운동경험 > EXPLV:(ABL:ARG:신체능력 + 3) || (EXP:ARG:운동경험 > 5 && ABL:ARG:신체능력 == 0)
		ABL:ARG:신체능력 ++
		PRINTFORMW %CALLNAME:ARG%의 신체능력이 LV{ABL:ARG:신체능력}되었습니다
	ENDIF
ENDIF
IF ABL:ARG:화술기능 < 6
	IF EXP:ARG:대화경험 > EXPLV:(ABL:ARG:화술기능 + 3)
		ABL:ARG:화술기능 ++
		PRINTFORMW %CALLNAME:ARG%의 화술기능이 LV{ABL:ARG:화술기능}되었습니다
	ENDIF
ENDIF

IF ABL:ARG:청소기능 < 5
	IF EXP:ARG:청소경험 > EXPLV:(ABL:ARG:청소기능 + 3)
		ABL:ARG:청소기능 ++
		PRINTFORMW %CALLNAME:ARG%의 청소기능이 LV{ABL:ARG:청소기능}되었습니다
	ENDIF
ENDIF
IF ABL:ARG:교양 < 6
	IF EXP:ARG:학습경험 > EXPLV:(ABL:ARG:교양 + 3) || (EXP:ARG:학습경험 > 5 && ABL:ARG:교양 == 0)
		ABL:ARG:교양 ++
		PRINTFORMW %CALLNAME:ARG%의 교양이 LV{ABL:ARG:교양}되었습니다
		SIF ABL:ARG:교양 == 1
			PRINTW 최음약을 쓸 수 있게 되었습니다
	ENDIF
ENDIF
IF ABL:ARG:요리기능 < 6
	IF EXP:ARG:요리경험 > EXPLV:(ABL:ARG:요리기능 + 3) || (EXP:ARG:요리경험 > 5 && ABL:ARG:요리기능 == 0)
		ABL:ARG:요리기능 ++
		PRINTFORMW %CALLNAME:ARG%의 요리기능이 LV{ABL:ARG:요리기능}되었습니다
	ENDIF
ENDIF
IF ABL:ARG:손가락 < 6
	LOCAL = EXP:ARG:수음경험
	LOCAL:1 = 10 + ABL:ARG:기교 / 2 + TALENT:ARG:솜씨좋은손가락 * 2 + TALENT:ARG:습득속도
	IF LOCAL * LOCAL:1 / 10 > EXPLV:(ABL:ARG:손가락 + 3) / 2 && EXP_UP(24,ARG)
		ABL:ARG:손가락 ++
		PRINTFORMW %CALLNAME:ARG%손재주가 생겼다
		IF ABL:ARG:손가락 >= 5
			PRINTFORMW %CALLNAME:ARG%은[재주좋은손]을 얻었다
			TALENT:ARG:솜씨좋은손가락 = 1
		ENDIF
	ENDIF
ENDIF
IF ABL:ARG:혀 < 6
	LOCAL = EXP:ARG:펠라경험
	LOCAL:1 = 10 + ABL:ARG:기교 / 2 + TALENT:ARG:혀사용자 * 2 + TALENT:ARG:습득속도
	IF  LOCAL * LOCAL:1 / 10 > EXPLV:(ABL:ARG:혀 + 3) && EXP_UP(25,ARG)
		ABL:ARG:혀 ++
		PRINTFORMW %CALLNAME:ARG%혀가 재주를 부렸다
		IF ABL:ARG:혀 >= 5
			PRINTFORMW %CALLNAME:ARG%은[혀놀림]을 얻었다
			TALENT:ARG:혀사용자 = 1
		ENDIF
	ENDIF
ENDIF
IF ABL:ARG:가슴 < 6
	LOCAL = EXP:ARG:파이즈리경험 * 2 +  EXP:ARG:Ｂ경험 / 5
	LOCAL:1 = 10 + ABL:ARG:기교 / 2 + BUST_SIZE(ARG) + TALENT:ARG:습득속도
	IF LOCAL * LOCAL:1 / 10 > EXPLV:(ABL:ARG:가슴 + 3) && EXP:ARG:파이즈리경험 >  EXPLV:(ABL:ARG:가슴 + 1) && EXP_UP(26,ARG)
		ABL:ARG:가슴 ++
		PRINTFORMW %CALLNAME:ARG%가슴을 이용한 성 기술이 늘어난 것 같다
		IF ABL:ARG:가슴 >= 5
			PRINTFORMW %CALLNAME:ARG%은[음유]를 얻었다
			TALENT:ARG:음유 = 1
		ENDIF
	ENDIF
ENDIF
IF ABL:ARG:허리 < 6
	LOCAL = EXP:ARG:Ｖ성교경험 +  EXP:ARG:Ａ성교경험
	LOCAL:1 = 10 + ABL:ARG:기교 / 2 + TALENT:ARG:습득속도
	IF LOCAL * LOCAL:1 / 10 > EXPLV:(ABL:ARG:허리 + 3)
		ABL:ARG:허리 ++
		PRINTFORMW %CALLNAME:ARG%허리놀림이 좋아졌다
	ENDIF
ENDIF
IF ABL:ARG:질 < 6
	;広げすぎるとガバガバになっちゃいますよ
	LOCAL = EXP:ARG:Ｖ성교경험  * 2 +  EXP:ARG:Ｖ경험 / 5 + (EXP:ARG:Ｖ확장경험 <= 10 ? EXP:ARG:Ｖ확장경험 * 5 # 100 - EXP:ARG:Ｖ확장경험)
	LOCAL:1 = 10 + ABL:ARG:기교 / 2 + TALENT:ARG:습득속도
	IF LOCAL * LOCAL:1 / 10 > EXPLV:(ABL:ARG:질 + 3) && EXP:ARG:Ｖ성교경험 >  EXPLV:(ABL:ARG:질 + 1) && EXP_UP(20,ARG)
		ABL:ARG:질 ++
		PRINTFORMW %CALLNAME:ARG%의 성교 기술이 늘어난 것 같다
		IF ABL:ARG:질 >= 5
			PRINTFORMW %CALLNAME:ARG%은[음호]얻었다
			TALENT:ARG:음호 = 1
		ENDIF
	ENDIF
ENDIF
IF ABL:ARG:애널 < 6
	LOCAL = EXP:ARG:Ａ성교경험  * 2 +  EXP:ARG:Ａ경험 / 5 + (EXP:ARG:Ａ확장경험 <= 25 ? EXP:ARG:Ａ확장경험 * 2 # 100 - EXP:ARG:Ａ확장경험)
	LOCAL:1 = 10 + ABL:ARG:기교 / 2 + TALENT:ARG:습득속도
	IF LOCAL * LOCAL:1 / 10 > EXPLV:(ABL:ARG:애널 + 3) && EXP:ARG:Ａ성교경험 > EXPLV:(ABL:ARG:애널 + 1) && EXP_UP(21,ARG)
		ABL:ARG:애널 ++
		PRINTFORMW %CALLNAME:ARG%의 항문을 이용한 성교 기술이 늘어난 것 같다
		IF ABL:ARG:애널 >= 5
			PRINTFORMW %CALLNAME:ARG%은[애널광]을 얻었다
			TALENT:ARG:엉덩이광 = 1
		ENDIF
	ENDIF
ENDIF
